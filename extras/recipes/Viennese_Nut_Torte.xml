<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Viennese Nut Torte</title>
      <categories>
        <cat>Cakes</cat>
        <cat>Chocolate</cat>
        <cat>Tortes</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>7</qty>
          <unit/></amt>
        <item>Eggs -- separated</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit>ounces</unit></amt>
        <item>Bittersweet chocolate --</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Grated</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Nuts -- ground</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Chocolate icing</item></ing></ingredients>
    <directions>
      <step>  Bear yolks and sugar till light.  Add chocolate, cinnamon and nuts. Fold in
  stiffly beaten whites.  Put into a tube pan and bake 45 min at 325F or till
  done.
  
