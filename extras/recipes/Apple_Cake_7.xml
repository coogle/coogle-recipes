<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Cake</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>COMBINE</title>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>Veg.oil</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit/></amt>
          <item>Eggs beaten</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Vanilla</item></ing></ing-div>
      <ing-div>
        <title>SIFT TOGETHER</title>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>All purpose flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Cinnamon</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Baking soda</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing></ing-div>
      <ing-div>
        <title>ADD TO EGG MIXTURE</title></ing-div>
      <ing-div>
        <title>STIR IN</title>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Diced apples (cooking apples)</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Raisins</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Chopped walnuts</item></ing></ing-div></ingredients>
    <directions>
      <step>  mix well,spoon into greased &amp; floured 9X13 pan. (dough will be stiff) Bake
  at 350 degrees for 1 hour, test for doneness. May need more cooking time.
  Posted to TNT - Prodigy's Recipe Exchange Newsletter  by Lucieka@aol.com on
  Aug 28, 1997
 
</step></directions></recipe></recipeml>
