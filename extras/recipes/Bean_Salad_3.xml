<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bean Salad</title>
      <categories>
        <cat>Salads</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Cider vinegar</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Vegetable oil</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>large</unit></amt>
        <item>Onion; cut into rings</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Green pepers; cut into rings</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Kidney beans; drained</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Green beans; drained</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Yellow beans; drained</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Chick peas; drained</item></ing></ingredients>
    <directions>
      <step>  Mix first five ingredients together. Pour over remaining ingredients. This
  salad tastes better if left to marinate overnight in the refrigerator. GOOD
  TASTING SALAD! Posted to EAT-L Digest by pat hogberg
  &lt;p_hogberg@HOTMAIL.COM&gt; on Dec 9, 1997
 
</step></directions></recipe></recipeml>
