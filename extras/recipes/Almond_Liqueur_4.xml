<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Almond Liqueur</title>
      <categories>
        <cat>Beverages</cat>
        <cat>Vodka</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2 1/4</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Lemons; the rind -- finely grated</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>quart</unit></amt>
        <item>Vodka</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Almond extract</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Vanilla extract</item></ing></ingredients>
    <directions>
      <step>  Combine first 3 ingredients in a Dutch oven; bring to a boil. Reduce heat
  and simmer 5 minutes, stirring occasionally; cool completely. Stir in
  remaining ingredients; store in airtight containers. Yield: about 6 1/2
  cups.
  
  Recipe By     :
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
