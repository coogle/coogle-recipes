<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>A Dilly-Of-An-Onion Bread (A B M)</title>
      <categories>
        <cat>Breads</cat>
        <cat>Breadmakers</cat></categories>
      <yield>1 Loaf-1.75</yield></head>
    <ingredients>
      <ing-div>
        <title>MM BY IRIS GRAYSON</title>
        <ing>
          <amt>
            <qty>3 1/2</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Water</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Chopped onion</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Dried dill</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Vegetable oil</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Paprika</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>package</unit></amt>
          <item>Dry yeast</item></ing></ing-div></ingredients>
    <directions>
      <step>  Follow manufacturer's instructions for placing ingredients into bread pan;
  select bake cycle, and start machine.  Yield:  1 (1 3/4-pound) loaf, 14
  servings.
  
  PER SERVING:  calories 137 (16% from fat); protein 3.8g; fat 2.5g (sat
  0.4g, mono 0.65g, poly 1.2g); carb 24.6g; fiber 0.3g; chol 0mg; iron 1.6mg;
  sodium 336mg; calc 11mg.
  
