<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbecued Beef Buns</title>
      <categories>
        <cat>Meats</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>cups</unit></amt>
        <item>Cooked Beef Strips; 2 X 1/4"</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Ketchup</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Cider Vinegar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Butter Or Regular Margarine</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>medium</unit></amt>
        <item>Onions; Thinly Sliced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>slices</unit></amt>
        <item>Lemon</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Worcestershire Sauce</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>teaspoons</unit></amt>
        <item>Prepared Mustard</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Chili Powder</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Black Pepper</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>8</qty>
          <unit/></amt>
        <item>Hamburger Buns</item></ing></ingredients>
    <directions>
      <step>  Combine all of the ingredients, except the hamburger buns, in a 4-quart
  Dutch oven.  Cook over high heat until the mixture comes to a boil. Reduce
  the heat to low and simmer for 25 minutes or until the mixture thickens.
  Remove the lemon slices.  Pour into a container, cover and refrigerate.
  
  TO SERVE:
  
  Warm over medium heat and serve in the hamburger buns.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
