<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>B/m Cornmeal Bread</title>
      <categories>
        <cat>Bread machi</cat>
        <cat>Bread maili</cat>
        <cat>Bread/muffi</cat>
        <cat>Grains</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing-div>
        <title>MEDIUM LOAF</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Water</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Vegetable Oil</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Egg</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Yellow Cornmeal</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Yeast</item></ing></ing-div></ingredients>
    <directions>
      <step>  This loaf rises very high and is surprisingly good.
  Posted to Digest bread-bakers.v097.n056 by Reggie Dwork &lt;reggie@reggie.com&gt;
  on Sep 12, 1997
 
</step></directions></recipe></recipeml>
