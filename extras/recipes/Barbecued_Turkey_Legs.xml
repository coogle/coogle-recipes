<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbecued Turkey Legs</title>
      <categories>
        <cat>Poultry</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Turkey drumsticks; thawed</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt and pepper; to taste</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Catsup</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Molasses</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Vinegar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Worcestershire sauce</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Hickory smoke salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Instant minced onion</item></ing></ingredients>
    <directions>
      <step>  From: Bobb1744@aol.com
  
  Date: Thu, 4 Apr 1996 21:37:02 -0500
  
  Recipe By: R. Banghart
  
  Sprinkle turkey drums with salt and pepper. Place in large crockpot.
  Combine catsup , molasses, vinegar, Worcestershire sauce, hickory smoked
  salt and instant minced onion. Pour over turkey. Cover and cook on Low for
  7-9 hours.
  
