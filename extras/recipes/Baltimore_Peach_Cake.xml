<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baltimore Peach Cake</title>
      <categories>
        <cat>Cakes</cat>
        <cat>Desserts</cat>
        <cat>Fruits/nuts</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Butter, softened</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2 1/2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking Powder</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Sugar, mixed with...</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit/></amt>
        <item>Large Peaches, peeled/sliced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter, melted</item></ing></ingredients>
    <directions>
      <step>  Preheat oven to 350. With electric mixer, blend first five ingredients.
  Spread in greased and floured 15x10" jelly roll pan, or 13x9" plus one 8"
  cake pan. Sprinkle half of cinnamon/sugar mixture lightly over dough base.
  Arrange peach slices by rows on top of base. Sprinkle with remainder of
  cinnamon/sugar mixture and drizzle melter butter over all. Bake for 25-30
  minutes. (For thicker dough, omit 8" cake pan -- use 13x9" pan only.)
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
