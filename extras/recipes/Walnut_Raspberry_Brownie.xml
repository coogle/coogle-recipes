<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Walnut Raspberry Brownie</title>
      <categories>
        <cat>Brownies</cat>
        <cat>Chocolate</cat>
        <cat>Nuts</cat>
        <cat>Fruits</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Unsweetened chocolate sqs</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Crisco</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Walnuts; chopped</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Raspberry jam</item></ing></ingredients>
    <directions>
      <step>  Melt chocolate and Crisco over warm water; cool
  slightly. Blend together eggs, vanilla, sugar and
  salt; stir in chocolate mixture, then flour. Fold in
  walnuts. Turn into well greased 8-inch square pan.
  Bake at 325~ for about 40 minutes. Spoon jam over hot
  brownies; spread carefully. Let cool. Top with Velvet
  Chocolate Glaze.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
