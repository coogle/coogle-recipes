<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Applesauce-Nut Cake</title>
      <categories>
        <cat>Cake</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Applesauce</item></ing>
      <ing>
        <amt>
          <qty>3 1/2</qty>
          <unit>cups</unit></amt>
        <item>Sifted flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking powder</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground cloves</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Ground nutmeg</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>teaspoons</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Seedless raisins</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Pecans</item></ing></ingredients>
    <directions>
      <step>  Cream butter and sugars. Add eggs and applesauce; beat well. Combine dry
  ingredients and stir into applesauce mixture. Add raisins and pecans and
  mix well. Pour into greased 13x9x2 inch pan. Bake at 325 for 2 hours.
  
  MRS HUGH BONNER (GAY)
  
  MARVELL, AR
  
  From the book &lt;High Cotton Cookin'&gt;, Marvell Academy Mothers Assn, Marvell,
  AR  72366, ISBN 0-918544-14-9, downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
