<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bay Scallops Saute</title>
      <categories>
        <cat>Seafood</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Bay scallops</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Minced garlic clove</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chopped fresh parsley</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Freshly ground pepper</item></ing></ingredients>
    <directions>
      <step>  In skillet melt butter &amp; add garlic. Cook until garlic is golden
  brown. Add scallops &amp; parsley. Cook 2-3 minutes, stirring often.
  Sprinkle with pepper &amp; serve.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmkah001.zip
 
</step></directions></recipe></recipeml>
