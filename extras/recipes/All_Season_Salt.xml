  <recipe>
    <head>
      <title>All Season Salt</title>
      <categories>
        <cat>Seasonings</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>INGREDIENTS</title>
        <ing>
          <amt>
            <qty>1 1/4</qty>
            <unit>cups</unit></amt>
          <item>Table salt</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>tablespoons</unit></amt>
          <item>Onion salt</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Celery salt</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>tablespoons</unit></amt>
          <item>Garlic salt</item></ing>
