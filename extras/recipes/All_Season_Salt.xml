          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Paprika</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Black pepper</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>White pepper</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>tablespoons</unit></amt>
          <item>Dry mustard</item></ing></ing-div>
      <ing-div>
        <title>PREPARATION</title></ing-div>
      <ing-div>
        <title>SOURCE: COOKBOOK USA</title></ing-div></ingredients>
