<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Austrian Malt Bread</title>
      <categories>
        <cat>Breads</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>ONE POUND LOAF</title>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Water</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Margarine/butter</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>tablespoons</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>tablespoons</unit></amt>
          <item>Malted milk powder</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Yeast</item></ing></ing-div></ingredients>
    <directions>
      <step>  Bake on regular cycle
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
