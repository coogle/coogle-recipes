<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bacon Bits for Dogs (Not For Humans)</title>
      <categories>
        <cat>Dog food</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>slices</unit></amt>
        <item>Cooked bacon; crumbled</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Eggs; well beaten</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>cups</unit></amt>
        <item>Bacon fat</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Non-fat dry milk powder</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Graham flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Wheat germ</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Cornmeal</item></ing></ingredients>
    <directions>
      <step>  Contributed by Sandra Strassman-Sundy
  Mix ingredients with a strong spoon; drop heaping tablespoonfuls onto a
  greased baking sheet. Bake in a 350 oven for 15 minutes. Turn off oven and
  leave cookies on baking sheet in the oven overnight to dry out.
  
  Yield: about 4 dozen dog cookies.
  
  Posted to Bread-bakers Digest by Marg aka MEMAW &lt;maf@fn.net&gt; on Mar 29,
  1998
 
</step></directions></recipe></recipeml>
