<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Almond Poppy Seed Bread</title>
      <categories>
        <cat>Bread machi</cat>
        <cat>Grains/puls</cat>
        <cat>Nuts</cat>
        <cat>Breads</cat></categories>
      <yield>14</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/4</qty>
          <unit>cups</unit></amt>
        <item>Buttermilk</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Almond extract</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Honey; Or Maple Syrup</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Poppy seeds</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking soda</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Vital Gluten; To 3 Tbs, Optional</item></ing>
      <ing>
        <amt>
          <qty>3 1/3</qty>
          <unit>cups</unit></amt>
        <item>Whole wheat flour</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Yeast</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Almonds; chopped</item></ing></ingredients>
    <directions>
      <step>  Place ingredients in order given by manufacturer.
  
  Add nuts at beep.
  
  Medium crust setting.
  
  Makes 1-1/2 lb loaf.
  Posted to Digest bread-bakers.v097.n070 by Reggie Dwork &lt;reggie@reggie.com&gt;
  on Nov 09, 1997
 
</step></directions></recipe></recipeml>
