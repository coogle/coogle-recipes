<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Almost Fat Free Brownies</title>
      <categories>
        <cat>None</cat></categories>
      <yield>16</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Unsweetened Cocoa Powder</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>ounces</unit></amt>
        <item>Jar</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Egg Whites</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Nuts -- (optional)</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Prune Baby Food</item></ing></ingredients>
    <directions>
      <step>  Preheat oven to 350 degrees.  Spray an 8-inch square baking pan with
  cooking spray.  Combine all ingredients.  Add nuts if desired. Bake about
  30 minutes or until springy to touch. Cool on rack.
  
  Cut into 16 pieces.
  
  Fat, about 0.3 g. per brownie.
  
  Recipe By     : Sharon Pack &lt;pack@lclark.edu&gt;
  
  From: Ladies Home Journal- August 1991
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
