<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Autumn Corn Bake</title>
      <categories>
        <cat>Vegetable</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>packages</unit></amt>
        <item>(10-oz) frozen corn; thawed, drained</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>pounds</unit></amt>
        <item>Velveeta pasteurized process cheese spread; cubed</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Cornmeal</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Egg; beaten</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Parkay margarine; melted</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Finely chopped green pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>dash</unit></amt>
        <item>Pepper</item></ing></ingredients>
    <directions>
      <step>  Combine ingredients; mix well.  Spoon into 1-1/2-quart casserole; cover.
  Bake at 350 degrees, 45 minutes. 6 servings.
  
  From a collection of my mother's (Judy Hosey) recipe box which contained
  lots of her favorite recipes, clippings, etc.  Downloaded from Glen's MM
  Recipe Archive, http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
