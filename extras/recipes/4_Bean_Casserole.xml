<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>4 Bean Casserole</title>
      <categories>
        <cat>Casseroles</cat>
        <cat>Beans &amp; pea</cat>
        <cat>Family &amp; fr</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>pounds</unit></amt>
        <item>Hamburger</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Bacon</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>cans</unit></amt>
        <item>Beans</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Catsup</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Mustard</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Worcestershire Sauce</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt; to taste</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Pepper; to taste</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Vinegar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Brown Sugar</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Onion</item></ing></ingredients>
    <directions>
      <step>  Place all ingredents in a casserole dish and bake.
  
  Recipe by: Janis Alling (Durrell),  Possum Kingdom Lake Cookbook
  
  Posted to MC-Recipe Digest by Barb at PK  &lt;abprice@wf.net&gt; on Apr 16, 1998
 
</step></directions></recipe></recipeml>
