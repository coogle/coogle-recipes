<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Basic White Bread</title>
      <categories>
        <cat>Breadmaker</cat>
        <cat>Breads</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>XKGR41A Don Fifield</item></ing>
      <ing>
        <amt>
          <qty>2 1/4</qty>
          <unit>cups</unit></amt>
        <item>Bread flour (10 1/2 oz)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Dry milk (1/5 oz)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt (1/5 oz)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Butter (2/5 oz)</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Sugar (3/5 oz)</item></ing>
      <ing>
        <amt>
          <qty>7/8</qty>
          <unit>cups</unit></amt>
        <item>Water (7 fl.oz)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Dry yeast (1/10 oz)</item></ing>
      <ing>
        <amt>
          <qty>Timer</qty>
          <unit>OR</unit></amt>
        <item>Bake (Rapid) mode may be used. Place all ingredients (except</item></ing></ingredients>
    <directions>
      <step>  liquids and yeast) inside the bread pan. Add liquid ingredients. Close
  cover and place dry yeast into the yeast holder. Press start.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
