<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Pineapple #4</title>
      <categories>
        <cat>Fruits</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Grated cheese; (Cheddar)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Roll Ritz crackers; crushed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Stick margarine; melted</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cans</unit></amt>
        <item>Pineapple (1 tidbits and 1 crushed; 10 1/2 oz.)</item></ing></ingredients>
    <directions>
      <step>  Pour both cans pineapple in 8 x 12-inch dish (one drained, one not
  drained). Mix together flour and sugar; sprinkle over pineapple. Spread
  shredded cheese over flour sugar mixture. Pour crushed crackers over all.
  Pour melted margarine over crackers. Bake at 350 degrees for 25 to 30
  minutes.
  Posted to recipelu-digest by "Diane Geary" &lt;diane@keyway.net&gt; on Feb 23,
  1998
 
</step></directions></recipe></recipeml>
