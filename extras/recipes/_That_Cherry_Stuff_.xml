<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>"That Cherry Stuff"</title>
      <categories>
        <cat>Desserts</cat>
        <cat>Fruits</cat>
        <cat>Pulses</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Can (21 oz.) cherry pie</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Filling</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Tub (8oz.) frozen whipped</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Topping, thawed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Can (14 oz.) sweetened</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Condensed milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Can (8 oz.) crushed pine-</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Apple, drained</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Coarsely chopped nuts, (any)</item></ing></ingredients>
    <directions>
      <step>  In a large bowl, fold together all the ingredients. Spoon into a serving
  bowl or dessert dishes. Chill before serving. FROM MR.FOOD the MIAMI HERALD
  10/6/94. Submitted By SALLI SCHWARTZ (&lt;102003.3307@COMPUSERVE.COM&gt; On THU,
        6    JUL 1995 021448 GMT
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
