<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Corn En Casserole</title>
      <categories>
        <cat>None</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Creamed corn; No. 303</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Whole kernel corn; No. 303</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>large</unit></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Bell pepper; chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>ounces</unit></amt>
        <item>Pimiento; chopped</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Egg; well beaten</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Cracker crumbs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Cheese; grated</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Butter or margarine; melted</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt and pepper; to taste</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Red pepper; to taste</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients and mix well. Place in a greased 2-quart casserole.
  Bake at 350 degrees for 1 hour.
  
  Recipe by: Southern Living Casseroles Cookbook
  
  Posted to MC-Recipe Digest by "Marie Smith" &lt;crafty1@earthlink.net&gt; on May
  2, 1998
 
</step></directions></recipe></recipeml>
