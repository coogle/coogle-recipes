<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Dapple</title>
      <categories>
        <cat>Desserts</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>3</qty>
            <unit/></amt>
          <item>Eggs</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>Oil</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Soda</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Cinnamon</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Vanilla</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Nuts; chopped fine</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Chopped raw apples</item></ing></ing-div>
      <ing-div>
        <title>TOPPING</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Stick butter</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Brown sugar</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Milk</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>can</unit></amt>
          <item>Coconut</item></ing></ing-div></ingredients>
    <directions>
      <step>  Cream eggs, sugar and oil. Sift together flour, salt, soda and cinnamon.
  Combine egg mixture with sifted dry ingredients. Stir in vanilla, nuts and
  apples. Pour into greased 13 x 9 inch pan and bake 45 minutes at 350
  degrees. Cook topping ingredients 2 1/2 minutes and pour over hot cake.
  
  busted by sooz
  
  Recipe by: Key Gourmet
  
  Posted to recipelu-digest by sooz &lt;kirkland@gj.net&gt; on Mar 23, 1998
 
</step></directions></recipe></recipeml>
