<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Corn Bread (Vegan)</title>
      <categories>
        <cat>Candy</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Cornmeal</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Diced Apple</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Brown Sugar</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking Powder</item></ing>
      <ing>
        <amt>
          <qty>1 1/4</qty>
          <unit>cups</unit></amt>
        <item>Milk Alternative -- * see</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Note</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Mixed with 1 tsp. Apple</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Cider Vinegar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Orange Marmalade -- (or</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Other jam)</item></ing></ingredients>
    <directions>
      <step>  * i.e. Plain Rice Milk, Lite Soy Milk, Almond Milk, etc.
  
  Mix all ingredients in order listed. Put one half of the batter in a
  greased 8 or 9 inch square baking pan. Evenly top batter with 1/2 cup jam
  and top with remaining batter.
  
  Bake at 400 F for 25 minutes. Do not overbake. Allow to cool slightly
  before cutting
  
  Recipe By     : rolyat@main.citynet.net
  
  From: Ladies Home Journal- August 1991
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
