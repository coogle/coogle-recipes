  <recipe>
    <head>
      <title>Au Gratin Potatoes #2</title>
      <categories>
        <cat>Vegetable</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>pounds</unit></amt>
        <item>Frozen hash browns; slightly thawed *OR*</item></ing>
      <ing>
        <amt>
          <qty>58</qty>
          <unit>ounces</unit></amt>
        <item>Hominy (2 large cans)</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Margarine</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit/></amt>
        <item>Chopped onion</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Cheddar longhorn cheese</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>ounces</unit></amt>
        <item>Diced green chiles (1 can)</item></ing>
      <ing>
        <amt>
          <qty>8</qty>
          <unit>ounces</unit></amt>
        <item>Sour cream</item></ing>
      <ing>
        <amt>
          <qty>10</qty>
          <unit>ounces</unit></amt>
        <item>Cream of chicken soup (1 can)</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Non-stick spray</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Cornflake crumbs</item></ing></ingredients>
    <directions>
      <step>  Mix all ingredients together (except cornflakes) and spoon into sprayed 13-
  by 9-inch dish.  Top with cornflake crumbs.  Bake at 350 degrees 45
  minutes. Makes 10 to 12 servings.
  
