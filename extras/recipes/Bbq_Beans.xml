<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bbq Beans</title>
      <categories>
        <cat>Crockpot</cat>
        <cat>Bean/legume</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>can</unit></amt>
          <item>Pork and beans; ex large</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Ketchup</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Mustard; dry</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Onions; chopped</item></ing>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit/></amt>
          <item>Barbecue sauce; bottled</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Brown sugar; if you  like</item></ing></ing-div>
      <ing-div>
        <title>SECOND CHOICE</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>can</unit></amt>
          <item>B&amp;M Baked beans; huge can</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Onions; chopped</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>Ketchup</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Grey Poupon mustard; well rounded</item></ing>
        <ing>
          <amt>
            <qty>14</qty>
            <unit>ounces</unit></amt>
          <item>Barbecue sauce</item></ing></ing-div></ingredients>
    <directions>
      <step>  Date: Thu, 23 May 1996 00:20:36 -0400
  
  From: BobbieB1@aol.com
  
  Mix all ingredients in crock pot and cook on high 8 to 10 hours or
  overnight.
  
  Posted to MealMaster Recipes List, Digest #143
 
</step></directions></recipe></recipeml>
