  <recipe>
    <head>
      <title>Amish Bread W/oatmeal</title>
      <categories>
        <cat>Breads</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing-div>
        <title>NANCY HAGFORS GXDB48A</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Sourdough starter</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Oil</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Light brown sugar</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>Oatmeal (quick)</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit/></amt>
          <item>Eggs</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Baking powder</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Baking soda</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Cinnamon</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Nutmeg</item></ing></ing-div></ingredients>
    <directions>
      <step>  Mix all ingredients together.  Bake at 350-degrees for 40 minutes.
  
