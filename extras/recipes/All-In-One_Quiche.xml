<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>All-In-One Quiche</title>
      <categories>
        <cat>Casseroles</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Bisquick mix</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit/></amt>
        <item>Stick butter</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Pinches salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Diced bacon; ham, turkey, chicken, or shrimp</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Green onions; chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(4 oz.) sliced mushrooms; drained</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Grated sharp Cheddar cheese</item></ing></ingredients>
    <directions>
      <step>  Combine the first 5 ingredients in the blender and mix well. Turn into 9 or
  10 inch pie plate. Add the remaining ingredients. Bake at 350 degrees for
  45 minutes. Let stand for 10 minutes before serving.
  
  Recipe by: Bisquick Site - Found By Brenda Whimsey Stephens
  
  Posted to brand-name-recipes by Tiina &lt;4Angels@videotron.ca&gt; on Mar 04,
  1998
 
</step></directions></recipe></recipeml>
