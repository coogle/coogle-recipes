<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bailey's Chocolate Chip Cheesecake</title>
      <categories>
        <cat>Cheesecakes</cat>
        <cat>Chocolate</cat>
        <cat>Cakes</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>GRAHAM CRACKER CRUMBS</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>SUGAR</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>tablespoons</unit></amt>
        <item>BUTTER</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Preheat oven to 325-degrees.</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Combine crumbs and sugar in pan. Stir in butter. Press into</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Bottom and 1" up sides of pan.</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Bake 7 minutes.</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Maintain oven temperature at 325-degrees.</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>FILLING</item></ing>
      <ing>
        <amt>
          <qty>2 1/2</qty>
          <unit>pounds</unit></amt>
        <item>CREAM CHEESE, room temperature</item></ing>
      <ing>
        <amt>
          <qty>1 2/3</qty>
          <unit>cups</unit></amt>
        <item>SUGAR</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit/></amt>
        <item>EGGS, room temperature</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>BAILEY'S IRISH CREAM</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>VANILLA</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>CHOCOLATE CHIPS</item></ing></ingredients>
    <directions>
      <step>  ** CRUST
 
</step></directions></recipe></recipeml>
