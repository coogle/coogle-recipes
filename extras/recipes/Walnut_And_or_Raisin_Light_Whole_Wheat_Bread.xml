<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Walnut And/or Raisin Light Whole Wheat Bread</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>BREAD FLOUR</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>WHOLE WHEAT FLOUR</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>teaspoons</unit></amt>
        <item>GLUTEN</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>YEAST</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>WATER; or more</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>POWDered MILK</item></ing>
      <ing>
        <amt>
          <qty>3/16</qty>
          <unit>cups</unit></amt>
        <item>WALNUTS; (or raisins, or a combo of both)</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>BROWN SUGAR</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>tablespoons</unit></amt>
        <item>HONEY</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>OIL</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>SALT</item></ing></ingredients>
    <directions>
      <step>  SET MEDIUM CRUST
  
