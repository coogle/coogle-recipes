<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bbq Lamb Shoulder</title>
      <categories>
        <cat>Meats</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Catsup</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Celery salt</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Worcestershire sauce</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Honey</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>pounds</unit></amt>
        <item>Boned shoulder of lamb</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>(rolled &amp; tied)</item></ing></ingredients>
    <directions>
      <step>  Combine catsup, celery salt, Worcestershire, pepper &amp; honey; mix well.
  Place lamb on spit; brush with honey mixture. Cook on rotisserie 2
  hours, brushing frequently with honey mixture during cooking.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmkah001.zip
 
</step></directions></recipe></recipeml>
