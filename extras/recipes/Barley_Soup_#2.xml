<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barley Soup #2</title>
      <categories>
        <cat>Soup</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>pounds</unit></amt>
        <item>Beef; cubed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Celery; chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Parsley</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Worcestershire</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Thyme</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Bay leaf</item></ing>
      <ing>
        <amt>
          <qty>8</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Potatoes; diced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Carrots; sliced</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Pearl barley</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Onions; chopped</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Kitchen Boquet</item></ing></ingredients>
    <directions>
      <step>  Brown beef in oil. Add celery, salt, parsley, Worcestershire, thyme, bay
  leaf and water. Simmer 2 hours. Skim and add remaining ingredients. Cook
  another 45 minutes. More water may be needed if soup cooks down and becomes
  too thick.
  
  MRS JOE WEISBERGER (NINA)
  
  MARVELL, AR
  
  From the book &lt;High Cotton Cookin'&gt;, Marvell Academy Mothers Assn, Marvell,
  AR  72366, ISBN 0-918544-14-9, downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
