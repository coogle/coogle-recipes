<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apricot Nectar Cake</title>
      <categories>
        <cat>Cake</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>package</unit></amt>
          <item>Lemon supreme cake mix</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Cooking oil</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Apricot nectar</item></ing>
        <ing>
          <amt>
            <qty>4</qty>
            <unit/></amt>
          <item>Eggs</item></ing></ing-div>
      <ing-div>
        <title>GLAZE</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Confectioner's sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Lemon; juice of</item></ing></ing-div></ingredients>
    <directions>
      <step>   Mix all ingredients well; beat for 3 minutes. Bake in tube pan in 350
  degree oven for about 1 hour. Glaze while warm.
  
   To make glaze: Combine ingredients until well mixed.
  
  From &lt;A Taste of Louisiana&gt;.  Downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
