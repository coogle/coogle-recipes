<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbara's Favorite Chicken Recipe</title>
      <categories>
        <cat>Poultry</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>6</qty>
          <unit/></amt>
        <item>Boneless chicken breasts</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Cream of mushroom soup</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Margarine or butter</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Garlic</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Container sour cream cooked rice; (8 oz.)</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Mushrooms; if desired</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients together, except chicken breasts and rice. In
  casserole, layer chicken breasts and cover with sauce. Bake, covered, at
  350 degrees (325 degrees for glass) for 60 minutes, or until chicken is
  cooked. Serve over rice. Serves 4.
  Posted to recipelu-digest by "Diane Geary." &lt;diane@keyway.net&gt; on Feb 4,
  1998
 
</step></directions></recipe></recipeml>
