<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Flounder Teriyaki</title>
      <categories>
        <cat>None</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>6</qty>
          <unit/></amt>
        <item>Fish fillets</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Apple juice</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Lemon juice</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Onions -- chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Ginger</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Honey</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Olive oil</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Black pepper</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Cloves garlic -- chopped</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Fine</item></ing></ingredients>
    <directions>
      <step>  Preheat oven at 375.  Prepare a 13 x 9 x 2 pan lined with foil, with
  cooking spray. Place fillets in pan.  Mix remaining ingredients; spoon over
  fish.  Cover with foil and refrigerate 1 hour.  Bake covered for 15 to 20
  minutes or until fillets flake easily with fork.
  
  Recipe By     : Betty Crocker's New Choice Cookbook
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
