<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apies Cookies</title>
      <categories>
        <cat>Cookies</cat>
        <cat>And</cat>
        <cat>Bars</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit>tablespoons</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cream of tartar</item></ing>
      <ing>
        <amt>
          <qty>4 1/2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking soda</item></ing></ingredients>
    <directions>
      <step>  Mix all ingredients well. Chill dough. Roll out and cut with cookie
  cutters. Bake at 350 degrees until lightly brown.
  
  Recipe by: Key Gourmet
  
  Posted to recipelu-digest by James and Susan Kirkland &lt;kirkland@gj.net&gt; on
  Feb 18, 1998
 
</step></directions></recipe></recipeml>
