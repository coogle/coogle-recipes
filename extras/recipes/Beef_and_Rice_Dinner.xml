        <cat>Beef</cat>
        <cat>Rice</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>package</unit></amt>
        <item>Hamburger Helper *</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(16oz) Mixed Chinese Veg ***</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Condensed Beef Broth **</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Small Green Pepper ****</item></ing></ingredients>
    <directions>
      <step>  * Rice Oriental Dinner ** 10-1/2 oz *** Drained **** Thinly sliced
  
