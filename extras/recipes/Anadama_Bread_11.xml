<recipeml version="0.5">
  <recipe>
    <head>
      <title>Anadama Bread</title>
      <categories>
        <cat>Bread machi</cat>
        <cat>Breads</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>5</qty>
          <unit>ounces</unit></amt>
        <item>Water; 1cup-1 1/2 Tbs (med)</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Molasses; 2 1/2 Tbs (med)</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter Or Margarine; 2/3 Tbs (med)</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt; 1/2 tsp (med)</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Yellow cornmeal; 2 1/2 Tbs (med)</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Bread flour; 2 cups (med)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Yeast; 1 1/2 tsp (med)</item></ing></ingredients>
    <directions>
      <step>  Double small recipe for large loaf.
  
