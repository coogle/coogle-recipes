  <recipe>
    <head>
      <title>Apple Jack Gorp</title>
      <categories>
        <cat>Lunch</cat>
        <cat>Snacks</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Dried Apples</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Raisins</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Walnuts -- chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sugar, White Or Brown</item></ing></ingredients>
    <directions>
      <step>  1. Mix ingredients and bag.
  
