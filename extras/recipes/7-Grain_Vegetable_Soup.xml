<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>7-Grain Vegetable Soup</title>
      <categories>
        <cat>Soups</cat>
        <cat>Vegetarian</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>INTO A LARGE KETTLE PUT</title>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>quarts</unit></amt>
          <item>Water</item></ing></ing-div>
      <ing-div>
        <title>BRING TO A BOIL, THEN ADD</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>quart</unit></amt>
          <item>Tomatoes &amp; juice</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>AM 7 Grain Cereal (uncooked)</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>can</unit></amt>
          <item>Green beans &amp; juice</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit/></amt>
          <item>Celery stalks; chopped</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>can</unit></amt>
          <item>Whole kernel corn &amp; juice</item></ing>
        <ing>
          <amt>
            <qty>6</qty>
            <unit>ounces</unit></amt>
          <item>Jar sliced mushrooms</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Squash, cubed</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Onion powder</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Garlic powder</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Celery seed</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Sea salt; to taste OR- vegetable seasoning</item></ing>
        <ing>
          <amt>
            <qty>Bring</qty>
            <unit>to</unit></amt>
          <item>boil and simmer, covered for 1 hour.  Add 1 tablespoon of AM</item></ing></ing-div></ingredients>
    <directions>
      <step>  Canola Oil before serving, and a handful of chopped parsley.  Any other
  vegetables of choice may be added or substituted.
  
  Source: Arrowhead Mills "7 Grain Cereal Recipes" tri-fold
