<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana-Nut Oatmeal Bread</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>FOR 1-1/2 LB. LOAF</title>
        <ing>
          <amt>
            <qty>2/3</qty>
            <unit/></amt>
          <item>Buttermilk</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit/></amt>
          <item>Mashed ripe banana</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Egg</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Margarine or butter</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>+ 2 Tbsp. WW flour</item></ing>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>Rolled Oats-toasted</item></ing>
        <ing>
          <amt>
            <qty>4</qty>
            <unit>tablespoons</unit></amt>
          <item>Honey</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Ground cinnamon</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Ground Nutmeg</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Active dry yeast</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Chopped walnuts</item></ing></ing-div></ingredients>
    <directions>
      <step>  Add ingredients to machine according to manufacter's direction Use light
  setting if availible
  
  If not using a bread machine add wet ingredients first then dry. Knead,
  rest, knead, rise knead,rise,knead, form into loaf, rise Bake 350 for 1
  hour. Eat at least one slice warm right out of the oven.
  
  BH&amp;G: Bread Machine Bounty
  
  Posted to JEWISH-FOOD digest Volume 98 #008 by Sara Sutherland
  &lt;msuther@gte.net&gt; on Jan 05, 1998
 
</step></directions></recipe></recipeml>
