<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Anytime Cake</title>
      <categories>
        <cat>Ww</cat>
        <cat>Cakes</cat>
        <cat>Desserts</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Pam Courson DWHV03B</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit/></amt>
        <item>Med. banana; mashed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>package</unit></amt>
        <item>Chocolate or double fudge Alba 77</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>package</unit></amt>
        <item>Sugar twin (not Equal or Sweet n'low)</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit>dashes</unit></amt>
        <item>Molly McButter or other butter flavoring</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Baking powder</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking soda</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>drops</unit></amt>
        <item>Rum, almond, or coconut ext.</item></ing></ingredients>
    <directions>
      <step>  Mix ingredients well.  Bake in 4" x 4" pan coated with
  non-stick spray at 350 deg. for 15 min.  1 Fr, 1 M,
      1/3    B(or 25 opt cal)
  
  Reformatted for MM by Penny Schenck  nfsm70b
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
