<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Austin Chili</title>
      <categories>
        <cat>Soups</cat>
        <cat>Chili</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>pounds</unit></amt>
        <item>Grd round</item></ing>
      <ing>
        <amt>
          <qty>28</qty>
          <unit>ounces</unit></amt>
        <item>Stewed tomatoes</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Large onions</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Garlic cloves</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cans</unit></amt>
        <item>Ranch beans</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chili powder</item></ing>
      <ing>
        <amt>
          <qty>12</qty>
          <unit>ounces</unit></amt>
        <item>Beer</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Oregano</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Cumin</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Cayenne</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing></ingredients>
    <directions>
      <step>  Saute onions and garlic in oil.  Brown meat and add ingredients, except for
  beans.  Simmer for two hours and then add beans before serving.
  
  Posted by Ken Strei
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
