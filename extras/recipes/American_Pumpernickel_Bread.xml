<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>American Pumpernickel Bread</title>
      <categories>
        <cat>Bread</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing-div>
        <title>FOR 1 POUND LOAF</title>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Active dry yeast</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>2/3</qty>
            <unit>cups</unit></amt>
          <item>Rye flour</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Cornmeal</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Dry milk powder</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Oil</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>tablespoons</unit></amt>
          <item>Honey</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Caraway seed</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Caramel color or gravy browning sauce</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>Plus</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Water</item></ing></ing-div>
      <ing-div>
        <title>FOR 1-1/2 POUND LOAF</title>
        <ing>
          <amt>
            <qty>2 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Active dry yeast</item></ing>
        <ing>
          <amt>
            <qty>2 1/4</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Rye flour</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Cornmeal</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Dry milk powder</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>tablespoons</unit></amt>
          <item>Oil</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Honey</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Caraway seed</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Caramel color or gravy browning sauce</item></ing>
        <ing>
          <amt>
            <qty>1 1/4</qty>
            <unit>cups</unit></amt>
          <item>Water</item></ing></ing-div></ingredients>
    <directions>
      <step>  From: Bobb1744@aol.com
  
  Date: Tue, 6 Feb 1996 19:56:56 -0500
  
  From "The Best Bread Machine Cookbook Ever", R. Banghart Place all
  ingredients in bread machine in order suggested by manufacturer. Process on
  basic or white bread cycle.
  
