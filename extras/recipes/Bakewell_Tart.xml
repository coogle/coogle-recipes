<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bakewell Tart</title>
      <categories>
        <cat>Dessert</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>(9-inch) unbaked pie crust</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Egg</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Cake crumbs</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Ground almonds</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Almond extract</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Raspberry jam</item></ing></ingredients>
    <directions>
      <step>  Cream butter &amp; sugar; add egg &amp; beat well. Add cake crumbs, almonds,
  extract &amp; milk; mix thoroughly. Spread jam over pie crust &amp; fill with
  mixture. Bake at
      400    for 30 minutes.
  
  MRS K.R. (IRENE) WALPOLE
  
  From the &lt;The Bliss of Cooking Returns&gt;, Fort Bliss Officers Wives Club,
  Ft. Bliss, TX.  Downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
