<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Beans #02</title>
      <categories>
        <cat>Casserole</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cans</unit></amt>
        <item>(large) pork and beans</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Bell pepper; chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>large</unit></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Chili</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Barbecue sauce or tomato sauce</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>slices</unit></amt>
        <item>Bacon</item></ing></ingredients>
    <directions>
      <step>  Mix all ingredients except bacon &amp; place in a long pyrex dish. Place bacon
  on top. Bake at 350 for 1 hour.
  
  JEWELL GRIFFIN
  
