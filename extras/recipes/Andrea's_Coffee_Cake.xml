<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Andrea's Coffee Cake</title>
      <categories>
        <cat>Cyberealm</cat>
        <cat>Breakfast</cat>
        <cat>Breads</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>pounds</unit></amt>
          <item>Margarine</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit/></amt>
          <item>Eggs</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Baking soda</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Baking powder</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Sour cream</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Vanilla</item></ing></ing-div>
      <ing-div>
        <title>FOR THE FILLING</title>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Brown sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Cinnamon</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>Chopped nuts</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>===========================</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Melted butter</item></ing></ing-div></ingredients>
    <directions>
      <step>  Mix all ingredients for the cake together. Beat 3 minutes. Put 1/2
  of the batter into a well greased tube pan. Sprinkle with half the
  filling mixture. Pour remaining batter on top. Sprinkle with remaining
  filling topping. Pour 3 T melted butter over the top of the cake and
  bake at 350F for 45 min.
  
  Source: Andrea Cassoni, Cyberealm BBS originally posted 10/92
  Typed in MM format by Linda Fields, Cyberealm BBS Watertown NY
  315-786-1120
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmcyber4.zip
 
</step></directions></recipe></recipeml>
