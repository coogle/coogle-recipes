  <recipe>
    <head>
      <title>Beef Jerky</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Flank steak</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Clove garlic; minced</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Honey</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pinch</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>tablespoons</unit></amt>
        <item>Lemon juice</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Soy sauce</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pinch</unit></amt>
        <item>Salt</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients. Marinate for at least 2 hours. Dry at least 12
  hours at 150 degrees. Posted to Bakery-Shoppe Digest V1 #214 by
  dandeescott@juno.com (Daniel T. Scott) on Sep 02, 1997
 
</step></directions></recipe></recipeml>
