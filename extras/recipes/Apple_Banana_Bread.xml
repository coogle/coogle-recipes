<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Banana Bread</title>
      <categories>
        <cat>Breads</cat>
        <cat>Fruits</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Butter, softened</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Brown Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Granulated Sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>x</unit></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Sour Cream</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Banana, mashed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Baking Powder</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Baking Soda</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>x</unit></amt>
        <item>Apples, cored and chopped</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chopped Walnuts</item></ing></ingredients>
    <directions>
      <step>   Preheat oven to 350 deg F.
   Cream butter and sugars, and beat in eggs. Stir in sour cream, banana and
  vanilla.
   In separate bowl, combine flour, baking powder, soda and cinnamon.
  Gradually add to butter mixture. Gently stir in apples and nuts. Spoon into
  greased bread pan and bake 1 hour.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
