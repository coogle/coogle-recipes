<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Beef and Potato Pie</title>
      <categories>
        <cat>Main dish</cat>
        <cat>Pies</cat>
        <cat>Hamburger</cat>
        <cat>Meats</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Ground Beef</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Onion</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Carrot</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit/></amt>
        <item>Green pepper</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Sage</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Tyme</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Ketchup</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Consomme</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Instant mashed potatoes</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt &amp; pepper</item></ing></ingredients>
    <directions>
      <step>  In fry pan brown meat. Add diced vegetables, sage, tyme, ketchup &amp;
  consomme, cook and stir 3 min. Remove from heat. Mix in potato flakes, salt
  and pepper. Prepare pie crust and line pie plate then add mixture and put
  top crust on, prick top. Bake at 425 F 10 min. lower heat to 350 F and
  continue baking 30 min.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
