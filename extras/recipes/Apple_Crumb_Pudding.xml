<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Crumb Pudding</title>
      <categories>
        <cat>Desserts</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>6</qty>
          <unit/></amt>
        <item>Apples -- sliced thin</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Lemon juice</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>teaspoons</unit></amt>
        <item>Ginger</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>cups</unit></amt>
        <item>Sponge cake -- in small</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Cubes</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Margarine -- melted</item></ing></ingredients>
    <directions>
      <step>  Mix apples with lemon juice, sugar and spices.  Toss cake cubes with
  margarine. Put 1/3 of cake in greased 8"x12" dish, top with hal f of
  apples. Repeat.  Top with remaining cubes.  Cover and bake 30 min. at 375F.
  Uncover and bake 30 Min. more.
  
  Recipe By     : My files
  
  From: Ladies Home Journal- August 1991
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
