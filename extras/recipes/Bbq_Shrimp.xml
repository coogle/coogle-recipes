<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bbq Shrimp</title>
      <categories>
        <cat>Seafood</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>pounds</unit></amt>
        <item>Large shrimp</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Olive oil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Worcestershire sauce</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Minced garlic cloves</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>teaspoons</unit></amt>
        <item>Lemon juice</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Chopped parsley</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Paprika</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit/></amt>
        <item>Teapoon basil</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Thyme</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Oregano</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cayenne</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Hot pepper sauce</item></ing></ingredients>
    <directions>
      <step>  Place shrimp in large mixing bowl. Add remaining ingredients to pan.
  Combine well &amp; heat with out boiling, then chill. Pour over shrimp.
  Marinate 30 minutes, stirring from time to time. Preheat oven to 325.
  Pour shrimp &amp; sauce on baking sheet; bake 8-10 minutes. Serve at once.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmkah001.zip
 
</step></directions></recipe></recipeml>
