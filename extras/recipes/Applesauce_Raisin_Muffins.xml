  <recipe>
    <head>
      <title>Applesauce Raisin Muffins</title>
      <categories>
        <cat>Bread</cat></categories>
      <yield>18</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Oil</item></ing>
