<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana Fritters</title>
      <categories>
        <cat>None</cat></categories>
      <yield>18</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Unsifted flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Baking powder</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Egg</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Melted butter or margarine</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Drained crushed pineapple</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>small</unit></amt>
        <item>Ripe banana;peeled and diced</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Powdered sugar</item></ing></ingredients>
    <directions>
      <step>  Mix flour, sugar,baking powder and salt. Combine egg, milk and melted
  butter; add to dry ingredients. Blend in pineapple, vanilla and banana.
  Drop by teaspoonfuls into deep fat (400 degrees) and fry for 2 minutes or
  until crisp and brown.
  
  NOTE: Dip spoon into deep fat between fritters; it makes batter slide off
  spoon more easily.
  
  Remove and drain onto paper towels. Dust with powdered sugar and serve at
  once.
  
  Makes 18
  
  Recipe by: DAZEY CORPORATION
  
  Posted to recipelu-digest Volume 01 Number 518 by ctlindab@mail1.nai.net on
  Jan 14, 1998
 
</step></directions></recipe></recipeml>
