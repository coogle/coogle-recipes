<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Almost Coco's Sour Cream Dressing</title>
      <categories>
        <cat/></categories>
      <yield>24</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/4</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Buttermilk</item></ing>
      <ing>
        <amt>
          <qty>2 1/4</qty>
          <unit>cups</unit></amt>
        <item>Mayonnaise</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Cider vinegar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Parmesan cheese; grated</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Garlic salt</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Sour cream</item></ing></ingredients>
    <directions>
      <step>  In a mixing bowl, combine milk, buttermilk, mayonnaise, vinegar, Parmesan
  cheese, pepper, and garlic salt. Mix well with wire whisk. Fold in sour
  cream, leaving lumps intact. Refrigerate in a tightly closed container.
  Makes 6 cups.
