<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Walnut and Veggie Burgers</title>
      <categories>
        <cat>Burgers</cat>
        <cat>Rice</cat>
        <cat>Pasta</cat>
        <cat>Vegetarian</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>MEALS WITHOUT MEAT, TV FOOD</title>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Veg oil, divided</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit/></amt>
          <item>Red onion, chpd</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Rib celery, chpd</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Carrot, chpd</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit/></amt>
          <item>Red bell pepper, chpd</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Garlic, opt</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Walnuts, toasted, ground</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Breadcrumbs</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Cooked brown rice OR orzo pasta</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit/></amt>
          <item>Eggs</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Salt and pepper</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Buns</item></ing></ing-div>
      <ing-div>
        <title>FIXINGS</title>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Avocado slices</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Swiss cheese slices</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Red onion slices</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Catsup</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Mustard</item></ing></ing-div></ingredients>
    <directions>
      <step>  Saute onion celery, carrots and red bell pepper in 1t oil until soft.
  Cover if desired. Garlic may be added if desired. Add nuts, crumbs,
  and rice. Form into patties. Fry in 1t oil until golden.
  Place on bun and assemble.
  
