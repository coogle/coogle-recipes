<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana Streusel Coffee Cake</title>
      <categories>
        <cat>New</cat>
        <cat>Text</cat>
        <cat>Import</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Bisquick Baking Mix</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Mashed Bananas</item></ing>
        <ing>
          <amt>
            <qty>2/3</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Milk</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit/></amt>
          <item>Tbl Margarine Or Butter; ,softened</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Vanilla</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit/></amt>
          <item>Eggs</item></ing></ing-div>
      <ing-div>
        <title>STREUSEL TOPPING</title>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Chopped Nuts</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Ground Cinnamon</item></ing></ing-div></ingredients>
    <directions>
      <step>  Heat oven to 350 degrees. Grease and flour square pan 9x9x2 inches. Prepare
  Streusel Topping (mix ingredients); reserve. Mix remaining ingredients;
  beat vigorously 30 seconds. Spread in pan; sprinkle with Streusel Topping.
  Bake until wooden pick inserted in center comes out clean, 20-25 minutes.
  Serve warm or cool
