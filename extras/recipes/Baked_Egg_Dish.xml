<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Egg Dish</title>
      <categories>
        <cat>Eggs</cat></categories>
      <yield>10</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>8</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>pounds</unit></amt>
        <item>Velveeta cheese, cut in</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>1/2" cubes</item></ing>
      <ing>
        <amt>
          <qty>8</qty>
          <unit/></amt>
        <item>Slices buttered bread, cut</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>In 1/2" cubes</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Ham, cut in 1/2" cubes</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt,</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Pepper to taste</item></ing></ingredients>
    <directions>
      <step>  Beat eggs;add milk and remaining ingredients.Pour into 9 x 13" buttered
  pan.Cover;refrigerate overnight.In the morning,bake,uncovered, @ 350
  degrees about 45 minutes.It should bubble and be golden brown. Serves 8 to
  10.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
