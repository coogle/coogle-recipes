<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbecued Spareribs #5</title>
      <categories>
        <cat>Meat</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>5</qty>
          <unit>pounds</unit></amt>
        <item>Spare ribs</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Hickory smoke salt</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Vinegar</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Liquid smoke and salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Catsup</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Steak sauce</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Minced onion</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Brown sugar</item></ing></ingredients>
    <directions>
      <step>  From: Bobb1744@aol.com
  
  Date: Thu, 4 Apr 1996 21:37:02 -0500
  
  Place spareribs on rack in baking pan. Brown in a 400 degree oven for 15
  minutes; turn and brown on other side for 10 ot 15 minutes. Drain fat and
  put ribs in crock pot. Combine remaining ingredients and pour over ribs.
  Cover and cook on low 6 to 8 hours. Makes 5 to 6 servings.
  
