<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Alaskan Seafood Marinade</title>
      <categories>
        <cat>Tabasco</cat>
        <cat>Seafood</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>8</qty>
          <unit>tablespoons</unit></amt>
        <item>Unsalted butter or margarine</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Brown sugar; (packed)</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Honey</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>tablespoons</unit></amt>
        <item>Fresh lemon juice</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Liquid smoke flavoring</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Dried rosemary</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>TABASCO brand Pepper Sauce</item></ing></ingredients>
    <directions>
      <step>  In medium saucepan, over medium heat, combine all ingredients. Cook,
  stirring until smooth, 5 to 7 minutes. Cool to room temperature. Marinate
  seafood 30 minutes prior to grilling. Makes approximately l l/2 cups.
  
