<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baby Ruth Cookies</title>
      <categories>
        <cat>Cookies</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Butter, softened</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Peanut butter, smooth</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs, beaten</item></ing>
      <ing>
        <amt>
          <qty>1 1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Flour, sifted</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Baking powder</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit/></amt>
        <item>Baby Ruth bars, 1.9oz, cutup</item></ing></ingredients>
    <directions>
      <step>  Cream butter, peanut butter and sugar until light and fluffy; beat in
  beaten eggs ad mix well; add vanilla. Sift dry ingredients together and add
  alternately with the milk. Stir in the cut-up Baby Ruth pieces. Drop from
  teaspoon on buttered cookie sheets. Bake in preheated 375~F oven until
  golden brown, about 12-15 minutes.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
