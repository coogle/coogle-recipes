<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Ancho-Oregano-Pecan-Butter</title>
      <categories>
        <cat>Sauce</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Cloves garlic minced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Ancho chile; briefly toasted and ground in a spice grinder</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Chopped fresh oregano</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Gold Tequila</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>(generous) chopped pecans</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt and pepper to taste</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>pounds</unit></amt>
        <item>Unsalted butter</item></ing></ingredients>
    <directions>
      <step>  From: gemini@synapse.net (Georgette &amp; Dave Burnside)
  
  Date: Sun, 11 Feb 1996 05:30:21 GMT
  
  Mix all the ingredients together well. Melt to serve.
  
