<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Ancini Di Pepe</title>
      <categories>
        <cat>Desserts</cat>
        <cat>Italian</cat>
        <cat>Pasta</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Ancini Di Pepe Macaroni,</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Cooked -- drained &amp; rinsed</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Well</item></ing>
      <ing>
        <amt>
          <qty>20</qty>
          <unit>ounces</unit></amt>
        <item>Can Crushed Pineapple</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>large</unit></amt>
        <item>Can Mandarin Oranges (13-16oz?)</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>small</unit></amt>
        <item>Conatiner Cool Whip</item></ing></ingredients>
    <directions>
      <step>  Cook juice from crushed pineapple and mandarin oranges with sugar, flour,
  eggs, and salt until thick. Pour over macaroni and fruit mixture. Cool 4
  hours or overnight. Add 1 small conatiner of cool whip. Mix well and
  refrigerate
  
  Recipe By     : Kathy Weeks &lt;KWEEKS@OA.ITHACA.EDU&gt;
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
