<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Waldine's Shrimp Dip</title>
      <categories>
        <cat>Appetizers</cat>
        <cat>Dips</cat>
        <cat>Seafood</cat>
        <cat>Wrv</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>WALDINE VAN GEFFEN VGHC42A</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>pounds</unit></amt>
        <item>Shrimp; boil, peel, devein</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Mayo</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>large</unit></amt>
        <item>Cream cheese; soft</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Cayenne</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Lemon juice</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Worcestershire</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Onion powder; or</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>small</unit></amt>
        <item>Onion; grate</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>teaspoons</unit></amt>
        <item>Garlic; mince</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Horseradish</item></ing></ingredients>
    <directions>
      <step>  Chop shrimp and mix with all ingredients. Refrigerate until time to serve.
