<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple-Nut Pound Cake</title>
      <categories>
        <cat>Cakes</cat>
        <cat>Fruits</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking soda</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Diced apples</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Walnuts</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing></ingredients>
    <directions>
      <step>   Beat eggs.  Add oil, vanilla &amp; dry ingredients.  Fold in apples &amp; walnuts.
  Bake in greased &amp; floured tube pan at 350 for 1 1/4 hrs.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
