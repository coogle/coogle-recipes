  <recipe>
    <head>
      <title>Amaretto Mousse Cheesecake</title>
      <categories>
        <cat>Cheesecakes</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>packages</unit></amt>
        <item>GRAHAM CRACKERS, crushed</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>SUGAR</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>tablespoons</unit></amt>
        <item>BUTTER, melted</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Mix and pat into bottom and up sides of 10" springform pan.</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>CAKE</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Envelopes KNOX GELATIN</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>WATER</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>pounds</unit></amt>
        <item>CREAM CHEESE, unwrapped and warmed in</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Microwave 2 minutes on high</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>SUGAR</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>(5 oz.) can EVAPORATED MILK</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>LEMON JUICE</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>AMARETTO</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>VANILLA</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>WHIPPING CREAM, whipped</item></ing></ingredients>
    <directions>
