<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Pineapple #3</title>
      <categories>
        <cat>Fruits</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Crushed pineapple; (20 oz.)</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs; well beaten</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Flour</item></ing></ingredients>
    <directions>
      <step>  Mix and pour into buttered casserole or baking pan. Bake 1/2 to 3/4 of an
  hour at 350 degrees. Brown 3 slices of cubed bread in butter. Place on top
  of baked pineapple. Serves 6 to 8 persons.
  Posted to recipelu-digest by "Diane Geary" &lt;diane@keyway.net&gt; on Feb 23,
  1998
 
</step></directions></recipe></recipeml>
