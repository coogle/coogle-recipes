<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apricot Coconut Balls</title>
      <categories>
        <cat>Candies</cat>
        <cat>Holiday</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Dried apricots finely chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Coconut; shredded</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Sweetened condensed milk</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Powdered sugar</item></ing></ingredients>
    <directions>
      <step>  Mix apricots and coconut. Add condensed milk and blend well. Shape into
  balls and roll in powdered sugar. Let stand until firm. MAKES: 32 (1 1/4
  inch) balls
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
