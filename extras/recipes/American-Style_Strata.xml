<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>American-Style Strata</title>
      <categories>
        <cat>None</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>12</qty>
          <unit>slices</unit></amt>
        <item>Sourdough bread -- or</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>slices</unit></amt>
        <item>Whole-grain bread -- halved</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1 3/4</qty>
          <unit>cups</unit></amt>
        <item>Mozzarella or monterey jack</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Dry mustard</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Cooked beef -- lamb or pork</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground black pepper</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Cut bite-size</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Worcestershire</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Carrot or zucchini -- shred</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Dried thyme leaves --</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Crumble</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Dried basil leaves --</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Crumble</item></ing></ingredients>
    <directions>
      <step>  In a 10x6x2 baking dish, layer bread slices to cover bottom of dish.
  Sprinkle cheese, meat and veggies over bread. In a bowl, stir togeteher the
  eggs, milk and seasonings till well mixed. Pour carefully over ingredients
  in baking dish. Cover. Let stand 1 hour at room temperature or in fridge
  for longer periods. Uncover. Bake 325~ for 50 to 60 minutes or until center
  is nearly set. Let stand 10 minutes. Cut into squares.
  
  Recipe By     :
  
  From: Ladies Home Journal- August 1991
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
