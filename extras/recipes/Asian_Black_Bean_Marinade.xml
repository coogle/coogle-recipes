<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Asian Black Bean Marinade</title>
      <categories>
        <cat>Marinades</cat>
        <cat>Bar-b-q</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>FOR POULTRY, PORK, LAMB</title>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Black bean sauce</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Dry mustard</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Dry sherry</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Soy sauce</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Sesame oil</item></ing></ing-div></ingredients>
    <directions>
      <step>  Combine all ingredients in a bowl.  Makes about 1/2 cup
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
