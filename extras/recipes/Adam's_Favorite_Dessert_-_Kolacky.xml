<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Adam's Favorite Dessert - Kolacky</title>
      <categories>
        <cat>Kaz</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Unsalted butter</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Confectioners' sugar</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit/></amt>
        <item>Egg</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Almond extract</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla extract</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cans</unit></amt>
        <item>Solo fruit filling</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit>cups</unit></amt>
        <item>Sifted flour</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Additional powdered sugar</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Yolks</item></ing></ingredients>
    <directions>
      <step>  Cream butter and sugar.  Add egg yolks, one at a time, beating well and
  flavorings.  Gradually add flour, dough will be stiff. Roll tablespoon size
  into balls. Place well apart on greased tin. Make thumb print in each ball
  and fill with Solo filling. Sift powdered sugar over when done.
  
  Recipe By     :
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
