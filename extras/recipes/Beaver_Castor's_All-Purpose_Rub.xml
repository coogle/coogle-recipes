  <recipe>
    <head>
      <title>Beaver Castor's All-Purpose Rub</title>
      <categories>
        <cat>Seasoning</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>cups</unit></amt>
        <item>Garlic salt</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>cups</unit></amt>
        <item>Onion salt</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>cups</unit></amt>
        <item>Celery salt</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>cups</unit></amt>
        <item>Seasoned salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Paprika</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Chile seasoning</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Dry mustard</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Oregano or Cumin</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Ginger</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Cloves</item></ing></ingredients>
    <directions>
      <step>  From: cstarz@teleport.com (Carey Starzinger)
  
  Date: Sun, 14 Jul 1996 13:03:02 -0700
  Combine ingredients in a bowl or jar with a screw-on lid.  Shake until
  thoroughly blended.
  
