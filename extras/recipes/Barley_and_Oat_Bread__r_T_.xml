<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barley and Oat Bread &lt;r T&gt;</title>
      <categories>
        <cat>Bread machi</cat>
        <cat>Breads</cat>
        <cat>Low fat</cat></categories>
      <yield>10</yield></head>
    <ingredients>
      <ing-div>
        <title>1 1/2 LB LOAF</title>
        <ing>
          <amt>
            <qty>2 1/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Active dry yeast</item></ing>
        <ing>
          <amt>
            <qty>2 1/2</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>Barley flour</item></ing>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>Rolled oats</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Wheat germ</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Nonfat dry milk powder</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Orange zest; grated</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Sesame seeds; optional</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Honey</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Wonderslim; * see note</item></ing>
        <ing>
          <amt>
            <qty>1 1/3</qty>
            <unit>cups</unit></amt>
          <item>Water</item></ing></ing-div>
      <ing-div>
        <title>1 LB LOAF</title>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Active dry yeast</item></ing>
        <ing>
          <amt>
            <qty>1 2/3</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Barley flour</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Rolled oats</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Wheat germ</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Nonfat dry milk powder</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Orange zest; grated</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Sesame seeds; optional</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Honey</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Wonderslim; * see note</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>Water; + 2 Tablespoons</item></ing></ing-div></ingredients>
    <directions>
      <step>  *NOTE: The original recipe used 1 1/2 T vegetable oil (1 T).
  
  A moist and chewy loaf like this one makes good peanut butter, tuna, or
  fresh ham sandwiches and excellent toast. It goes well with vegetable and
  meat soups, especially Scotch broth.
  
  Add all ingredients in the order suggested by your bread machine manual and
  process on the basic bread cycle according to the manufacturer's
  directions. Eat warm - but not hot - or cool.
  
  Entered into MasterCook II and tested for you by Reggie Dwork
  
  Recipe by: Best Bread Machine Cookbook Ever,Madge Rosenberg(R.
  
  Posted to recipelu-digest by "Christopher E. Eaves" &lt;cea260@airmail.net&gt; on
  Mar 04, 1998
 
</step></directions></recipe></recipeml>
