<recipeml version="0.5">
  <recipe>
    <head>
      <title>Ambrosia Fruit Salad</title>
      <categories>
        <cat>Brunch</cat>
        <cat>Christmas</cat>
        <cat>Fruit</cat>
        <cat>Children</cat>
        <cat>Salads</cat></categories>
      <yield>20</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>2.00 cn Mandarin oranges; drained</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>2.00 Pineapple; tidbits; drained</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>2.00 ea Banana; sliced</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>2.00 c Grapes; green or red seedless</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>2.00 Vanilla yogurt</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>1.00 c Almonds; slivered</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>2.00 c Coconut; flaked</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>2.00 c Marshmallows; mini</item></ing></ingredients>
    <directions>
      <step>  Mix all ingredients together and chill.
