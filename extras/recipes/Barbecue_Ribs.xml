<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbecue Ribs</title>
      <categories>
        <cat>Meats</cat>
        <cat>Sauces</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>pounds</unit></amt>
        <item>Spareribs Cut To Serve</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Catsup</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Dry Mustard</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Dark Rum</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Cloves Garlic, Crushed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Brown Sugar, Firmly Packed</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Soy Sauce</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chili Sauce</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Worcestershire Sauce</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Dash Pepper</item></ing></ingredients>
    <directions>
      <step>  Wrap ribs in double thickness of foil and bake for 1 1/2 hours at 350
  degrees F.  Unwrap and drain drippings.  Combine all ingredients and pour
  over ribs.  Marinate at room temperature for 1 hour.  Bake at 350 degrees
  30 minutes, basting wiht sauce, or grill 30 minutes, 4 inches above coals
  turning and basting.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/meats.zip
 
</step></directions></recipe></recipeml>
