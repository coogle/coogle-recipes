<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Cabbage</title>
      <categories>
        <cat>Vegetables</cat>
        <cat>Low fat</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Head cabbage; quarter</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Fresh dill</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Vegetable broth</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Apple; peel, chop</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>White wine; optional</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Fresh parsley; chopped</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Non-fat mozzarella; shredded</item></ing></ingredients>
    <directions>
      <step>  Put cabbage in casserole. Top with onion, apples, cheese and parsley. Put
  broth in bottom. Cover with foil and bake 350~ until cabbage is tender.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
