  <recipe>
    <head>
      <title>Banana Babe</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>ounces</unit></amt>
        <item>Bacardi rum</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>ounces</unit></amt>
        <item>Nassau Royale</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>ounces</unit></amt>
        <item>Pineapple juice</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>ounces</unit></amt>
