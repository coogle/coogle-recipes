    <head>
      <title>Aliter Baedinam Sive Agninam Excaldatam</title>
      <categories>
        <cat>Meat</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>10</qty>
          <unit/></amt>
        <item>Lamb cutlets</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>liter</unit></amt>
        <item>White wine</item></ing>
      <ing>
        <amt>
          <qty>100</qty>
          <unit>mililiters</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Big onions; diced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Ground coriander</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Ground pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Liebstoeckl</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Ground cumin</item></ing>
      <ing>
        <amt>
          <qty>200</qty>
          <unit>mililiters</unit></amt>
        <item>Liquamen -or-</item></ing>
