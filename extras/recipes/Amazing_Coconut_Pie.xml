<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Amazing Coconut Pie</title>
      <categories>
        <cat>Coconut</cat>
        <cat>Pies</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Bisquick</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Coconut</item></ing></ingredients>
    <directions>
      <step>  Combine everything but coconut in blender on low for 3 minutes. Pour into
  pie plate, let sit for 5 min. Top with coconut. Bake at 350 for 40 minutes.
  Posted to recipelu-digest Volume 01 Number 580 by RecipeLu
  &lt;recipelu@geocities.com&gt; on Jan 23, 1998
 
</step></directions></recipe></recipeml>
