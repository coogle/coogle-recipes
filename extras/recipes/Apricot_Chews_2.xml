<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apricot Chews</title>
      <categories>
        <cat>Cookies</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Coconut</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>(6 oz) pack finely chopped</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Dried apricots</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Sweetened condensed milk</item></ing></ingredients>
    <directions>
      <step>  Heat oven to 350. Grease cookie sheets. Mix all ingredients until well
  blended. Roll rounded teaspoonfuls into balls. Place 1" apart on
  sheets. Bake 12 minutes or until set. Remove to wire rack to cool.
  Store in airtight containers up to 2 weeks.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmkah001.zip
 
</step></directions></recipe></recipeml>
