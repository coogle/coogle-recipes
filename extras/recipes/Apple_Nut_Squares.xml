<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Nut Squares</title>
      <categories>
        <cat>Bars</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>SQUARES</title>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>Salad oil</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit/></amt>
          <item>Eggs</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Baking soda</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Pared; chopped apples</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Vanilla</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Chopped nuts</item></ing></ing-div>
      <ing-div>
        <title>TOPPING</title>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Butter</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Firmly packed brown sugar</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Evaporated milk</item></ing></ing-div></ingredients>
    <directions>
      <step>  Squares: Combine sugar, oil, eggs, soda, flour, apples, vanilla and nuts.
  Mix well; batter is stiff. Spread evenly in a greased 13 x 9 x 2-inch pan.
  Bake for 1 hour in a preheated oven at 325 degrees. Cake can be iced while
  still warm. Topping: Combine all ingredients in a small saucepan. Cook over
  low heat. Bring to a boil and cook for 2 minutes longer. Spread topping on
  squares. Cool cake squares completely before cutting.
  
  Recipe by: diane@keyway.net
  
  Posted to recipelu-digest Volume 01 Number 294 by "Diane Geary"
  &lt;diane@keyway.net&gt; on Nov 23, 1997
 
</step></directions></recipe></recipeml>
