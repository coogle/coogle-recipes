  <recipe>
    <head>
      <title>Barbecued Chicken and Andouille Hash</title>
      <categories>
        <cat>Poultry</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>6</qty>
            <unit>ounces</unit></amt>
          <item>Chicken breast</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Prepared bbq sauce</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Salt and pepper</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Olive oil</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Cooked potatoes; 1/2" dice</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Onions; diced small</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Shallots; minced</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Andouille sausage (8oz); chopped</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Garlic; minced</item></ing></ing-div>
      <ing-div>
        <title>POACHED EGGS</title>
        <ing>
          <amt>
            <qty>4</qty>
            <unit/></amt>
          <item>Eggs</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Green onions; sliced</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Sauce picante (see recipe)</item></ing>
        <ing>
          <amt>
            <qty>Date:</qty>
            <unit>27</unit></amt>
          <item>Apr 96 22:30:57 EDT</item></ing></ing-div></ingredients>
