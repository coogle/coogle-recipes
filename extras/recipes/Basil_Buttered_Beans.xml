<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Basil Buttered Beans</title>
      <categories>
        <cat>Digest</cat>
        <cat>Oct.</cat>
        <cat>Lacto</cat>
        <cat>Fatfree</cat></categories>
      <yield>2</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Green Beans -- fresh, cut</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Into 2" pieces</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Chopped Onion</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Chopped Celery</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Butter Buds. -- or butter,</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Melted</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Fresh Basil -- minced, or</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Dry</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Pieces</item></ing></ingredients>
    <directions>
      <step>  REVISED: the original recipe called for 2 T butter or margarine melted
  
  In a saucepan, combine beans, onion, celery and water.  Cover and cook for
  5 min or until beans are tender.  Drain.  Add the butter, basil, salt and
  pepper; stir to coat.  Serve immediately.
  
