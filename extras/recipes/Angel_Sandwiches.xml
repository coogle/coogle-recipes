<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Angel Sandwiches</title>
      <categories>
        <cat>Cookies</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Shortending (part butter)</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Confectioners sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Cut up dates</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Raisins</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Sweetened Whipped Cream</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Topping</item></ing></ingredients>
    <directions>
      <step>  Heat oven to 350. Mix shortening &amp; sugar. Stir flour into shortening
  mixture. Press &amp; flatten with hand to cover bottom of ungreased
  oblong pan. Bake 15 minutes. Blend fruit, sugar &amp; water in saucepan.
  Cook over low heat, stirring constantly, until thickened, 10 minutes.
  Cool both crust &amp; filling. Spread filling over crust; spread topping
  over filling. Chill before cutting in 1" squares. Store in
  refrigerator.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmkah001.zip
 
</step></directions></recipe></recipeml>
