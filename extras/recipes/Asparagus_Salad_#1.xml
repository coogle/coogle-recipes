<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Asparagus Salad #1</title>
      <categories>
        <cat>Salad</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(15-oz) asparagus; chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Chopped nuts</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Chopped celery</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Jar (2-oz) pimiento; chopped</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>packages</unit></amt>
        <item>Unflavored gelatin; soften in: cold water</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Vinegar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit/></amt>
        <item>Lemon; juice of</item></ing></ingredients>
    <directions>
      <step>  Heat vinegar, salt, sugar and lemon juice. Add gelatin and stir. Add
  asparagus, nuts, celery and pimiento. Pour into 1-1/2 quart mold and
  refrigerate until firm. Good to serve with meat. Serves 8-10.
  
  MRS A.B. THOPMSON (BETTY)
  
  MARVELL, AR
  
  From the book &lt;High Cotton Cookin'&gt;, Marvell Academy Mothers Assn, Marvell,
  AR  72366, ISBN 0-918544-14-9, downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
