<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Crabmeat</title>
      <categories>
        <cat>Seafood</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit/></amt>
        <item>French bread sliced 1"</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Thick</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Onion, large -- finely</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Chopped</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Milk or enough to soak</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Bread</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Celery rib -- finely</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Egg -- well beaten</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Crabmeat</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt and pepper to taste</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Parsley -- chopped</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Bread crumbs</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Garlic cloves -- finely</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Minced</item></ing></ingredients>
    <directions>
      <step>  In a skillet, saute onions and celery in butter until clear and tender. Add
  crabmeat, parsley, and garlic. Stir fry about 10 minutes. Add French bread
  and egg. Salt and pepper to taste. Mix well. Place in a buttered baking
  dish. Cover with bread crumbs. Bake at 350 degrees for 30 to 40 minutes.
  From "Jambalaya, Crawfish Pie, File Gumbo" formatted by Mary Dishongh
  Bowles.
  
  Recipe By     :
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
