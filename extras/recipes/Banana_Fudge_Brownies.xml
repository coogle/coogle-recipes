<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana Fudge Brownies</title>
      <categories>
        <cat>Cookie</cat></categories>
      <yield>24</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Margarine</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Chocolate chips</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Hodgson Mill 50/50 flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Banana</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking powder</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Egg Beaters 99% egg substitute</item></ing>
      <ing>
        <amt>
          <qty>From:</qty>
          <unit/></amt>
        <item>charles moffat &lt;moffats@USAOR.NET&gt;</item></ing>
      <ing>
        <amt>
          <qty>Date:</qty>
          <unit/></amt>
        <item>Thu, 3 Jan 1980 13:30:15 -0500</item></ing></ingredients>
    <directions>
      <step>  Recipe By: Betty Crocker's Snacks, Salads &amp; Desserts
  
  Melt margarine &amp; chocolate chips in a 3 qt saucepan over low heat, stirring
  constantly. Beat in remaining ingredients with a wooden spoon until
  thoroughly blended. Spread evenly in a 13 x 9 pan ( grease bottom only).
  
  Bake at 350F for about 30 minutes or until center is set. Cool completely.
  
  NOTES : 2 eggs can be used instead of EggBeaters.  2/3 cup of chopped nuts
  can be added.
  
  EAT-L Digest 13 July 96
  
  From the EAT-L recipe list.  Downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
