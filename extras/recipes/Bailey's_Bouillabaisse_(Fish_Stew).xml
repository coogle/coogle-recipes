<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bailey's Bouillabaisse (Fish Stew)</title>
      <categories>
        <cat>Fish</cat>
        <cat>Main dish</cat></categories>
      <yield>9</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Onion, large diced</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Potatoes, small diced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Skimmed milk</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>pounds</unit></amt>
        <item>Cod or Flounder</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Cauliflower, diced</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Basil</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Celery, diced</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Boiling water</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Broccoli, diced</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Marjoram</item></ing></ingredients>
    <directions>
      <step>  Cook the onion, celery, and potatoes in the water for 10 minutes.  Mix a
  little milk with the flour to form a soupy paste and set aside.  Add the
  remaining ingredients and bring to a boil.  Add the flour mixture,
  stirring constantly.  Simmer for 15 to 20 minutes.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmealnew.zip
 
</step></directions></recipe></recipeml>
