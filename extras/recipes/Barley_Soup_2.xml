<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barley Soup</title>
      <categories>
        <cat>Beef</cat>
        <cat>Soups</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Stew meat</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Clove garlic</item></ing>
      <ing>
        <amt>
          <qty>7</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>32</qty>
          <unit>ounces</unit></amt>
        <item>Tomatoes</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Bay leaf</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Barley</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Celery</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Carrots</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Cubes beef bouillon</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Basil</item></ing>
      <ing>
        <amt>
          <qty>10</qty>
          <unit>ounces</unit></amt>
        <item>Green beans</item></ing></ingredients>
    <directions>
      <step>  Brown beef in Dutch oven with oil, garlic and onions. Add remaining
  ingredients except beans and simmer 1 hour. Add beans and simmer another 20
  minutes.
  
  Recipe by: Scotch Brand
  
  Posted to TNT - Prodigy's Recipe Exchange Newsletter  by "LauraG"
  &lt;gaggini@gate.usaor.net&gt; on Nov 25, 1997
 
</step></directions></recipe></recipeml>
