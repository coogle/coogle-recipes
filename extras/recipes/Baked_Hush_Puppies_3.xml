<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Hush Puppies</title>
      <categories>
        <cat>Breads</cat></categories>
      <yield>24</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Yellow Cornmeal</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Baking Powder</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Minced Onion</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Skim Milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Vegetable Oil</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Egg Beaten</item></ing></ingredients>
    <directions>
      <step>  Combine Cornmeal, Flour, Baking Powder &amp; Salt in A Medium Bowl; Make A Well
  in The Center Of Mixture. Combine Onion, Milk, Oil, Pepper &amp; Egg.Add To Dry
  Ingredients, Stirring Just Until Moistened. Spoon 1 T. Batter Into Minature
  Muffin Pans Coated With Cooking Spray. Bake At 450 For 10 Minutes OR Until
  Lightly Browned. Remove From Pan Immediately.
    (Fat 1. Chol. 11.)
 
</step></directions></recipe></recipeml>
