<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Pineapple Pudding</title>
      <categories>
        <cat>Desserts</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>5</qty>
          <unit>slices</unit></amt>
        <item>Bread</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(Large) Crushed Pineapple</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pinch</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Flour</item></ing></ingredients>
    <directions>
      <step>  Cut 5 slices of bread in cubes. Fry in 3/4 cup of butter until crisp. Mix
  all ingredients and put in greased casserole. Bake at 350 degrees for 1/2
  hour.
  
  Recipe by: Key Gourmet
  
  Posted to recipelu-digest by James and Susan Kirkland &lt;kirkland@gj.net&gt; on
  Feb 24, 1998
 
</step></directions></recipe></recipeml>
