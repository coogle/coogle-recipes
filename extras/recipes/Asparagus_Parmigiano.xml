<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Asparagus Parmigiano</title>
      <categories>
        <cat>Vegetables</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>LISA CRAWLEY TSPN00B-----</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>pounds</unit></amt>
        <item>Fresh Asparagus -- cooked</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Onion -- chopped</item></ing>
      <ing>
        <amt>
          <qty>10</qty>
          <unit>mililiters</unit></amt>
        <item>Garlic -- minced</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Tabasco sauce</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Cn Tomatoes</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Thyme</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>8 ounces Cn</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>ounces</unit></amt>
        <item>Mozzarella cheese -- thin</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Slic</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Parmesan -- grated</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Tomato Sauce</item></ing></ingredients>
    <directions>
      <step>  Drain cooked asparagus; arrange in shallow baking dish. Saute onion and
  garlic in oil in saucepan til golden. Add salt, Tabasco sauce, and
  tomatoes; simmer, uncovered, 10 min. Add thyme and tomato sauce; simmer 20
  min.  Pour over asparagus.  Place slices of mozzarella cheese over top.
  Sprinkle with Parmesan.  Bake in preheated 350 oven 30 min. Yield: 6-8
  servings.
  
  Recipe By     :
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
