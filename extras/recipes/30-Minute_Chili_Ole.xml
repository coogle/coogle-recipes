<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>30-Minute Chili Ole</title>
      <categories>
        <cat>Chili</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Onion -- chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Cloves garlic -- minced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Vegetable oil</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>pounds</unit></amt>
        <item>Ground beef</item></ing>
      <ing>
        <amt>
          <qty>15</qty>
          <unit>ounces</unit></amt>
        <item>Can tomato sauce</item></ing>
      <ing>
        <amt>
          <qty>14 1/2</qty>
          <unit>ounces</unit></amt>
        <item>Can stewed tomatoes</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>A-1 Steak Sauce</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Chili powder</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Ground cumin</item></ing>
      <ing>
        <amt>
          <qty>16</qty>
          <unit>ounces</unit></amt>
        <item>Can black beans</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Rinsed/drained</item></ing>
      <ing>
        <amt>
          <qty>11</qty>
          <unit>ounces</unit></amt>
        <item>Can corn -- drained</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Shredded cheese</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Sour cream and</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Chopped tomato -- for</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Garnish</item></ing></ingredients>
    <directions>
      <step>  In 6 qt. heavy pot, over medium-hight heat, saute onion and garlic in oil
  until tender. Add beef; cook and stir until browned. Drain; stir in tomato
  sauce, stewed tomatoes, steak sauce, chili powder and cumin. Heat to a
  boil; reduce heat to low. Cover; simmer for 10 minutes, stirring
  occasionally.   Stir in beans and corn; simmer, uncovered, for 10 minutes.
  Serve hot; garnish with cheese, sour cream and tomato.
  
  Recipe By     : Sizzling Mexican
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
