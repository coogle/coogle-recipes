<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked French Onion Veggies</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>medium</unit></amt>
        <item>Potatoes</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>large</unit></amt>
        <item>Carrots</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>large</unit></amt>
        <item>Onion</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit/></amt>
        <item>Cloves garlic</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Sweet potato</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Any kind of beans (green; butter)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Parsnip</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Some whole mushrooms</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>packages</unit></amt>
        <item>French onion soup mix</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Approx 1 1/2 cups water</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Mixed herbs</item></ing></ingredients>
    <directions>
      <step>  Preheat oven to fairly hot, chop all veggies and put in an oven bag. Mix
  soup packets with water, pour onto veggies and seal bag. Put in oven for
  approx. 30 mins or until veggies are tender.
  
  Serves: 4
  Posted to recipelu-digest by "Bunny" &lt;layla696@ix.netcom.com&gt; on Mar 20,
  1998
 
</step></directions></recipe></recipeml>
