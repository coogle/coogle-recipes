<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Alaska Salmon Chowder</title>
      <categories>
        <cat>Soups</cat>
        <cat>Fish</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>7 1/2</qty>
          <unit>ounces</unit></amt>
        <item>Canned Alaska salmon</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chopped onions</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chopped celery</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Garlic clove; minced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Margarine</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Diced potatoes</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Diced carrots</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Low salt chicken broth</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Thyme</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Black pepper</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chopped broccoli</item></ing>
      <ing>
        <amt>
          <qty>13</qty>
          <unit>ounces</unit></amt>
        <item>Low-fat evaporated milk</item></ing>
      <ing>
        <amt>
          <qty>10</qty>
          <unit>ounces</unit></amt>
        <item>Frozen corn kernels; thawed</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Minced parsley</item></ing></ingredients>
    <directions>
      <step>  Drain and flake salmon, reserving liquid. Saute onions, celery and garlic
  in margarine. Add potatoes, carrots, reserved salmon liquid, chicken broth
  and seasonings. Simmer, covered, 20 minutes, or until vegetables are nearly
  tender. Add broccoli and cook 5 minutes. Add flaked salmon, evaporated milk
  and corn; heat thoroughly. Sprinkle with minced parsley to serve.
  
  Nutrients Per Serving:
  
  Calories:               238 Protein: 15.6 g Fat-Total: 5.71 g
  Carbohydrates:          33 g Cholesterol: 21.9 mg Sodium: 552 mg Dietary
  Fiber:          4.26 g
  
