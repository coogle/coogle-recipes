<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>2 Tangy Sweet Bbq Sauce</title>
      <categories>
        <cat>Sauces/dres</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>large</unit></amt>
        <item>Onion</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>tablespoons</unit></amt>
        <item>Vinegar</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>tablespoons</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Worcestershire sauce</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Ketchup</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt; to taste</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Cayenne; to taste</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>tablespoons</unit></amt>
        <item>Margarine</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Lemon juice</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>teaspoons</unit></amt>
        <item>Mustard</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>teaspoons</unit></amt>
        <item>Chili powder</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Tabasco sauce; to taste</item></ing></ingredients>
    <directions>
      <step>  Chop onion. Saute in margerine until lightly colored. Add all other
  ingredients and simmer for 30 min. makes 7 cups.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
