<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Beef and Green Chile Stew</title>
      <categories>
        <cat>Crockpot</cat>
        <cat>Beef</cat>
        <cat>Stews</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>pounds</unit></amt>
        <item>Round steak; cut in 1" cubes</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Cloves garlic; minced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Lard or butter</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Or more green chile peppers; seeded &amp; chopped</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Dried oregano</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Tomatillo pulp</item></ing></ingredients>
    <directions>
      <step>  Dust meat with flour. Brown with onion and garlic in lard or butter.
  Combine the meat and remaining ingredients in slow cooker with 1/2 cup
  water. Cover and cook on low 6 to 8 hours. Garnish with coriander and serve
  with tortillas.
  Posted to recipelu-digest Volume 01 Number 406 by James and Susan Kirkland
  &lt;kirkland@gj.net&gt; on Dec 27, 1997
 
</step></directions></recipe></recipeml>
