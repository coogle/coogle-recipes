<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbecued Leg of Lamb with Yogurt</title>
      <categories>
        <cat>Burmese</cat>
        <cat>Lamb</cat>
        <cat>Bar-b-q</cat>
        <cat>Mid east</cat>
        <cat>Yogurt</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>10</qty>
          <unit>pounds</unit></amt>
        <item>Leg of lamb</item></ing>
      <ing>
        <amt>
          <qty>2 1/2</qty>
          <unit>cups</unit></amt>
        <item>Plain yogurt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Tumeric</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Paprika</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Cumin</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cloves</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Nutmeg</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit>tablespoons</unit></amt>
        <item>Veg oil</item></ing></ingredients>
    <directions>
      <step>  Bone the lamb and cut into 1" cubes. Combine all
  ingredients in a large bowl, mix, cover and
  refrigerate overnight. Skewer cubes and grill over
  charcoal  about 7 min. per side.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
