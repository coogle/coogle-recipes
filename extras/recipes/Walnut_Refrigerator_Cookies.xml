<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Walnut Refrigerator Cookies</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Fat (margarine)</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Light corn syrup</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Egg</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla extract</item></ing>
      <ing>
        <amt>
          <qty>2 1/2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chopped nuts</item></ing></ingredients>
    <directions>
      <step>  1. Cream the fat (margarine) and sugar together
  
  2. Add corn syrup, unbeaten egg and vanilla. Beat well.
  
  3. Add siften dry ingredients, 1/2/3 at a time. Mix well before adding the
  remaining flour. Stir in the wet ingredients
  
  4. Roll in wax paper. Chill overnight
  
  5. Slice very thin. Bake in 400 degree oven for 10-12 minutes. Makes 5
  dozen.
  
  6. Keep roll in refrigerator and bake as you wish. Posted to TNT -
  Prodigy's Recipe Exchange Newsletter  by JeannePie@aol.com on Jul 5, 1997
 
</step></directions></recipe></recipeml>
