<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana Blueberry Nut Bran Muffins</title>
      <categories>
        <cat>Muffins</cat></categories>
      <yield>24</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>White sugar</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Ripe bananas; pureed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>package</unit></amt>
        <item>Frozen blueberries</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>liter</unit></amt>
        <item>Cup chopped walnuts</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking soda</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Oat bran</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>quart</unit></amt>
        <item>Buttermilk</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>cups</unit></amt>
        <item>Kellogg's All Bran</item></ing>
      <ing>
        <amt>
          <qty>2 1/2</qty>
          <unit>cups</unit></amt>
        <item>Unsifted flour</item></ing></ingredients>
    <directions>
      <step>  Makes approximately 2 dozen muffins. Combine first 8 ingredients. Add
  remaining ingredients. Fill MUFFIN tins 2/3 full and bake at 375 F. for 30
  minutes. Dick McMillen submitted to: Floating Kitchens, cooking with
  Seattle's Houseboaters. Typos by Brenda Adams &lt;adamsfmle@aol.com&gt;
  Posted to MC-Recipe Digest V1 #198
  
  Date: Wed, 14 Aug 1996 20:24:31 -0400
  
