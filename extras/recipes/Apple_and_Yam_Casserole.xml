<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple and Yam Casserole</title>
      <categories>
        <cat>Vegetables</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Apple pie filling</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Chopped nuts</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cans</unit></amt>
        <item>Whole sweet potatoes, drain</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Nutmeg</item></ing></ingredients>
    <directions>
      <step>     Combine apple pie filling and sweet potatoes in buttered casserole. Dot
  with 3 tbs. butter.  Sprinkle lightly with nutmeg. Bake in preheated 325 F.
  oven for 30 minutes, or untill bubbly.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
