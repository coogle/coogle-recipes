      <ing-div>
        <title>PATTI - VDRJ67A</title>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>ounces</unit></amt>
          <item>Cream Cheese; softened</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>small</unit></amt>
          <item>Cool Whip</item></ing>
