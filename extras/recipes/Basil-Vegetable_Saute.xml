<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Basil-Vegetable Saute</title>
      <categories>
        <cat>Vegetables</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Green Onions, Cut Into 3</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>In. pieces</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Zucchini, Cut Into 3 X 1/4</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>In. Strips</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Size Green Pepper, Cut</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Into 1/4 in. Strips</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Size Red Bell Pepper</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Cut Into 1/4 in. Strips.</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Vegetable Oil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>clove</unit></amt>
        <item>Garlic Minched</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Finely Chopped Fresh Basil</item></ing></ingredients>
    <directions>
      <step>  Coat A Large Skillet With Cooking Spray; Add Oil. Place Over Medium- High
  Heat Until Hot.  Add Garlic; Stir Fry 1 Min. Add Reserved Vegetables; Stir
  Fry 2 Min. Add Basil. Cover &amp; Reduce Heat. Cook 2 Min. Serve Immediately.
     (Fat 3.8. Chol. 0.)
 
</step></directions></recipe></recipeml>
