<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bbq Meatbaalls</title>
      <categories>
        <cat>Crs</cat>
        <cat>Totest</cat>
        <cat>Post</cat>
        <cat>Chopmeat</cat></categories>
      <yield>60</yield></head>
    <ingredients>
      <ing-div>
        <title>MEATBALLS</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>pound</unit></amt>
          <item>Lean ground beef</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Dry bread crumbs</item></ing>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>Minced onion</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Milk</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Egg, beaten</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Snipped fresh parsley</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Pepper</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Worcestershire sauce</item></ing></ing-div>
      <ing-div>
        <title>SAUCE</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Bottle chili sauce</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Jar (10oz) grape jelly</item></ing></ing-div></ingredients>
    <directions>
      <step>  Combine meatball ingredients. Brown meatballs in large skillet. Drain off
  fat and remove meatballs.
  
  Combine sauce ingredients in same skillet and heat, stirring until jelly
  melts. Return meatballs and heat through. Serve warm. Makes about 5 dozen.
  
  Taste of Home Ground Beef Collection 1996 Edition
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
