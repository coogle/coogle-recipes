<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bean and Beet Salad</title>
      <categories>
        <cat>Salads &amp; dr</cat>
        <cat>To post</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cans</unit></amt>
        <item>French Cut Green Beans</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cans</unit></amt>
        <item>Sliced Beets</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>medium</unit></amt>
        <item>Onions; cut in rings</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Vinegar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Celery Seed</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Salad Oil</item></ing></ingredients>
    <directions>
      <step>  Combine beans, beets and onion rings. Combine the next six ingredients and
  pour over the obove. Marinate over night.
  
  Source: Recipes Out of This World Women of St. Agnes Catholic Church
  Charleston, WV 1969
  
