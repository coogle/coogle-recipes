<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Appetizers - Ham Biscuits</title>
      <categories>
        <cat>Pork</cat>
        <cat>Ham</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>packages</unit></amt>
        <item>Pepperidge Farm Party Rolls</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Sliced in half</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Chopped Ham</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Stick Margarine -- softened</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>small</unit></amt>
        <item>Onion -- chopped</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Prepared Mustard</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Poppy Seeds</item></ing></ingredients>
    <directions>
      <step>  Mix margarine, onion, mustard and poppy seeds together. spread mixture on
  top side of rolls. Cover bottom halves with ham, assemble and bake 10
  minutes at 400 degrees
  
  Recipe By     : clays@mindspring.com (Clay Stovall)
  
  From: Ladies Home Journal- August 1991
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
