<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Crisp #01</title>
      <categories>
        <cat>Dessert</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>cups</unit></amt>
        <item>Sliced apples</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Nutmeg</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Sifted flour</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Powdered milk</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Stick butter</item></ing></ingredients>
    <directions>
      <step>  Mix 1/2 cup sugar with 1/2 teaspoon cinnamon, pour over sliced apples in
  1/2-quart casserole. Combine remaining ingredients and work together until
  crumbly. Put on apples and bake at 325 for 45 minutes.
  
  MRS J.C. BABER (STELLA)
  
  BARTON, AR
  
  From the book &lt;High Cotton Cookin'&gt;, Marvell Academy Mothers Assn, Marvell,
  AR  72366, ISBN 0-918544-14-9, downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
