  <recipe>
    <head>
      <title>Anadama Bread</title>
      <categories>
        <cat>Bread machi</cat>
        <cat>Breads</cat></categories>
      <yield>10</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>1 Lb Loaf:</item></ing>
      <ing>
        <amt>
          <qty>2 1/4</qty>
          <unit>cups</unit></amt>
        <item>Bread Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Dry milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Cornmeal; (1/2 Oz)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Molasses</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Olive oil</item></ing>
      <ing>
        <amt>
          <qty>7/8</qty>
          <unit>cups</unit></amt>
        <item>Water; (7 1/2 Fl.Oz)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Active Dry Yeast</item></ing>
      <ing>
        <amt>
          <qty>Timer</qty>
          <unit>OR</unit></amt>
        <item>Bake (Rapid) mode may be used.</item></ing></ingredients>
