<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Asparagus Casserole</title>
      <categories>
        <cat>American</cat>
        <cat>Casseroles</cat>
        <cat>Eggs</cat>
        <cat>Luncheon</cat>
        <cat>Vegetables</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Asparagus</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Mushroom soup</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Eggs; hard-boiled</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Bread crumbs; toasted</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sweet milk</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt and pepper; to taste</item></ing></ingredients>
    <directions>
      <step>  1. Grease caserole dish with butter and arrange in alternate layers:
  asparagus, mushroom soup, bread crumbs and sliced eggs.
  
  2. Pour over this: 1 cup of milk and dot top with butter. Bake in hot
  oven(moderately) for 40 minutes.
  
  Recipe by: Annette
  
  Posted to TNT - Prodigy's Recipe Exchange Newsletter  by Luke Murden
  &lt;lmurden@visi.net&gt; on Sep 16, 1997
 
</step></directions></recipe></recipeml>
