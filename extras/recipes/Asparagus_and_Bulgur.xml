<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Asparagus and Bulgur</title>
      <categories>
        <cat>Vegetarian</cat>
        <cat>Vegan</cat>
        <cat>Main dish</cat></categories>
      <yield>2</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>ounces</unit></amt>
        <item>Asparagus; cut into 1/2-inch pieces</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>ounces</unit></amt>
        <item>Bulgur; cooked</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Sunflower oil</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Thyme</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Basil</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing></ingredients>
    <directions>
      <step>   Steam asparagus for about 10 minutes. Combine all ingredients and mix
  well. Serve at room temperature.
  
