<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bailey's Irish Cream</title>
      <categories>
        <cat>Beverages</cat>
        <cat>Alcohol</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>15</qty>
          <unit>ounces</unit></amt>
        <item>Canned milk or "Eagle Brand"</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>pints</unit></amt>
        <item>Whipping cream</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Coconut extract</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Chocolate syrup</item></ing>
      <ing>
        <amt>
          <qty>13</qty>
          <unit>ounces</unit></amt>
        <item>Scotch Whiskey, (mickey)</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients in a blended for 1 minute. Let stand over night.
  Bottle the next day. No waiting time.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
