<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apricot Coconut Tartlets</title>
      <categories>
        <cat>Cookies</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Pack (3 oz) cream cheese</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Apricot jam</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Flaked coconut</item></ing></ingredients>
    <directions>
      <step>  Mix shortening, cheese, sugar &amp; salt. Blend in flour. Form into a
  ball &amp; chill at least 3 hours. Heat oven to 400. Roll out dough 1/8"
  thick on cloth covered board. Cut with 2" cookie cutter; place on
  ungreased sheet. Top each with 1/2 tsp. apricot jam. Bake about 10
  minutes. Remove from oven; quickly sprinkle with coconut. Bake 2 more
  minutes. Remove from sheet &amp; cool.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmkah001.zip
 
</step></directions></recipe></recipeml>
