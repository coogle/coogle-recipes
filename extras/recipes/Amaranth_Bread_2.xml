<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Amaranth Bread</title>
      <categories>
        <cat>Breads</cat></categories>
      <yield>10</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 2/3</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Amaranth flour</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>3 1/3</qty>
          <unit>cups</unit></amt>
        <item>Bread flour</item></ing>
      <ing>
        <amt>
          <qty>1 1/3</qty>
          <unit>tablespoons</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Nonfat dry milk</item></ing>
      <ing>
        <amt>
          <qty>1 1/3</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>2 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Yeast</item></ing></ingredients>
    <directions>
