<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Avocado and Shells Crab Salad</title>
      <categories>
        <cat>Salads</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>pounds</unit></amt>
        <item>Med shells</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Fresh broccoli flowerets</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Freshly sliced radishes</item></ing>
      <ing>
        <amt>
          <qty>12</qty>
          <unit>ounces</unit></amt>
        <item>Fresh or canned crab meat</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Avocados, peeled, pitted &amp;</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Diced</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Red bell peppers, thinly</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Sliced</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Yellow bell peppers, thinly</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Sliced</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Sliced green onion</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Chopped parsley</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Vegetable oil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Honey</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Fresh lemon juice</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Garlic powder</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Dijon mustard</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Seasoned salt</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing></ingredients>
    <directions>
      <step>  Prepare shells according to pack directions except adding broccoli
  last 4 minutes of cooking; drain. In large bowl, combine first 9
  ingredients. In small bowl, blend remaining ingredients; toss with
  salad. Serve immediately or chill.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmkah001.zip
 
</step></directions></recipe></recipeml>
