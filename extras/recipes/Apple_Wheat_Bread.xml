<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Wheat Bread</title>
      <categories>
        <cat>Bread</cat>
        <cat>Machine</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2 1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Yeast</item></ing>
      <ing>
        <amt>
          <qty>2 2/3</qty>
          <unit>cups</unit></amt>
        <item>Bread flour</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Whole wheat flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Nutmeg</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>tablespoons</unit></amt>
        <item>Powdered milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Butter or margarine</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Water; plus 3 TB</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Apple; tart,chopped, peeled</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Lemon juice</item></ing></ingredients>
    <directions>
      <step>  Place all ingredients in machine and push start! From THE ULTIMATE BREAD
  MACHINE COOKBOOK by Lacalamita Yield: 1 loaf Posted on NVN #134205 by
  Laguda on 06/12/94, MM format
  
  Recipe by: The Ultimate Bread Machine Cookbook
  
  Posted to recipelu-digest by Jill Proehl &lt;jpxtwo@swbell.net&gt; on Feb 24,
  1998
 
</step></directions></recipe></recipeml>
