<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbara Billingsley's "Sweet Potato Casserole"</title>
      <categories>
        <cat>Side dishes</cat>
        <cat>Bobbie - no</cat>
        <cat>Sweet potat</cat>
        <cat>Celebrity</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>pounds</unit></amt>
        <item>Cooked yams</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Butter; melted</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Eggs; slightly beaten</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Orange juice</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Brown sugar; packed</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Brandy</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Grated lemon peel</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing></ingredients>
    <directions>
      <step>  Beat cooked yams with electric mixer, adding melted butter - add remaining
  ingredients and beat until smooth - butter a 2 quart casserole and spoon in
  the mixture - bake at 350 degrees for 45 minutes or until set. MC
  formatting by bobbi744@sojourn.com
  
