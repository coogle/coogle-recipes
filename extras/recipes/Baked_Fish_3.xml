<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Fish</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Fish Filets</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Lemon</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Pepper (fresh ground is best)</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Basil</item></ing></ingredients>
    <directions>
      <step>  Place fish in a pan and salt, pepper, lightly put basil, and squeeze lemon,
  on top. Turn over and do again. Put in oven and bake on 300degrees for
  about 45 min. Posted to Bakery Shoppe Digest149 by Sandy Courrege
  &lt;sandy@aisp.net&gt; on Jul 07, 1997
 
</step></directions></recipe></recipeml>
