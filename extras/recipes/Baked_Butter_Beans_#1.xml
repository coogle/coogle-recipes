<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Butter Beans #1</title>
      <categories>
        <cat>Butter</cat>
        <cat>Beans</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>White Lima Beans 40 Oz; Butter Beans</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit>tablespoons</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>large</unit></amt>
        <item>Can tomato sauce</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>large</unit></amt>
        <item>Onion; cut fine</item></ing>
      <ing>
        <amt>
          <qty>8</qty>
          <unit>slices</unit></amt>
        <item>Bacon; chopped</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt and pepper to taste</item></ing></ingredients>
    <directions>
      <step>  Mix all ingredients. Put in casserole and bake in slow oven (325 degrees)
  for 1 to 1 1/2 hours.
  Posted to recipelu-digest by "Diane Geary" &lt;diane@keyway.net&gt; on Feb 27,
  1998
 
</step></directions></recipe></recipeml>
