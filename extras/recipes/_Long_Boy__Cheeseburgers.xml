<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>"Long Boy" Cheeseburgers</title>
      <categories>
        <cat>Crs</cat>
        <cat>Totest</cat>
        <cat>Post</cat>
        <cat>Chopmeat</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Xtra lean ground beef</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Worcestershire sauce</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Catsup</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Chopped onion</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Crushed cornflakes</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Whipping cream</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Loaves brown n serve French</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Bread, cut in half</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Lengthwise</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>(4oz) shredded cheddar</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Cheese</item></ing></ingredients>
    <directions>
      <step>  Preheat oven to 350F.
  
  Combine meat, seasonings, cornflakes and cream. Spread to edges of bread
  and bake about 25 minutes until meat is cooked. Sprinkle with cheese during
  last 5 minutes of cooking. Cut into slices.
  
  Taste of Home Ground Beef Collection 1996 Edition
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
