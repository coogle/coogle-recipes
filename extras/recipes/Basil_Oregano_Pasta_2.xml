    <ingredients>
      <ing-div>
        <title>HAND</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Durum semolina</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
