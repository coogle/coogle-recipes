<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>4-Hour Beef Stew</title>
      <categories>
        <cat>Main dish</cat>
        <cat>Meats</cat>
        <cat>Cajun</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>pounds</unit></amt>
        <item>Cubed stew beef</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Onions, quartered</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Tomato sauce (small)</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit/></amt>
        <item>Carrots, cut in chunks</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Tapioca</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Potatoes</item></ing></ingredients>
    <directions>
      <step>     Mix all ingredients, except potatoes , and place in casserole. Cover top
  with raw potatoes.  Cover and bake at 250 F. for 4 hrs.
  
  C. Younker--Shreveport, La.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
