<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bacon Artichoke Dip</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>8 1/2</qty>
          <unit>ounces</unit></amt>
        <item>Artichoke hearts; drained &amp; chopped</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Green onions; chopped</item></ing>
      <ing>
        <amt>
          <qty>8</qty>
          <unit>ounces</unit></amt>
        <item>Sour cream</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Grated parmesan cheese</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Miracle Whip</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Grated mozzarella</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Chopped spinach; drained</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Grated sharp cheddar</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>packages</unit></amt>
        <item>Bacon; fried and crumbled</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients and place in greased 9x9 pan. Bake at 375 degrees
  for 30 minutes or until bubbly. Serve warm with flour tortilla chips. Great
  with pita bread pieces!
  
  Posted to TNT - Prodigy's Recipe Exchange Newsletter  by
  slea@hightowerservices.com (Shawn Zehnder Lea) on Nov 24, 1997
 
</step></directions></recipe></recipeml>
