<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Rice #3</title>
      <categories>
        <cat>Rice</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Cooked rice</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Green onions; chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Egg whites; slightly beaten</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Fat free cheese; shredded</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Canned corn; drained</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Lemon juice</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt &amp; black pepper to taste</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>dash</unit></amt>
        <item>Garlic powder</item></ing></ingredients>
    <directions>
      <step>  Preheat oven to 350 degrees.  Combine all ingredients in large bowl. Spray
  round or square baking dish with non-stick cooking spray. Pour mixture in
  dish &amp; bake at 350 deg for 25-35 minutes.  Entire recipe makes 4 servings.
  
  FAT FREE
  
  FROM SOME COOKBOOK!
  
  From a collection of my mother's (Judy Hosey) recipe box which contained
  lots of her favorite recipes, clippings, etc.  Downloaded from Glen's MM
  Recipe Archive, http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
