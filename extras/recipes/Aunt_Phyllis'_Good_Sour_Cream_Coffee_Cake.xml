<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Aunt Phyllis' Good Sour Cream Coffee Cake</title>
      <categories>
        <cat>Canada</cat></categories>
      <yield>18</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Sour cream</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Baking soda</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Butter or margarine</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit/></amt>
          <item>Eggs</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Vanilla</item></ing>
        <ing>
          <amt>
            <qty>1 3/4</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Baking powder</item></ing></ing-div>
      <ing-div>
        <title>TOPPING</title>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Brown sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Cinnamon</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Nuts</item></ing></ing-div></ingredients>
    <directions>
      <step>  Mix soda with cream. Add rest of ingredients except topping. Put batter in
  2 bread pans. Mix topping ingredients and sprinkle on top.
  
  Bake a 350F for 1/2 hour.
  
  Recipe by: Aunt Phyllis
  
  Posted to recipelu-digest by Nesb2 &lt;Nesb2@aol.com&gt; on Mar 2, 1998
 
</step></directions></recipe></recipeml>
