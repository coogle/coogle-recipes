<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Absolutely Apricot Bread</title>
      <categories>
        <cat>Bread</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing-div>
        <title>FOR 1-LB LOAF</title>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Yeast</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Dry milk</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Butter</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Apricot jam</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Dried apricots; chopped</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>Water</item></ing></ing-div>
      <ing-div>
        <title>FOR 1-1/2 LB LOAF</title>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>teaspoons</unit></amt>
          <item>Yeast</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Dry milk</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Butter</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Apricot jam</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>Dried apricots; chopped</item></ing>
        <ing>
          <amt>
            <qty>1 1/4</qty>
            <unit>cups</unit></amt>
          <item>Water</item></ing></ing-div></ingredients>
    <directions>
      <step>  For all recipes, load ingredients into breadmachine pan according to
  manufacturer's directions. Bake on white bread setting unless otherwise
  indicated. "Yeast" refers to active dry yeast.  Use light crust setting.
  Delayed timer mode okay.  From Electric Bread.
  
