<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apricot Fruit Flip</title>
      <categories>
        <cat>Drinks</cat>
        <cat>Beverages</cat>
        <cat>Desserts</cat>
        <cat>Fatfree</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Apricot nectar (12 oz)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Ripe banana</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Instant nonfat dry milk powder</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Lemon juice</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Honey</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Grated lemon rind</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Ice cubes</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients, except ice cubes, in container of an electric
  blender. Add enough ice cubes to measure 4 cups. Blend until smooth.
  
  From Tuscon area newspapers, 1994, 3rd quarter, courtest Mike Orchekowski.
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/tn-94q3.zip
 
</step></directions></recipe></recipeml>
