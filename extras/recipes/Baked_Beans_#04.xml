<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Beans #04</title>
      <categories>
        <cat>Vegetable</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cans</unit></amt>
        <item>(16-oz) pork &amp; beans</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chopped onion</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chopped green pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Black pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Mustard</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Catsup</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Molasses</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Light corn syrup</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>slices</unit></amt>
        <item>Bacon</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients except bacon. Pour into 2-quart baking dish; lay
  bacon across the top. Bake at 425 for 15 minutes, reduce oven to 350 &amp; bake
  20-30 minutes longer.
  
  MRS JAMES E (JEAN) RECORD
  
