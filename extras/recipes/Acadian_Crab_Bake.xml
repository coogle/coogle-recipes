<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Acadian Crab Bake</title>
      <categories>
        <cat>Seafood</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>package</unit></amt>
        <item>(8-oz) cream cheese</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Sour cream</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Green onions w/tops; chopped</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Cooked rice</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(8-oz) crabmeat; flaked</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>medium</unit></amt>
        <item>Tomatoes; peeled, slice</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt to taste</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Buttered bread crumbs</item></ing></ingredients>
    <directions>
      <step>    Combine cream cheese, sour cream and milk; whip until smooth. Fold in
  green onions, rice and crabmeat. Turn mixture into a buttered 2 quart
  casserole. Arrange tomato slices over casserole mixture. Sprinkle with
  salt; top with bread crumbs. Bake in a preheated 350 degree oven for 30
  minutes.
  
    Serves 6.
  
  From &lt;A Taste of Louisiana&gt;.  Downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
