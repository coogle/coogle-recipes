        <item>BEEF;OVEN ROAST FZ</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>pounds</unit></amt>
        <item>CARROTS FRESH</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
