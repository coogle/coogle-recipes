<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Potato Bread</title>
      <categories>
        <cat>Electric</cat>
        <cat>Breadmaker</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Bread Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Nonfat Dried Milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Sour cream</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Instant potato flakes</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Bacon bits</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Chives</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Yeast (active dry)</item></ing></ingredients>
    <directions>
      <step>  *** This dough will be very dry. Resist the urge to
  add water. 1 POUND LOAF Marie Frainier *DGCP02C*
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
