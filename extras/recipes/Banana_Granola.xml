<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana Granola</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>REG LOAF</title>
        <ing>
          <amt>
            <qty>2/3</qty>
            <unit>cups</unit></amt>
          <item>WATER</item></ing>
        <ing>
          <amt>
            <qty>1 1/4</qty>
            <unit>cups</unit></amt>
          <item>WHITE BREAD FLOUR</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>WHEAT BREAD FLOUR</item></ing>
        <ing>
          <amt>
            <qty>4</qty>
            <unit>teaspoons</unit></amt>
          <item>DRY MILK</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>SALT</item></ing>
        <ing>
          <amt>
            <qty>4</qty>
            <unit>teaspoons</unit></amt>
          <item>BUTTER</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>MASHED BANANAS</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>GRANOLA</item></ing>
        <ing>
          <amt>
            <qty>4</qty>
            <unit>teaspoons</unit></amt>
          <item>MOLASSES</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>CHOPPED WALNUTS</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>DRIED BANANA CHIPS</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>FASTRISE YEAST -or- 1 1/2 tsp. ACTIVE DRY YEAST</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit>large</unit></amt>
          <item>LOAF:</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>WATER</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>WHITE BREAD FLOUR</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>WHEAT BREAD FLOUR</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>DRY MILK</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>SALT</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>BUTTER</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>MASHED BANANAS</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>GRANOLA</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>MOLASSES</item></ing>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>CHOPPED WALNUTS</item></ing>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>DRIED BANANA CHIPS</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>FASTRISE YEAST -or- 3 tsp. ACTIVE DRY YEAST</item></ing></ing-div></ingredients>
    <directions>
      <step>  Mash bananas well and measure after mashing. Use your favorite granola mix.
  This is a nice moist bread, not as sweet as the ingredients may suggest.
  
  Posted to TNT Recipes Digest by rdpatrick@juno.com (Robert D. Patrick) on
  Feb 19, 1998
 
</step></directions></recipe></recipeml>
