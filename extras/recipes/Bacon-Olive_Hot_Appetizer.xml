<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bacon-Olive Hot Appetizer</title>
      <categories>
        <cat>Appetizer</cat></categories>
      <yield>16</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>slices</unit></amt>
        <item>Bacon (fry; drain,crumble)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Ripe olives (chopped)</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Green onion tops (chopped)</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Sharp Chedar cheese (shred)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Curry</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Mayonnaise</item></ing></ingredients>
    <directions>
      <step>  Date: Wed, 1 May 1996 00:25:05 -0400
  
  From: BobbieB1@aol.com
  
  Mix all ingredients.  Toast bread on one side and cut in assorted shapes.
  Spread mixture on untoasted side. Broil until brown or freeze until ready
  to use.
  
