<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Bread</title>
      <categories>
        <cat>Bread</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing-div>
        <title>LIST A</title>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>Oil</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit/></amt>
          <item>Eggs</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Vanilla</item></ing></ing-div>
      <ing-div>
        <title>LIST B</title>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Baking soda</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Cinnamon</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt (optional--I don't add it)</item></ing></ing-div>
      <ing-div>
        <title>LIST C</title>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Diced apples</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Chopped pecans</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Raisins</item></ing>
        <ing>
          <amt>
            <qty>From:</qty>
            <unit/></amt>
          <item>Melisa Wells &lt;MelisaLW@AOL.COM&gt;</item></ing>
        <ing>
          <amt>
            <qty>Date:</qty>
            <unit/></amt>
          <item>Tue, 30 Jul 1996 15:46:48 -0400</item></ing></ing-div></ingredients>
    <directions>
      <step>  In large bowl, mix List A. In seperate bowl, mix List B well, then add to
  A. When A and B are combined well, add List C. Grease and flour either 2
  loaf pans or a tube pan.
  
  Loaf pans: Bake at 325 degrees for 1 1/2 hours
  
  Tube pan: Bake at 350 degrees for 2 hours
  
  EAT-L Digest 29 July 96
  
  From the EAT-L recipe list.  Downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
