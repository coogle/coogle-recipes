<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Beef Elegante over Rice</title>
      <categories>
        <cat>Meats</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>pounds</unit></amt>
        <item>Round steak (thinly sliced)</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Seasoned flour</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Corn oil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Chopped onions</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Dry red wine</item></ing>
      <ing>
        <amt>
          <qty>1 3/4</qty>
          <unit>cups</unit></amt>
        <item>Beef broth</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(4oz) sliced mushrooms</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>W/liquid</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Hot cooked rice</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Tomato wedges (for garnish)</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Fresh parsley (for garnish)</item></ing></ingredients>
    <directions>
      <step>  Tenderize steak; cut in 1" strips. Coat steak with flour; brown in
  oil. Add onions &amp; cook 2-3 minutes longer. Stir in wine, broth &amp;
  mushrooms with liquid; bring to boil. Cover, reduce heat, &amp; simmer
  20-30 minutes or until tender. Serve over warm rice; garnish with
  tomato &amp; parsley. NOTE: Beef broth may be substituted for wine.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmkah001.zip
 
</step></directions></recipe></recipeml>
