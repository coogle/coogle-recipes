<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Cake (Flourless)</title>
      <categories>
        <cat>Apples</cat>
        <cat>Cakes</cat>
        <cat>Fruit</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Grated apples</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit/></amt>
        <item>Egg yolks</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit>teaspoons</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>8</qty>
          <unit>ounces</unit></amt>
        <item>Grated nuts</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Juice of one lemon</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Raisins</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Full  tsp.</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Dry yeast</item></ing></ingredients>
    <directions>
      <step>  Mix all ingredients.  Beat egg whites to stiff peaks and add them carefully
  to the mixture.  Bake as any cake in a floured, oiled pan.
  
  Recipe By     : suzana@amhost.amcham.com.br
  
  From: Ladies Home Journal- August 1991
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
