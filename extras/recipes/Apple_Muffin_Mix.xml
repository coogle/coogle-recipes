<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Muffin Mix</title>
      <categories>
        <cat>Mixes</cat>
        <cat>Muffins</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Self-rising flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Nutmeg</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Chopped dried apple</item></ing></ingredients>
    <directions>
      <step>  In a large mixing bowl, combine all the ingredients.  Place the mix
  into an airtight container.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmkah001.zip
 
</step></directions></recipe></recipeml>
