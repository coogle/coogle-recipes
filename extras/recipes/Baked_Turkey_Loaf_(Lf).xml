<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Turkey Loaf (Lf)</title>
      <categories>
        <cat>Poultry</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>pounds</unit></amt>
        <item>Ground turkey</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>medium</unit></amt>
        <item>Onion -- finely chopped</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Celery stalk -- finely</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Chopped</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Fresh parsley -- minced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Bell pepper -- finely</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Chopped</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Whole-wheat bread crumbs --</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Toasted</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Skim milk -- or chicken</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Stock</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Egg white -- slightly</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Beaten</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Black pepper -- freshly</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Ground</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Dried oregano -- crushed</item></ing>
      <ing>
        <amt>
          <qty>10</qty>
          <unit>mililiters</unit></amt>
        <item>Garlic -- crushed</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>PER SERVING-----</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>*cals</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>*mg chol</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>*gm dietary fiber</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>*gm fat</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>*mg sodium</item></ing></ingredients>
    <directions>
      <step>  Mix all ingredients until just blended.  Do not overmix. Press the mixture
  into a 4 x 9-inch nonstick loaf pan. Bake at 325F for 45 minutes to 1 hour.
  Serve hot with Mushroom Glaze (see recipe same name) or serve cold. Makes 6
  servings.
  
  Recipe By     :
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
