<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbecue Chicken</title>
      <categories>
        <cat>Poultry</cat></categories>
      <yield>20</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>pounds</unit></amt>
        <item>Skinless boneless chicken breast</item></ing>
      <ing>
        <amt>
          <qty>30</qty>
          <unit>ounces</unit></amt>
        <item>Tomatoes; chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Onions; chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Water; *</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Chili powder</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt; to taste</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Black pepper; fresh ground</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Catsup</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Cider vinegar</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Worcestershire sauce</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Garlic cloves; minced</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>teaspoons</unit></amt>
        <item>Hot pepper sauce</item></ing></ingredients>
    <directions>
      <step>  *add water if it seems to dry
     Combine all ingredients in a crockpot.  Cover and cook on low heat for 5
  to 6 hours or on high 2 to 3 hours. Watch carefully the last hour to make
  sure that it is not sticking.  Shred chicken with forks and mix Mixture
  should be very thick.
  
  Serve on hamburger buns with mustard.
 
</step></directions></recipe></recipeml>
