<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Airport Slaw</title>
      <categories>
        <cat>Salad</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Head white cabbage</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Head red cabbage</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>large</unit></amt>
        <item>Purple onions; sliced into rings</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Stalk celery; diced coarsley</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Green apple; unpeeled, diced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Red apple; unpeeled, diced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Head cauliflower; separate into flowerets &amp; dice</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>large</unit></amt>
        <item>Bell peppers; sliced into rings</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Limes; juice of (or lemons)</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Bottles cole slaw dressing</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Celery seed</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt &amp; pepper to taste</item></ing></ingredients>
    <directions>
      <step>  Chop cabbages coarsely. Mix all ingredients in large pan &amp; store in
  refrigerator 4-24 hours (the longer, the better). Stir bottom to top
  several times during chilling.
  
