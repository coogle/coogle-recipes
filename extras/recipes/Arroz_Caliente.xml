<recipeml version="0.5">
  <recipe>
    <head>
      <title>Arroz Caliente</title>
      <categories>
        <cat>Beef</cat>
        <cat>Rice</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Hamburger</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Onion -- chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Garlic Powder</item></ing>
      <ing>
        <amt>
          <qty>12</qty>
          <unit>ounces</unit></amt>
        <item>Salsa</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Rice, Cooked</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Nonfat Sour Cream</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Nonfat Monterey Jack Cheese</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Grated</item></ing></ingredients>
    <directions>
      <step>  Brown meat and onions in seasonings. Drain fat.
  
  Mix remaining ingredients and top with cheese.
  
  Microwave 5 minutes on medium high.
  
