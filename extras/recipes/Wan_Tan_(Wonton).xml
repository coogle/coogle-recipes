<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Wan Tan (Wonton)</title>
      <categories>
        <cat>Appetizers</cat>
        <cat>Chinese</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>FILLING</title>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>pounds</unit></amt>
          <item>Ground pork</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>pound</unit></amt>
          <item>Spinach; finely chopped</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Soy sauce</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Ginger; finely chopped</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing></ing-div>
      <ing-div>
        <title>WRAPPERS</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>package</unit></amt>
          <item>Wonton wrappers</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Egg</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Peanut oil for deep frying</item></ing></ing-div></ingredients>
    <directions>
      <step>  1. Mix ground pork, spinach, soy sauce, ginger and salt in a bowl.
  
  2. Place 1/2 tbsp on wrapper and fold, seal with egg.
  
  3. Deep fry at 375F and serve with sweet and sour sauce, or use in wonton
  soup.
  
  Notes: Makes: 24 Wontons
  
