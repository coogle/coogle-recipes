<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Basic Bread</title>
      <categories>
        <cat>Bread machi</cat>
        <cat>Bread maili</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Yeast</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>All-purpose flour</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>teaspoons</unit></amt>
        <item>Gluten</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Dry milk</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Water</item></ing></ingredients>
    <directions>
      <step>  Toss yeast in bread machine first; cover w/ flour and add other dry
  ingredients; take butter from fridge, crumple and add; use room temperature
  water.
  
  &gt;From: TrinaMaria@aol.com
  From: Bread-Bakers Archives: ftp.best.com/pub/reggie/archives/bread/recipe
 
</step></directions></recipe></recipeml>
