<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbequed Beef Ribs</title>
      <categories>
        <cat>Bar-b-q</cat>
        <cat>Beef</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>pounds</unit></amt>
        <item>Beef short ribs</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chopped onions</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Margarine</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>teaspoons</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Dry mustard</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Paprika</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>teaspoons</unit></amt>
        <item>Worchestershire sauce</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Catsup</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>White vinegar</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing></ingredients>
    <directions>
      <step>  Contributed to the echo by: BOYD NARON BARBEQUED BEEF RIBS Brown ribs.
  Saute onions in margarine and add remaining ingredients. Simmer 30 minutes.
  Baste ribs with sauce and bake at 350 degrees for 2 hours. Drain off fat,
  baste again and bake at 300 degrees for 1 hour.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
