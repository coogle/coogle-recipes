<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbecued Duck #3</title>
      <categories>
        <cat>Game</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Ducks; well washed</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt to taste</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Pepper to taste</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Bottle (14-oz) ketchup</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Worcestershire sauce</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Vinegar</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Cayenne pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Stick butter</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Hot water</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Cloves garlic</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>small</unit></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(4-oz) mushrooms; chopped and drained; or fresh mushrooms, chopped</item></ing></ingredients>
    <directions>
      <step>  Place ducks breast down in an electric skillet. Mix remaining ingredients,
  pour over ducks, cook on very low heat, covered, for 4 hours or until meat
  is very tender. Add water if needed. The last hour of cooking add chopped
  onion and mushrooms. This makes a good gravy. Yield: 6 servings.
  
  KATHY COBB (MRS. THOMAS)
  
  From &lt;Traditions: A Taste of the Good Life&gt;, by the Little Rock (AR) Junior
  League.  Downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
