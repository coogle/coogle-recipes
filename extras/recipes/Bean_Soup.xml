<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bean Soup</title>
      <categories>
        <cat>Soups &amp; ste</cat>
        <cat>To post</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Navy Beans</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Ham Pieces</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>small</unit></amt>
        <item>Onions; chopped</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit/></amt>
        <item>Carrot; diced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Stalks Celery; diced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Potato; diced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Celery Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>dash</unit></amt>
        <item>Garlic Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Celery Seed</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Parsley Flakes</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Tomato Juice</item></ing></ingredients>
    <directions>
      <step>  Wash beans. Cover with water and bring to a boil. Add ham, vegetables, and
  seasonings. When mixture begins to thicken, add tomato juice. Cook until
  thick and well done, at least 3 hours. Use low heat. Add water, if
  necessary.
  
  Serves 8
  
  Source: "Mountain Measures" -- Junior League of Charleston, WV ed. 1974
  
