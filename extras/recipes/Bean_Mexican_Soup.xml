<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bean Mexican Soup</title>
      <categories>
        <cat>Mexican</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>pounds</unit></amt>
        <item>Turkey -- ground/cooked</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Onions -- chopped</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Bell peppers -- chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>package</unit></amt>
        <item>Taco seasoning mix</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>8 oz jar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>5 1/2 oz can  red beans --</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Drained/rinsed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>6 oz can</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Drained/rinsed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>4 oz can</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Taco sauce</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Kidney beans --</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Tomatoes -- crushed/undraine</item></ing></ingredients>
    <directions>
      <step>  In a skillet, cook turkey, onions, and peppers until vegetables are tender
  and meat is no longer pink. Add taco seasoning mix, taco sauce, beans, and
  tomatoes. Simmer for 30 minutes.
  
  Recipe By     : Anita A. Matejka
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
