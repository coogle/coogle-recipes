<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Aphrodisiac Bread</title>
      <categories>
        <cat>Breads</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Dry yeast</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Warm water</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Unbleached white flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Curry powder</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>pinches</unit></amt>
        <item>Parsley</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Garlic minced</item></ing></ingredients>
    <directions>
      <step>  In a large bowl, sprinkle yeast into water &amp; dissolve. Add the rest of the
  ingredients in order &amp; beat well. On a floured surface, knead dough lightly
  for 5 minutes. Form into a round bread &amp; set aside until its has doubled.
  Preheat oven to 350F &amp; bake for 45 minutes till light brown.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
