<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Chicken and Rice</title>
      <categories>
        <cat>Kaz</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>cups</unit></amt>
        <item>Chicken bouillon cubes</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit/></amt>
        <item>Cube</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Rice -- uncooked</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>small</unit></amt>
        <item>Onion -- chopped</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit/></amt>
        <item>Green</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Celery -- chopped</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Piece chicken</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Butter or margarine</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Pepper -- chopped</item></ing></ingredients>
    <directions>
      <step>  Bring water to boil in Dutch oven and then add bouillon cubes. Mix the
  other ingredients, except chicken, with bouillon and water.
   Season the chicken with seasoning salt and lay pieces on top of the rice
  mixture. Cook 1 1/2 hours at 400 degrees.
  
  Recipe By     :
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
