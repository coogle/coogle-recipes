<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Peanut Crumble</title>
      <categories>
        <cat>Pastry</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>5</qty>
          <unit/></amt>
        <item>Cooking apples -- peeled and</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Sliced</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Brown sugar -- packed</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Quick-cooking rolled oats</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Nutmeg</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Butter -- softened</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Peanut butter</item></ing></ingredients>
    <directions>
      <step>  Place apple slices in slow-cooking pot.  In medium bowl, combine sugar,
  flour, oats, cinnamon, and nutmeg.  Mix in butter and peanut butter with
  pastry blender or fork.  Sprinkle over apples.  Cover pot and cook on low
  for 5 to 6 hours.  Serve warm; plain, with ice cream or whipped cream.
  
  Recipe By     :
  
  From: Ladies Home Journal- August 1991
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
