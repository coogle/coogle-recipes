<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana Split Cake #5</title>
      <categories>
        <cat>Dessert</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Graham cracker crumbs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Box (16-oz) powdered sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(16-oz) crushed pineapple (drained)</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit/></amt>
        <item>Bananas</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Strawberries (optional)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>container</unit></amt>
        <item>(large) cool whip</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Nuts to garnish</item></ing>
      <ing>
        <amt>
          <qty>From:</qty>
          <unit/></amt>
        <item>Tina Rogers &lt;CAJUNT@UGA.CC.UGA.EDU&gt;</item></ing>
      <ing>
        <amt>
          <qty>Date:</qty>
          <unit/></amt>
        <item>Mon, 29 Jul 1996 19:33:53 EDT</item></ing></ingredients>
    <directions>
      <step>  Melt 1/2 c. butter &amp; blend with cracker crumbs; press into 13x9 pan.  Mix 1
  cup butter with powdered sugar &amp; 2 eggs; beat 15 minutes &amp; spread on top of
  crust.  Next, slice bananas on top of filling, spread pineapple &amp;
  strawberries over bananas. Top with cool whip &amp; nuts. Refrigerate several
  hours.
  
  EAT-L Digest 29 July 96
  
  From the EAT-L recipe list.  Downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
