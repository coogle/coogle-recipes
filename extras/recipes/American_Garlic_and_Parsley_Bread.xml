<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>American Garlic and Parsley Bread</title>
      <categories>
        <cat>Bread</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Active dry yeast</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Bread flour</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Wheat germ</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Wheat bran</item></ing>
      <ing>
        <amt>
          <qty>1 3/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Vegetable oil</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Cloves garlic; minced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Parsley; fresh chopped</item></ing>
      <ing>
        <amt>
          <qty>1 1/4</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing></ingredients>
    <directions>
      <step>  From: gemini@synapse.net (Georgette &amp; Dave Burnside)
  
  Date: Wed, 14 Feb 1996 05:09:54 GMT
  
  Add all ingredients in the order suggested by your bread machine manual and
  process on the basic bread cycle according to the manufacturer's
  directions. Let the loaf cool before slicing. Serve plain or toasted. Makes
    1-1/2    pound loaf.
  
