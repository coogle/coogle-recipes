<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Chunk Bread (1 1/2 Lb)</title>
      <categories>
        <cat>Bread maker</cat>
        <cat>Sweet bread</cat></categories>
      <yield>16</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Milk</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Vegetable oil</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Ground cinnamon</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>2 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Yeast</item></ing></ing-div>
      <ing-div>
        <title>AT BEEP ADD</title>
        <ing>
          <amt>
            <qty>1 1/3</qty>
            <unit>medium</unit></amt>
          <item>Apple; peeled and diced</item></ing></ing-div></ingredients>
    <directions>
      <step>  NOTES : Use raisin setting &amp; add apple at pause.
  
  Recipe by: Donna German 'The Bread Machine Cookbook
  
  Posted to recipelu-digest by "D. MacLean" &lt;dmaclean@bfree.on.ca&gt; on Mar 25,
  1998
 
</step></directions></recipe></recipeml>
