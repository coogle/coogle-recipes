<recipeml version="0.5">
  <recipe>
    <head>
      <title>Arroz Con Pollo (Stewed Chicken and Rice)</title>
      <categories>
        <cat>Poultry</cat>
        <cat>Mexico</cat>
        <cat>Archived</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty/>
            <unit>x</unit></amt>
          <item>Vegetable cooking spray;</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Onion; chopped</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Celery; chopped</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cloves</unit></amt>
          <item>Garlic; finely chopped</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>pound</unit></amt>
          <item>Chicken thighs; skinless</item></ing>
        <ing>
          <amt>
            <qty>16</qty>
            <unit>ounces</unit></amt>
          <item>Tomatoes, diced; undrained</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Rice, long-grain; uncooked</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Bouillon, chicken; instant =or=- 2 cubes</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Bay leaf;</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Pepper, ground;</item></ing>
        <ing>
          <amt>
            <qty>10</qty>
            <unit>ounces</unit></amt>
          <item>Green peas, frozen; optl</item></ing></ing-div>
      <ing-div>
        <title>PER SERVING</title>
        <ing>
          <amt>
            <qty>270</qty>
            <unit>x</unit></amt>
          <item>*cals</item></ing>
        <ing>
          <amt>
            <qty>21</qty>
            <unit>x</unit></amt>
          <item>*gm protein</item></ing>
        <ing>
          <amt>
            <qty>38</qty>
            <unit>x</unit></amt>
          <item>*gm carbo</item></ing>
        <ing>
          <amt>
            <qty>4</qty>
            <unit>x</unit></amt>
          <item>*gm fat</item></ing>
        <ing>
          <amt>
            <qty>13</qty>
            <unit>x</unit></amt>
          <item>*% cals from fat</item></ing>
        <ing>
          <amt>
            <qty>63</qty>
            <unit>x</unit></amt>
          <item>*mg chol</item></ing>
        <ing>
          <amt>
            <qty>553</qty>
            <unit>x</unit></amt>
          <item>*mg sodium</item></ing></ing-div></ingredients>
    <directions>
      <step>  Spray nonstick Dutch oven with vegetable cooking spray and place over
  medium heat.  Add onion, celery and garlic; cook 3 to 5 minutes, or until
  tender, stirring often. Add remaining ingredients except peas; bring to a
  boil.  Reduce heat to low; cover and simmer 30 minutes. Add peas, cover and
  simmer 10 minutes longer. To serve, remove bay leaf. Makes 6 servings. Pre
  Time: about 10 minutes. Cooking Time: about 45 minutes.
  
  S: Woman's Day Light Meals in Minutes Vol 3 No. 2 Formatted for MM:dianeE
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
