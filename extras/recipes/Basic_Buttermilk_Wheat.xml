<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Basic Buttermilk Wheat</title>
      <categories>
        <cat>Dg</cat>
        <cat>Whole wheat</cat>
        <cat>Breadmaker</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/8</qty>
          <unit>cups</unit></amt>
        <item>Buttermilk</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Apple Juice,Bottled Or Can</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Brown Sugar -- or Maple</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking Soda</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Gluten</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Whole-Wheat Flour</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Yeast</item></ing></ingredients>
    <directions>
      <step>  D. German IV Page 32 - Medium
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
