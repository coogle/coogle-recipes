<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Date Torte</title>
      <categories>
        <cat>Apples</cat>
        <cat>Desserts</cat>
        <cat>Fruit</cat>
        <cat>Part of spe</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>cups</unit></amt>
        <item>Apples; unpared/diced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking powder</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Egg</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Butter; melted</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Nuts</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Dates; cut up</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Whipped cream; whipped topping, OR ice cream</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients except whipped cream in order given. Stir but do
  not beat. Pour into greased 8" square pan. BAke at 400 F. for 40 minutes.
  Serve with whipped cream, topping or ice cream.
  
  Recipe by: R. Banghart Posted to Bakery-Shoppe Digest V1 #207 by Roberta
  Banghart &lt;bobbi744@sojourn.com&gt; on Aug 29, 1997
 
</step></directions></recipe></recipeml>
