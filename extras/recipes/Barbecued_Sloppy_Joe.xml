    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Ground meat</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>large</unit></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
