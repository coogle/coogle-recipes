<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baby Food Cake</title>
      <categories>
        <cat>Cakes</cat>
        <cat>Desserts</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>3</qty>
            <unit/></amt>
          <item>Eggs</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1 1/4</qty>
            <unit>cups</unit></amt>
          <item>Salad oil</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Jar applesauce baby food</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Jar apricot baby food</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Jar carrot baby food</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Cinnamon</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Baking soda</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Chopped pecans</item></ing></ing-div>
      <ing-div>
        <title>FROSTING</title>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Margarine</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>package</unit></amt>
          <item>(8 oz.) cream cheese</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Box (16 oz.) powdered sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Vanilla</item></ing></ing-div></ingredients>
    <directions>
      <step>  Combine eggs, sugar and salad oil. Add baby food. Sift flour, cinnamon and
  baking soda and add to baby food mixture. Mix in pecans. Bake in 9x13x2
  inch baking pan at 350 for 35 minutes. For frosting, combine margarine,
  cream cheese, powdered sugar and vanilla. Frost cake when cool. MasterCook
  formatted by Ethel Snyder.
  
  Recipe by: CAPPER'S JUNE 3, 1997 Posted to MC-Recipe Digest V1 #637 by
  essie49@juno.com (Ethel R Snyder) on Jun 07, 1997
 
</step></directions></recipe></recipeml>
