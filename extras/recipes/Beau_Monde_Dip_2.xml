<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Beau Monde Dip</title>
      <categories>
        <cat>Dips</cat></categories>
      <yield>10</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Sour Cream</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Mayonnaise</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Parsley Flakes</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Dehydrated Onions</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Beau Monde Seasoning</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Dill Weed; Fresh, diced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>packages</unit></amt>
        <item>Corned Beef - Hermetically Sealed, chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Loaf Dark Rye Bread</item></ing></ingredients>
    <directions>
      <step>  Mix all ingredients (except bread) chill for 1 hour.
  
  Hollow bread &amp; cube the middle to serve w/dip
  
  Pour dip into bread shell &amp; serve on platter.
  
  NOTES : Beau monde spice can be found any where that sells "Spice Island
  Products"
  Recipe by: Mary Ryznick
  
  Posted to MC-Recipe Digest V1 #887 by LMeyers325@aol.com on Nov 6, 1997
 
</step></directions></recipe></recipeml>
