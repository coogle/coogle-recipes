<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Amaranth Bread</title>
      <categories>
        <cat>Bread</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing-div>
        <title>FOR 1 LB LOAF</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Yeast</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Nonfat dry milk</item></ing>
        <ing>
          <amt>
            <qty>1 2/3</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>Amaranth flour</item></ing>
        <ing>
          <amt>
            <qty>2/3</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>tablespoons</unit></amt>
          <item>Honey</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Margarine</item></ing>
        <ing>
          <amt>
            <qty>7/8</qty>
            <unit>cups</unit></amt>
          <item>Water</item></ing></ing-div>
      <ing-div>
        <title>FOR 1-1/2 LB LOAF</title>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Yeast</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Nonfat dry milk</item></ing>
        <ing>
          <amt>
            <qty>2 1/2</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Amaranth flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Honey</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Margarine</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Plus</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Water</item></ing></ing-div></ingredients>
    <directions>
      <step>  For all recipes, load ingredients into breadmachine pan according to
  manufacturer's directions. Bake on white bread setting unless otherwise
  indicated. "Yeast" refers to active dry yeast.  Delayed timer mode okay.
  Adapted from Donna German, The Bread Machine Cookbook.
  
