<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bay Seafood Seasoning Blend</title>
      <categories>
        <cat>Seasoning</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Ground bay leaves</item></ing>
      <ing>
        <amt>
          <qty>2 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Celery salt</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Dry mustard</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Black pepper</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground nutmeg</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground cloves</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground ginger</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Paprika</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Red pepper (cayenne)</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground mace (optional)</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground cardamom (optional)</item></ing></ingredients>
    <directions>
      <step>  Date: Mon, 25 Mar 96 13:05:40 EST
  
  submitted by: CatRescuer@aol.com (L. J. Robinson, Tulsa, Oklahoma)
  
  Combine ingredients; store in an airtight container. Use with seafood or
  chicken. Makes approx. 1/4 c.
  
