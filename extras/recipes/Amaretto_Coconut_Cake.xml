    <head>
      <title>Amaretto Coconut Cake</title>
      <categories>
        <cat>Desserts</cat>
        <cat>Spirits</cat>
        <cat>Coconut</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>package</unit></amt>
        <item>Cake mix -- prepared as</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Directed</item></ing>
      <ing>
        <amt>
          <qty>12</qty>
          <unit>ounces</unit></amt>
        <item>Coconut, dried</item></ing>
