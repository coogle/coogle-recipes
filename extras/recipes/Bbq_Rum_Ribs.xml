<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bbq Rum Ribs</title>
      <categories>
        <cat>Bbq</cat>
        <cat>Ribs</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>pounds</unit></amt>
        <item>Spareribs</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Ketchup</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Dry mustard</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Dark rum</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Cloves garlic -- crushed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Brown sugar -- packed</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Soy sauce</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chili sauce</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Worcestershire sauce</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit>dashes</unit></amt>
        <item>Pepper</item></ing></ingredients>
    <directions>
      <step>  Wrap ribs in double thickness of foil and bak for 1 1/2 hours at 350 .
  Unwrap and drain drippings. Combine all ingredients and pour over ribs.
  Marinate at room temperature for 1 hour. Bake at 350 for 30 min., basting
  with sauce, or grill 30 min., turning and basting.
  
  Recipe By     :
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
