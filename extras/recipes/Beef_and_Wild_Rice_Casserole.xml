<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Beef and Wild Rice Casserole</title>
      <categories>
        <cat>Crs</cat>
        <cat>Totest</cat>
        <cat>Post</cat>
        <cat>Chopmeat</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>pounds</unit></amt>
        <item>Ground beef</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Chopped onion</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Uncooked wild rice, rinsed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Cream of mushroom soup</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Chicken noodle soup</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cans</unit></amt>
        <item>Water</item></ing></ingredients>
    <directions>
      <step>  Brown beef and onion. Drain; place in a 3 qt. casserole. Combine remaining
  ingredients and mix well. Cover and bake 1 hour 20 minutes at 375F.,
  stirring occasionally.
  
  Taste of Home Ground Beef Collection 1996 Edition
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
