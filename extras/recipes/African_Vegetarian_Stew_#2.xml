<recipeml version="0.5">
  <recipe>
    <head>
      <title>African Vegetarian Stew #2</title>
      <categories>
        <cat>Vegetable</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>small</unit></amt>
        <item>Kohlrabies; peeled and cut Into chunks</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>large</unit></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Sweet Potatoes; peeled and Cut into chunks</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Zucchini; sliced thick</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit/></amt>
        <item>Tomatoes; fresh -or-</item></ing>
      <ing>
        <amt>
          <qty>16</qty>
          <unit>ounces</unit></amt>
        <item>Can Tomatoes</item></ing>
      <ing>
        <amt>
          <qty>15</qty>
          <unit>ounces</unit></amt>
        <item>Can Garbanzo beans</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>(chick-peas) with liquid</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Couscous -or- Bulgar Wheat</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Raisins; dark or golden</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Ground Coriander</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground Turmeric</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground Ginger</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground Cumin</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing></ingredients>
    <directions>
      <step>  From: "Tina D. Bell" &lt;tdbell@altair.csustan.edu&gt;
  
  Date: Thu, 4 Jul 1996 15:20:28 -0700 (PDT)
  Combine all the ingredients in a large saucepan.  Bring to a boil, lower
  the heat, and simmer until the vegetables are tender, about 30 minutes.
  
  Note: Serve the couscous separately, if desired.  Parsnips may be
  substituted for the kohlrabi.
  
