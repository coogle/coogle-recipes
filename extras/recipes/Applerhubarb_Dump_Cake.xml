<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Applerhubarb Dump Cake</title>
      <categories>
        <cat>Cakes</cat>
        <cat>Desserts</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Rhubarb</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Apple pie filling</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>package</unit></amt>
        <item>Strawberry Jello</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Small size</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Stick of butter</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>White cake mix</item></ing></ingredients>
    <directions>
      <step>  1)    Put rhubarb &amp; pie filling in greased 13x9x2 inch pan... 2) Mix sugar
  and Jello; sprinkle over rhubarb... 3) Sprinkle cake mix over that and
  sprinkle water over all... Slice butter on top of cake mix... 4) Bake at
  350 degrees for 45 minutes. Serve warm with vanilla ice cream or whipped
  cream. Serve cold if you're lucky enough to have any left...
  Posted to MM-Recipes Digest  by "Wayne T. Jones" &lt;waynej@mail.austasia.net&gt;
  on Mar 26, 1998
 
</step></directions></recipe></recipeml>
