<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Avocado Dip</title>
      <categories>
        <cat>Dips</cat></categories>
      <yield>10</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>RIPE AVOCADO (PEELED AND SEE</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>MEDIUM ONION (FINELY CHOPPED</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>MAYONNAISE</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>THYME</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>BASIL</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>ROSEMARY</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>SAGE</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>MARJORAM</item></ing>
      <ing>
        <amt>
          <qty>8</qty>
          <unit>ounces</unit></amt>
        <item>SOUR CREAM</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>CRACKERS OR CHIPS</item></ing></ingredients>
    <directions>
      <step>         In a mixing bowl, mash avocado. Add onion,
       mayonnaise, thyme, basil, rosemary, sage,
       marjoram, and sour cream. Mix well. serve with
       crackers or chips.
       Yields 2 cups.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
