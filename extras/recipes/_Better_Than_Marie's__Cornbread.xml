<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>"Better Than Marie's" Cornbread</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Buttermilk Baking Mix; (like Bisquick)</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Yellow cornmeal</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Margarine; melted</item></ing></ingredients>
    <directions>
      <step>  Combine baking mix, cornmeal and sugar in a mixing bowl. Add eggs and milk.
  Beat until well blended. Carefully stir in melted margarine (it may
  splatter). Pour into greased 9 in. square pan. Bake at 350 for 35-40
  minutes or until done. Serves 6.
  
  Posted to TNT Recipes Digest by Lisa &lt;byhisgrc@lightspeed.net&gt; on Mar 24,
  1998
 
</step></directions></recipe></recipeml>
