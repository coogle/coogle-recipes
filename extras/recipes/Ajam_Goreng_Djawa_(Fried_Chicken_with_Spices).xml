<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Ajam Goreng Djawa (Fried Chicken with Spices)</title>
      <categories>
        <cat>Chicken</cat>
        <cat>Indonesian</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Chicken, frying; cut up</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Chilies; split</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Onion, small; sliced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Coriander seed, ground</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Caraway seed, ground</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Turmeric</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Tamarind juice</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Coconut milk</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Oil</item></ing></ingredients>
    <directions>
      <step>  Put chicken in a pan with all the ingredients except the oil. Bring to boil
  then reduce heat and simmer for 20 min. or until liquid has been absorbed.
  Heat oil and deep fry chicken until golden brown.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
