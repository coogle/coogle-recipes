<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Ammonite Chowder</title>
      <categories>
        <cat>Cambrian</cat>
        <cat>Critters</cat>
        <cat>Extinct</cat>
        <cat>Seafood</cat>
        <cat>Weird</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>6 1/2</qty>
          <unit>cups</unit></amt>
        <item>Canned tomatoes; peeled</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>medium</unit></amt>
        <item>Onions; diced</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit/></amt>
        <item>Carrots; peeled, sliced</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>medium</unit></amt>
        <item>Potatoes; peeled, sliced</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Jalapeno peppers; peeled, deveined, sliced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Beef bouillon</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit>pounds</unit></amt>
        <item>Ammonites*</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Butter; melted</item></ing>
      <ing>
        <amt>
          <qty>3 1/4</qty>
          <unit>cups</unit></amt>
        <item>Canned corn</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt; or to taste</item></ing>
      <ing>
        <amt>
          <qty>13</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>dash</unit></amt>
        <item>Tabasco; or to taste</item></ing></ingredients>
    <directions>
      <step>  *If ammonites are unavailable or extinct in your area, squid or conch will
  make an excellent substitute.
  
  In a large pot, break up the tomatoes and add the onion, carrots, potatoes,
  jalapeno peppers and beef bouillon. Cook until tender.
  Contributor:  Roy Olsen (roy@indy.net) (htto://www.indy.net/~roy/) Posted
  to MM-Recipes Digest V4 #147 by roy@indy.net (Roy) on May 27, 1997
 
</step></directions></recipe></recipeml>
