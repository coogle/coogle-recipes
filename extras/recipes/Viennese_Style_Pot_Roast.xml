<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Viennese Style Pot Roast</title>
      <categories>
        <cat>Crockpot</cat>
        <cat>Beef</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>pounds</unit></amt>
        <item>Rump roast or sirloin tip</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Carrot; chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Turnips; chopped</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Dried figs; chopped</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>White wine</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Beef stock</item></ing>
      <ing>
        <amt>
          <qty>8</qty>
          <unit/></amt>
        <item>New potatoes</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Gingersnap cookies; crushed</item></ing></ingredients>
    <directions>
      <step>  Brown meat on all sides in butter. Put carrots, turnips and potatoes in
  bottom of crock pot and place roast on top. Add remaining ingredients,
  except gingersnaps. Cover and cook on low 8 to 10 hours. Add gingersnaps
  and cook on high, uncovered, until thickened.
  Posted to recipelu-digest Volume 01 Number 396 by James and Susan Kirkland
  &lt;kirkland@gj.net&gt; on Dec 22, 1997
 
</step></directions></recipe></recipeml>
