<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Corn Casserole</title>
      <categories>
        <cat>Vegetables</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Creamed corn</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Whole kernel corn</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>large</unit></amt>
        <item>Onion -- chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Bell pepper -- chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>ounces</unit></amt>
        <item>Jar pimiento -- chopped</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt and pepper -- to taste</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Red pepper -- to taste</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Egg -- beaten</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Cracker crumbs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Cheddar cheese -- grated</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Butter -- melted</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients in order given; mix well. Pour into greased 2-quart
  ovenproof casserole.  Bake at 350F for 1 hour.
  
  Recipe By     :
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
