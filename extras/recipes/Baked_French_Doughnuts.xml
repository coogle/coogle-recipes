<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked French Doughnuts</title>
      <categories>
        <cat>Dessert</cat></categories>
      <yield>14</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>5</qty>
            <unit>tablespoons</unit></amt>
          <item>Margarine</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Egg</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>2 1/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Baking powder</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Nutmeg</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Milk</item></ing></ing-div>
      <ing-div>
        <title>TOPPING</title>
        <ing>
          <amt>
            <qty>6</qty>
            <unit>tablespoons</unit></amt>
          <item>Margarine; melted</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Cinnamon-sugar mixture</item></ing></ing-div></ingredients>
    <directions>
      <step>  Date: Tue, 19 Mar 1996 18:30:18 -0500
  
  From: "Vernon E. Phipps Jr." &lt;cook4u@vivanet.com&gt;
  Cream margarine, sugar and mix well. Add egg and mix. Combine dry
  ingredients; add alternately with milk. Fill greased muffin tins half full
  and bake 20 to 25 minutes at 350 degrees.
  
  Remove doughnuts from pan immediately. Dip or toss in melted margarine,
  then to cinnamon-sugar mixture. Makes 14 doughnuts. To make 36 small
  doughnuts, use smaller tart tins and bake for 15 minutes.
  
