<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana Bread with Sour Cream Bread and Wheat Germ</title>
      <categories>
        <cat>Bread machi</cat>
        <cat>Bread maili</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Active dry yeast          ; 2 tsp</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Bread flour           ;  3 c</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Wheat germ    ; 3 T</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Powdered milk ; 3 T</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Sugar  ; 1 1/2T</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt    ; 1 1/2 tsp</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Nutmeg  ; 3/4 tsp</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Vegetable oil ; 1 1/2 T</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Banana; 1 cup plus 2 Tbsp mashed</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Sour cream; 3/4 cup or plain yogurt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Water   ; 1/4 cup plus 1T</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Raisins; 3/4 cup or chopped nuts</item></ing></ingredients>
    <directions>
      <step>  &gt;From: Miriam Meier &lt;MEIER@sals.edu&gt;
  From: Bread-Bakers Archives: ftp.best.com/pub/reggie/archives/bread/recipe
  NOTES : This recipe has TOO MUCH LIQUID! I tried the 1 lb. loaf, and it
       mixed up in the machine much more like a batter than a bread--
       I had to add an extra 3/4 cup of flour before it formed a ball.
 
</step></directions></recipe></recipeml>
