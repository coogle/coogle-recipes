<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Autumn Sweet Potato Bake</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>3</qty>
            <unit/></amt>
          <item>Fresh sweet potatoes or 16 oz can drained - sliced</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>can</unit></amt>
          <item>(16 oz) apricot halves</item></ing></ing-div>
      <ing-div>
        <title>SAUCE</title></ing-div>
      <ing-div>
        <title>COMBINE</title>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Brown sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Corn Starch</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1/8</qty>
            <unit>teaspoons</unit></amt>
          <item>Cinnamon</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Shake of nutmeg</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Liquid from apricots; (about 1 cup)</item></ing></ing-div>
      <ing-div>
        <title>ADD</title>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>Light raisins</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Sherry or 2 T lemon juice</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Grated orange rind</item></ing></ing-div></ingredients>
    <directions>
      <step>  Spread sauce over potatoes/apricot mixture.
  
  Bake at 350 degrees for 20 minutes
  
  Posted to Bakery-Shoppe Digest V1 #472 by christine miller &lt;jcm@iquest.net&gt;
  on Dec 27, 1997
 
</step></directions></recipe></recipeml>
