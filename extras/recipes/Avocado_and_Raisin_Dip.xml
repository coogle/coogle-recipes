<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Avocado and Raisin Dip</title>
      <categories>
        <cat>Vegetables</cat>
        <cat>Dips</cat>
        <cat>Fruits</cat>
        <cat>Mexican</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Avocados; Peeled &amp; Chopped</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Raisins</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Vegetable Oil</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Lime Juice</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Freshly Ground Pepper</item></ing></ingredients>
    <directions>
      <step>  Place all ingredients in blender container.  Cover and blend on high speed
  until smooth, about 45 seconds.  Serve with raw vegetables, assorted
  crackers or fried tortillas. Makes 1 2/3 cups of dip.
  
  Miscellaneous recipes from the collection of Mike Orchekowski.
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mo-misc.zip
 
</step></directions></recipe></recipeml>
