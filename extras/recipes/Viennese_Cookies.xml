<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Viennese Cookies</title>
      <categories>
        <cat>Cookies</cat>
        <cat>Ethnic</cat>
        <cat>Chocolate</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Pecans</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cloves</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>pinches</unit></amt>
        <item>Lemon rind 1"x2"</item></ing>
      <ing>
        <amt>
          <qty>2 1/2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sweet chocolate chips</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Butter; softened</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cinnamon</item></ing></ingredients>
    <directions>
      <step>  MAKES 7 DOZEN COOKIES PREHEAT OVEN TO 400 DEGREES CHOP PECANS. ADD FLOUR
  AND CHIPS. GRATE LEMON INGREDIENTS IN MIXER OR PROCESSOR. POUR INTO FLOUR
  MIX AND MIX TOGETHER WELL. DROP BY TEASPOONFULS ON UNGREASED BAKING SHEET.
  BAKE 6 TO 8 MINUTES UNTIL LIGHTLY BROWNED.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
