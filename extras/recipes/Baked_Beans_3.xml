  <recipe>
    <head>
      <title>Baked Beans</title>
      <categories>
        <cat>Pasta/beans</cat>
        <cat>Vegetables/</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>30</qty>
          <unit>ounces</unit></amt>
        <item>Kidney beans, canned</item></ing>
      <ing>
        <amt>
          <qty>15</qty>
          <unit>ounces</unit></amt>
        <item>Black beans, canned</item></ing>
      <ing>
        <amt>
          <qty>15</qty>
          <unit>ounces</unit></amt>
        <item>Cannellini beans, canned</item></ing>
      <ing>
        <amt>
          <qty>30</qty>
          <unit>ounces</unit></amt>
        <item>Canned Italian plum tomatoes, drained and chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Chopped onion</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>large</unit></amt>
        <item>Garlic cloves</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Dark molasses</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Cider vinegar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Ons, honey</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>S, dried oregano</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>S, dry mustard</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>S, ground cumin</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>S, ground ginger</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>On, chili powder</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pinch</unit></amt>
        <item>Crushed red pepper</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt to taste</item></ing></ingredients>
    <directions>
      <step>  Drain and rinse the beans, mix with all the other ingredients. Bake,
  covered, at 350Ã¸F for 45 minutes, then another 30 minutes uncovered.
