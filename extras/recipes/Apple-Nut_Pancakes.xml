<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple-Nut Pancakes</title>
      <categories>
        <cat>Breakfast</cat></categories>
      <yield>10</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Egg</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Oil or melted margarine</item></ing>
      <ing>
        <amt>
          <qty>1 1/4</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Baking powder</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Walnuts</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Peeled apple/cubes</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Cloves</item></ing></ingredients>
    <directions>
      <step>  Preheat griddle, 350F (180C) or medium high. Put all ingredientes in the
  process at MIX until batter is almost smooth.
  
  Bake on lightly greased griddle until nicely browned. Turn only once.
  Yield: 10-12 pancakes.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
