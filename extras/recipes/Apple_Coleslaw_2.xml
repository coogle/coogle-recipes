<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Coleslaw</title>
      <categories>
        <cat>Salad</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Plain; nonfat yogurt, drained overnight in a coffee filter in the refridgerator</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Prepared horseradish</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Prepared mustard</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>dash</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Apple; cored and chopped</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Shredded cabbage</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Shredded carrot</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Thinly sliced green onion</item></ing></ingredients>
    <directions>
      <step>  Date: Mon, 10 Jun 1996 20:57:53 -0700 (PDT)
  
  From: "Tina D. Bell" &lt;tdbell@altair.csustan.edu&gt;
  
  BH&amp;G LOW CALORIE RECIPES, SUMMER, 1986 Combine first 6 ingredients. Add
  apple, stir to coat well.  Stir in rest of ingredients, until evenly mixed
  and coated.  Cover and chill at least 3 hours or overnight. Makes 4
  servings
  
  FATFREE DIGEST V96 #161
  
  From the Fatfree Vegetarian recipe list.  Downloaded from Glen's MM Recipe
  Archive, http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
