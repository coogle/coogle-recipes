<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana Nut Bread, Memorable</title>
      <categories>
        <cat>Breads</cat>
        <cat>Yeast free</cat>
        <cat>Breads: qui</cat></categories>
      <yield>16</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Egg Beaters 99% egg substitute</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Light corn syrup</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar; (1/2 to 1 cup)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Bananas; mashed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Baking soda</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Baking powder</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>1% low-fat buttermilk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Grape-Nuts</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>No-stick cokking spray</item></ing></ingredients>
    <directions>
      <step>  Mix all ingredients; bake in a large sprayed loaf pan at 350 degrees F. for
  50-60 minutes. Do not over-bake. Test for doneness with toothpick.
  
  Per serving: 141 Calories; less than one gram Fat (2% calories from fat);
  3g Protein; 33g Carbohydrate; 0mg Cholesterol; 178mg Sodium =46ood
  Exchanges: 1 Starch/Bread; 1/2 Fruit; 1 Other Carbohydrates
  
  NOTES : Easy preparation!
  
  Recipe by: The Lite Switch - June McLean Jeter
  
  Posted to EAT-LF Digest by "Ellen C." &lt;ellen@elekta.com&gt; on May 18, 1998
 
</step></directions></recipe></recipeml>
