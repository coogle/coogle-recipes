<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Onion Rye for Bread Machine</title>
      <categories>
        <cat>Breadmaker</cat>
        <cat>Fruits</cat>
        <cat>Vegetables</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2 1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Active dry yeast</item></ing>
      <ing>
        <amt>
          <qty>1 3/4</qty>
          <unit>cups</unit></amt>
        <item>Bread flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Bread flour</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Rye flour</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Cracked wheat</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Caraway seeds</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Cloves garlic; minced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Onion; minced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>large</unit></amt>
        <item>Apple; chopped</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Unsalted butter</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing></ingredients>
    <directions>
      <step>  Add all the ingredeints in the order suggested by your bread machine manual
  and process on the basic bread cycle according to the manufacturer's
  directions. Let loaf cool completely before slicing From: Best Bread
  Machine Cookbook Ever, by Madge Rosenburg. Shared by June Hoffman on ECHO,
  and formatted for MM by Marge Nemeth.
  Posted to MC-Recipe Digest V1 #813 by Nancy Berry &lt;nlberry@prodigy.net&gt; on
  Sep 28, 1997
 
</step></directions></recipe></recipeml>
