<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Abm Whole Wheat Bread</title>
      <categories>
        <cat>Breads</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>1 POUND LOAF</title>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>1.50 ts Active dry or fastrise yeast</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>1.00 c Whole wheat flour</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>1.00 c Bread flour</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>2.00 tb Wheat germ</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>2.00 tb Sunflower seeds</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>1.00 ts Salt</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>2.00 tb Vegetable oil</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>1.00 tb Mild molasses</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>0.75 c Plus 1 T water</item></ing></ing-div>
      <ing-div>
        <title>1 1/2 POUND LOAF</title>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>2.50 ts Yeast</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>1.50 c Whole wheat flour</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>1.50 c Bread flour</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>0.25 c Wheat germ</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>0.25 c Sunflower seeds</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>1.50 ts Salt</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>0.25 c Oil</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>2.00 tb Molasses</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>1.00 c Plus 1 T water</item></ing></ing-div>
      <ing-div>
        <title>VARIATIONS</title>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>0.50 c Raisins OR</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>0.50 c Walnuts; chopped OR</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>0.50 c Apricots; chopped</item></ing></ing-div></ingredients>
    <directions>
      <step>  Bake according to ABM directions.
  
  Source: LHJ, 11/93 :: MM by Sue Woodward
  
