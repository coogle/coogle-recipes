<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Cucumbers</title>
      <categories>
        <cat>Vegetables</cat></categories>
      <yield>3</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>large</unit></amt>
        <item>Cucumbers, sliced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Spanish onions</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Chili sauce</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Margarine</item></ing></ingredients>
    <directions>
      <step>  Arrange cucumbers and onions in layers, adding to each layer, salt, pepper
  and flour.  Cover with chili sauce and dot with butter. Bake at 350 degrees
  until tender, about 30 minutes. Randy Rigg
 
</step></directions></recipe></recipeml>
