<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Italian Zuchinni</title>
      <categories>
        <cat>Vegetables</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>MEDIUM ZUCCHINI SLICED 1/2"</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>MEDIUM ONION, SLICED</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>OREGANO</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>TOMATO, SLICED (OPTIONAL)</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>DRIED BASIL</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>dash</unit></amt>
        <item>SALT</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>dash</unit></amt>
        <item>PEPPER</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>TOMATO SAUCE</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>CUBED BREAD, BUTTERED *</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>GRATED PARMESAN CHEESE</item></ing></ingredients>
    <directions>
      <step>  *      USE 1/2 CUP BREAD CRUMBS INSTEAD OF CUBED BREAD *
  ~------------------------------------------------------------------- *
  LAYER IN A ONE QUART BAKING DISH, ZUCCHINI, ONION AND TOMATO, IF DESIRED
  SEASONNING EACH LAYER WITH OREGANO, PEPPER, BASIL AND SALT. POUR TOMATO
  SAUCE TO BARELY COVER VEGETABLES. COVER AND BAKE AT 350 DEG F. FOR 45
  MINUTES. UNCOVER AND TOP WITH BREAD CUBES OR CRUMBS AND GRATED CHEESE. BAKE
  UNCOVERED FOR 10 MINUTES.
  Posted to MM-Recipes Digest  by scotlyn@juno.com (Daniel S Johnson) on Mar
  22, 1998
 
</step></directions></recipe></recipeml>
