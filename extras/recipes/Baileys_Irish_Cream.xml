<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baileys Irish Cream</title>
      <categories>
        <cat>Beverage</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Irish whiskey</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Sweetened condensed milk</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Whipping cream</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Chocolate extract</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Coconut extract</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Instant coffee</item></ing></ingredients>
    <directions>
      <step>  Blend all ingredients together.  Store a couple of days in the
  refrigerator.
  
