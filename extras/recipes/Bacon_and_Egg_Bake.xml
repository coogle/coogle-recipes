<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bacon and Egg Bake</title>
      <categories>
        <cat>None</cat></categories>
      <yield>7</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>slices</unit></amt>
        <item>Bacon</item></ing>
      <ing>
        <amt>
          <qty>16</qty>
          <unit>ounces</unit></amt>
        <item>Cheddar cheese -- shredded</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>medium</unit></amt>
        <item>Onions -- sliced</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit>dashes</unit></amt>
        <item>Salt and pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Cream of mushroom soup</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>English muffins -- split</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Split and toasted</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit/></amt>
        <item>Hard cooked eggs -- sliced</item></ing></ingredients>
    <directions>
      <step>  Heat oven to 350~.  Fry bacon until crisp; remove from skillet and drain,
  reserve 2 Tb fat.  Saute onion in the reserved bacon fat. Stir in soup,
  milk, eggs, cheese, and seasonings. Pour mixture into 9x13 inch baking
  dish; top with bacon. Bake for 20 minutes. Serve over muffin halves.
  
  Recipe By     :
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
