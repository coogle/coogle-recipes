<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bbq Beef Brisket</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>pounds</unit></amt>
        <item>Beef Brisket; (up to 4)</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Liquid Smoke</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Garlic salt 1 tsp. onion salt</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Worcestershire</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Celery seed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Barbecue sauce</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground pepper</item></ing></ingredients>
    <directions>
      <step>  Preparation : Cover both sides of brisket with Liquid Smoke, garlic, onion,
  celery seed and Worcestershire sauce. Wrap in foil and place in crock pot.
  Cook on low for 8-10 hours. Remove and slice across grain. Return to pot,
  add barbecue sauce and cook 1 hour.
  
  Posted to recipelu-digest Volume 01 Number 624 by molony &lt;molony@scsn.net&gt;
  on Jan 28, 1998
 
</step></directions></recipe></recipeml>
