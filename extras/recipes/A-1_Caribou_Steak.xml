<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>A-1 Caribou Steak</title>
      <categories>
        <cat>Game</cat>
        <cat>Caribou</cat>
        <cat>Steak</cat>
        <cat>Jw</cat></categories>
      <yield>2</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Caribou steak</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter; divided</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>small</unit></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Sherry</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Worcestershire</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>A-1 steak sauce</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Chopped chives</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Chopped parsley</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt &amp; pepper</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Brandy</item></ing></ingredients>
    <directions>
      <step>  Cook onions in 2 tbsp butter until golden.  Pound steak to tenderize
  and sear over high heat in butter &amp; onion, about 2 min on each side. Add
  sherry, A-1, 2 tbsp butter, Worcestershire, chives, parsley, pepper and
  salt.
  Reduce heat to med and cook for 3-4 mins per side. Add brandy, bring to the
  table, flame and serve.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/caribou.zip
 
</step></directions></recipe></recipeml>
