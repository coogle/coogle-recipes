<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apricot Chews</title>
      <categories>
        <cat>Cookie</cat></categories>
      <yield>24</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Dried Apricots; finely Chopped</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Raisins</item></ing>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>Water</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Baking Powder</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Baking Soda</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit/></amt>
          <item>Eggs</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Lemon Juice</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Crushed Pineapple; Drained</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Nuts; chopped (optional)</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Powdered Sugar or Frosting</item></ing></ing-div>
      <ing-div>
        <title>FROSTING</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Powdered Sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Butter</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>APricot Preservesp</item></ing></ing-div></ingredients>
    <directions>
      <step>  Cook apricots and raisins over low heat in water until water is absorbed.
  Cool slightly and add remaining ingredients except powdered sugar or
  frosting. Blend well and spread in a greased 9" pan. Bake at 350F for 35-40
  minutes. Cut into bars while warm and sprinkle with powdered sugar or
  spread with frosting.
  
  Blend well.  Spread on bars.
  
  Makes 24 bars
  
