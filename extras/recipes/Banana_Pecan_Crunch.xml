<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana Pecan Crunch</title>
      <categories>
        <cat>Desserts</cat>
        <cat>Fruits</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Molasses</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit/></amt>
        <item>Bananas -- peeled and</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Sliced</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Crosswise</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter -- melted</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Pecans -- chopped</item></ing></ingredients>
    <directions>
      <step>  In an 11x7x1-1/2-inch baking pan, melt 2 tablespoons butter; layer bananas
  and nuts in pan.  Drizzle with molasses. Sprinkle brown sugar and melted
  butter atop.  Bake in a 350 degree oven for 10 minutes. Serve with ice
  cream.  Yield: 6
  
  Recipe By     :
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
