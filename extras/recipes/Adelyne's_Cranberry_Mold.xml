<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Adelyne's Cranberry Mold</title>
      <categories>
        <cat>Side</cat>
        <cat>Dishes</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cans</unit></amt>
        <item>Cranberries -- whole</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>packages</unit></amt>
        <item>Raspberry Gelatin Powder --</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Or Strawberry</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Boiling water</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Cranberry Juice -- (drained</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>From cans)</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Whole Apples -- chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Walnuts -- chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Mayonnaise</item></ing></ingredients>
    <directions>
      <step>  1.  Heat and drain the cranberries, reserve the juice.
  
  2.  Make jello with juice and boiling water.
  
  3.  Chill until slightly molded.
  
  4.  Add mayonnaise and beat until smooth.
  
  5.  Add cranberries, apples, and nuts.  Chill until firm.
  
  Recipe By     : Adelyne Janousek
  
  From: Ladies Home Journal- August 1991
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
