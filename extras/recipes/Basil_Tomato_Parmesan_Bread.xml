<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Basil Tomato Parmesan Bread</title>
      <categories>
        <cat>A</cat>
        <cat>B</cat></categories>
      <yield>10</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Olive oil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Dried basil</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Parmesan cheese; grated</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Bread flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Yeast</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Sun-dried tomatoes; chopped</item></ing></ingredients>
    <directions>
      <step>  From: "Tracie Radtke" &lt;tracie_host@email.msn.com&gt;
  
  Makes a 1 pound loaf Add all ingredients except tomatoes in the order
  suggested by bread machine instructions. Set machine for white bread,
  medium crust. Add tomatoes when machine beeps.
  
  Recipe by: KAWARD6 ROTW - Eat-L
  
  Posted to recipelu-digest by jeryder@juno.com on Mar 10, 1998
 
</step></directions></recipe></recipeml>
