<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Avocado Corn Salsa</title>
      <categories>
        <cat>Dips</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Fresh corn kernels</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Diced medium avocado</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Tomato seeded and diced</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit/></amt>
        <item>Jalapeno chile minced</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit/></amt>
        <item>Minced small onion</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Lime juice</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt to taste</item></ing></ingredients>
    <directions>
      <step>  Cook corn and drain. Put in glass bowl and add avocado, tomato, jalapeno,
  onion, lime juice and salt. TIP: Use fresh lime.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
