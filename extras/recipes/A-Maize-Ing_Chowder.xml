<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>A-Maize-Ing Chowder</title>
      <categories>
        <cat>Soups</cat>
        <cat>Low-fat</cat>
        <cat>Lunch</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sliced leek or onion</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sliced carrot</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>small</unit></amt>
        <item>Baking potato cubed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Defatted chicken broth</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Evaporated skim milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Frozen whole kernel corn</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Chopped parsley</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt &amp; pepper to taste</item></ing></ingredients>
    <directions>
      <step>  1. In medium saucepan, combine leek, carrot and potato with chicken broth.
  2. Cover and simmer 10 minutes or until vegetables are tender. 3. Puree
  then add milk and corn. Heat without boiling, to serving temperature. 4.
  Season to taste with salt and pepper. 5. Serve sprinkled with parsley.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
