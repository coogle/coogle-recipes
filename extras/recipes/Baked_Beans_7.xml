<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Beans</title>
      <categories>
        <cat>All newly t</cat>
        <cat>Vegetables</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>quart</unit></amt>
        <item>Cooked brown beans; drained, rinsed</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Catsup</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Prepared mustard</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>small</unit></amt>
        <item>Chopped onion</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Chopped green chilies</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Light molasses</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients in bean pot or 1 3/4 quart casserole. Bake at 350,
  covered, for 1 hour. (Can prepare in microwave on HIGH for 20 minutes.)
  Yield 4-6 servings.
  
  Recipe by: Taste of Home Posted to MC-Recipe Digest V1 #730 by L979@aol.com
  on Aug 9, 1997
 
</step></directions></recipe></recipeml>
