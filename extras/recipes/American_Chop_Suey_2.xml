<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>American Chop Suey</title>
      <categories>
        <cat>Beef</cat>
        <cat>Main dish</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Ground beef</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Celery; chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>small</unit></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Mushrooms</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Cream of chicken soup</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>tablespoons</unit></amt>
        <item>Soy sauce</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Rice</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Chop suey noodles</item></ing></ingredients>
    <directions>
      <step>  Brown beef, and add celery and onion and simmer. Add mushrooms, soup, and 1
  1/2 cup water, soy sauce, and rice. Bake for 1 hour. Put Chop Suey noodles
  on top and bake for 15 minutes more. busted by sooz
  Posted to recipelu-digest Volume 01 Number 170 by James and Susan Kirkland
  &lt;kirkland@gj.net&gt; on Oct 27, 1997
 
</step></directions></recipe></recipeml>
