<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Beans and Eggs</title>
      <categories>
        <cat>Beans</cat>
        <cat>Breakfast</cat>
        <cat>Eggs</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>ounces</unit></amt>
        <item>Sausage links -- cut in 1/2"</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Cubes</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Onion -- chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Vegetable oil</item></ing>
      <ing>
        <amt>
          <qty>1 2/3</qty>
          <unit>cups</unit></amt>
        <item>Beans</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Catsup</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Worcestershire sauce</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Basil -- crushed</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Avocado -- sliced, opt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Orange -- peeled,sliced,opt</item></ing></ingredients>
    <directions>
      <step>  Brown sausage; remove from pan.  Saute onion in oil. Add beans, sausage,
  catsup, water, and seasonings. Simmer, covered, 20 minutes. Cook eggs as
  desired. Arrange bean mixtureon platter with eggs, avocado and orange.
  Makes 6 servings.
  
  Recipe By     : Idaho Bean Commission
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
