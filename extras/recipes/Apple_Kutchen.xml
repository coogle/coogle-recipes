<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Kutchen</title>
      <categories>
        <cat>Pies</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>All-Purpose Flour, sifted</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>teaspoons</unit></amt>
          <item>Baking Powder</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Margarine, softened</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Whole Egg</item></ing>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>Milk</item></ing></ing-div>
      <ing-div>
        <title>TOPPING</title>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Margarine, cold</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Brown Sugar</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Granulated Sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>All-Purpose Flour, sifted</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Vanilla</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Cinnamon, to taste</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Apples</item></ing></ing-div></ingredients>
    <directions>
      <step>  Mix first six ingredients till soft dough forms. Dump into greased jelly
  roll pan, With buttered hands spread till covers pan evenly. Peel and slice
  apples to cover dough. Use lots. For topping: mix remaining ingredients and
  crumble on top. Bake at 350 for 45 minutes.
  
  Recipe by: Dianne Waller Posted to MC-Recipe Digest V1 #657 by Barbara
  Dianne Waller &lt;dwaller@frontier.gulf.net&gt; on Jul 06, 1997
 
</step></directions></recipe></recipeml>
