<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Beans for the Crockpot</title>
      <categories>
        <cat>Crockpot</cat>
        <cat>Beans</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Dried small white beans; rinsed</item></ing>
      <ing>
        <amt>
          <qty>4 1/2</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Molasses</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Onion; chopped</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>pounds</unit></amt>
        <item>Salt pork; cut into 1" cubes</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Dijon mustard</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing></ingredients>
    <directions>
      <step>  In crock pot, combine all ingredients. Cover and cook on low 13 to 14 hrs.
  , stirring occasionally, if possible. 6 to 8 servings.
  Posted to recipelu-digest Volume 01 Number 411 by James and Susan Kirkland
  &lt;kirkland@gj.net&gt; on Dec 28, 1997
 
</step></directions></recipe></recipeml>
