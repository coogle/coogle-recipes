  <recipe>
    <head>
      <title>Banana &amp; Walnut Muffins</title>
      <categories>
        <cat>Bread</cat></categories>
      <yield>18</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>ounce</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Nutmeg</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Egg</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Chopped walnuts</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Grated lemon rind</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Bananas</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking powder</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Grated orange rind</item></ing></ingredients>
    <directions>
      <step>  Melt butter, mash bananas. Sift flour, b/pdr, nutmeg, sugar &amp; salt into a
  bowl. Add melted butter, milk, egg, oil, nuts &amp; rinds to bananas. Make a
  well in centre of dry ingredients &amp; gently stir in banana mixture. Spoon
  into prepared pans &amp; bake at 400Ã¸F for 15-20 mins. Makes 18.
  
