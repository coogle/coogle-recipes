<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Walnut Vinaigrette</title>
      <categories>
        <cat>Dressings</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Canned No Salt Chicken</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Broth</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Walnut Oil</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Balsamic Vinegar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Sherry Vinegar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Dijon Mustard</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/8</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cloves</unit></amt>
        <item>Garlic Minced</item></ing></ingredients>
    <directions>
      <step>  Bring Chicken Broth To A Boil in A Small Saucepan. Cook About 6Min. OR
  Until Reduced To 2/3 C.  Combine Chicken Broth &amp; Remainingingredients in A
  Jar; Cover Tightly &amp; Shake Vigorously. Cover &amp; Chill. (Fat 1.7. Chol. 0.)
 
</step></directions></recipe></recipeml>
