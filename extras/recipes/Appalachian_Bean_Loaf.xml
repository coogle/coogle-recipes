<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Appalachian Bean Loaf</title>
      <categories>
        <cat>Beans</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Beans -- mashed</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Soft bread crumbs</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs -- beaten</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>OR bacon drippings</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Onion -- chopped</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients using enough milk to moisten. Pack firmly into well
  buttered loaf pan.  Bake at 350F for 1 hour or until firm. Serve with
  tomato sauce.
  
  Recipe By     : The Little Mountain Bean Bible Cookbook
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
