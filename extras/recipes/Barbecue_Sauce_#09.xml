<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbecue Sauce #09</title>
      <categories>
        <cat>Sauce</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>pounds</unit></amt>
        <item>Onions</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Bell peppers</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Stalk celery</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Stem garlic</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pint</unit></amt>
        <item>Cooking oil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pint</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cans</unit></amt>
        <item>Tomato paste</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Tomato sauce</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Prepared mustard</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Bottle (24-oz) catsup</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt &amp; pepper to taste</item></ing></ingredients>
    <directions>
      <step>  Grind together onions, bell pepper, celery and garlic. Mix all the
  ingredients together and cook on medium to low heat for 1-1/2 to 2 hours.
  Makes 3-1/2 quarts.
  
  From &lt;A Taste of Louisiana&gt;.  Downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
