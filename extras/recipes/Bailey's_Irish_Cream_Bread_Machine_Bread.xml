<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bailey's Irish Cream Bread Machine Bread</title>
      <categories>
        <cat>Irish</cat>
        <cat>Abm</cat>
        <cat>Lo/no-fat</cat>
        <cat>Bread</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/4</qty>
          <unit>cups</unit></amt>
        <item>Water ; very warm</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>package</unit></amt>
        <item>Yeast</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Quaker Oats</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Bread flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Honey</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Bailey's irish cream</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>; or to taste</item></ing></ingredients>
    <directions>
      <step>  Recipe by: "Aine.McManus" , Gaelic List
  
  Put ingredients in pan in order listed,  Select "white  bread" setting on
  machine and press Start. The bread doesn't rise much, but the texture is
  great.
  Posted to Bread-bakers Digest by endy Lockman on Apr 22, 1996
 
</step></directions></recipe></recipeml>
