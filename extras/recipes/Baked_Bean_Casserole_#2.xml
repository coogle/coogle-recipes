<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Bean Casserole #2</title>
      <categories>
        <cat>Casserole</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>8</qty>
          <unit>slices</unit></amt>
        <item>Bacon; fried, drained and crumbled</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Onion; cut in rings</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Dry mustard</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Garlic salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Vinegar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(16-oz) lima beans</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(16-oz) kidney beans</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(16-oz) New England style baked beans</item></ing>
      <ing>
        <amt>
          <qty>From:</qty>
          <unit/></amt>
        <item>Kelly &lt;kellyh@FOTO.INFI.NET&gt;</item></ing>
      <ing>
        <amt>
          <qty>Date:</qty>
          <unit/></amt>
        <item>Wed, 26 Jun 1996 19:27:28 -0400</item></ing></ingredients>
    <directions>
      <step>  After frying bacon, place onion in skillet with bacon drippings.  Add
  sugar, dry mustard, garlic salt, salt and vinegar. Cook covered, 10
  minutes. Combine beans. Add bacon to onion mixture and pour over beans.
  Pour in a 3-quart casserole and bake at 350 degrees for 1 hour. Piedmont
  Region, North Carolina, USA
  
  EAT-L Digest 25 June 1996
  
  From the EAT-L recipe list.  Downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
