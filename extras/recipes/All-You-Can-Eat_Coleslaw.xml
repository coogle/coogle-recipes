  <recipe>
    <head>
      <title>All-You-Can-Eat Coleslaw</title>
      <categories>
        <cat>Salads</cat>
        <cat>Vegetables</cat>
        <cat>Formatted</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Cabbage, head; grated</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt; to taste</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Pepper, black; to taste</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>packages</unit></amt>
        <item>Sweetener, artificial</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Vinegar, wine</item></ing></ingredients>
    <directions>
      <step>    Toss and chill. Each serving = however much you eat!
  
