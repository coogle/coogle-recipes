<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baby Kay's Shrimp Roumalade</title>
      <categories>
        <cat>Appetizers</cat>
        <cat>Fish</cat></categories>
      <yield>16</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Parsley, fresh, chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Garlic clove, fresh, minced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Mayonnaise</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Horseradish (or to taste)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Tabasco</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>tablespoons</unit></amt>
        <item>Lemon juice, (2 lemons)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Creole Mustard (up to 2 T)*</item></ing>
      <ing>
        <amt>
          <qty>72</qty>
          <unit/></amt>
        <item>Shrimp, fresh, boiled(to 84)</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt to taste</item></ing></ingredients>
    <directions>
      <step>  *Creole Mustard such as Zatarain's brand can be found
  in well stocked supermarkets and gourmet food stores.
  :       Mix all ingredients except shrimp together and
  refrigerate. Serve with shrimp.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
