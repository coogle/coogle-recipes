<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Autumn Pumpkin Bars</title>
      <categories>
        <cat>Cookies</cat></categories>
      <yield>48</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>15</qty>
            <unit>ounces</unit></amt>
          <item>Pumpkin; canned</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>1 3/4</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Baking powder</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Soda</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Cinnamon</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Nutmeg</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Cloves</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Orange peel</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Oil</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Raisins</item></ing>
        <ing>
          <amt>
            <qty>4</qty>
            <unit/></amt>
          <item>Eggs</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Pecans; chopped</item></ing></ing-div>
      <ing-div>
        <title>FROSTING</title>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>ounces</unit></amt>
          <item>Cream cheese</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Margarine</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Orange juice</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Powdered sugar</item></ing></ing-div></ingredients>
    <directions>
      <step>  Grease 15x10" baking pan. In large bowl, blend all ingredients except
  raisins and nuts. Blend 2 minutes. Stir in raisins and nuts. Spread evenly
  in pan. Bake at 350~ for 25-30 minutes or until inserted toothpick comes
  out clean. Cool. Frosting. In small bowl, beat cream cheese, margarine and
  orange juice. Gradually beat in powdered sugar until smooth. Spread over
  cooled bars.
  Posted to MC-Recipe Digest V1 #836 by Lynda Beaugez &lt;beaugez@bellsouth.net&gt;
  on Oct 10, 1997
 
</step></directions></recipe></recipeml>
