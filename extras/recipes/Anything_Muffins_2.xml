<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Anything Muffins</title>
      <categories>
        <cat>Low-fat</cat>
        <cat>Muffins</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>large</unit></amt>
          <item>Eggs; or 4 egg whites</item></ing>
        <ing>
          <amt>
            <qty>3/4</qty>
            <unit>cups</unit></amt>
          <item>Brown sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Vanilla</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Applesauce</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Baking powder</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing></ing-div>
      <ing-div>
        <title>USE ONE OF THE FOLLOWING</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Mashed banana &amp; 1/2 C low sugar marmalade</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Mashed banana &amp; 1/2 C low sugar strawberry preserves</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>Fresh blueberries</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>Apples; chopped granny smith</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Banana &amp; 1/2 C mandarin orange pieces; mashed</item></ing></ing-div></ingredients>
    <directions>
      <step>  Preheat oven to 350 degrees. Mix dry ingredients thoroughly, set aside. Mix
  wet ingredients in a large bowl, add dry ingredsients, mix until just
  blended, add fruit. Bake for 30 minutes or until toothpick inserted in
  center of muffin comes out clean
  
  Recipe by: Net
  
