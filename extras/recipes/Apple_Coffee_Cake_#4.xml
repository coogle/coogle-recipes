<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Coffee Cake #4</title>
      <categories>
        <cat>Cake</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>All-purpose flour</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Granulated sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking soda</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Oil; salad</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs; large</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Coffee</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Vanilla extract</item></ing>
      <ing>
        <amt>
          <qty>2 1/2</qty>
          <unit>cups</unit></amt>
        <item>Apples; peeled and sliced (i.e. Granny Smith)</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Nuts; chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Non-stick spray (or butter); for cake pan</item></ing>
      <ing>
        <amt>
          <qty>Date:</qty>
          <unit/></amt>
        <item>Fri, 14 Jun 1996 11:44:22 -0500</item></ing>
      <ing>
        <amt>
          <qty>From:</qty>
          <unit/></amt>
        <item>TeAntae Turner &lt;tturner@GRAPHSOFT.COM&gt;</item></ing></ingredients>
    <directions>
      <step>  Source: The Disney Institute, From Studio Bakery You will need a bundt cake
  pan (or other baking pan).  Mix all ingredients together in a bowl. Grease
  the cake pan or use non-stick spray. Pour mixture into the pan. Bake at
  350'F for 60 minutes.
  
