<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>5 Minute Vegetable Soup ( in Pressure Cooker )</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Diced carrots</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Diced onions</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Stalk celery chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Clove garlic chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Stewed tomatoes</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Tomato puree</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Beef bouillon cubes</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>quart</unit></amt>
        <item>Plus 1 C water</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Cleaned and cut up fresh string beans</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Barley</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pinch</unit></amt>
        <item>Dried basil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Worcestershire sauce</item></ing></ingredients>
    <directions>
      <step>  In a 6 qt. pressure cooker sprayed with Pam, saute the carrots, onions and
  celery till limp. Dust with the flour. Add all remaining ingredients and
  cook under high pressure for 5 minutes. Remove from heat, let pressure drop
  naturally, remove lid carefully and enjoy!
  
  Posted to EAT-L Digest  by Cynthia Ryan &lt;cdryan@JUNO.COM&gt; on Nov 24, 1997
 
</step></directions></recipe></recipeml>
