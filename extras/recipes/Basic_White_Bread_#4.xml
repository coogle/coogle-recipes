<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Basic White Bread #4</title>
      <categories>
        <cat>Bread</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>teaspoons</unit></amt>
        <item>Active Dry Yeast</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>Bread Flour</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Dry Milk Powder</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>9</qty>
          <unit>ounces</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>From:</qty>
          <unit/></amt>
        <item>Joel Ehrlich &lt;Joel.Ehrlich@SALATA.COM&gt;</item></ing>
      <ing>
        <amt>
          <qty>Date:</qty>
          <unit/></amt>
        <item>Mon, 5 Aug 1996 06:50:45 -0800</item></ing></ingredients>
    <directions>
      <step>  All ingredients should be at room temperature before starting.  Place the
  ingredients in the pan in the order listed.  Select "White Bread".  Press
  "Start".
  
