<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Ann's Salsa for Rubio's Fish Tacos</title>
      <categories>
        <cat>Tex-mex</cat>
        <cat>Condiments</cat>
        <cat>Main dish</cat>
        <cat>Spicy</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>PUCKETT; (HGPS48A)</item></ing>
      <ing>
        <amt>
          <qty>28</qty>
          <unit>ounces</unit></amt>
        <item>Diced tomatoes; cans of Tomatoes, 2 dor 3 each from one of the cans, of tomatoes</item></ing>
      <ing>
        <amt>
          <qty>14</qty>
          <unit>ounces</unit></amt>
        <item>Ortega diced green chiles</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>bunches</unit></amt>
        <item>Green onions; chopped</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Fresh cilantro; chopped</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Cloves Garlic; chopped</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Jalapeno pepper; chopped (note on jalapenos)</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Comino seed; ground</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Black pepper; and a tiny</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>dash</unit></amt>
        <item>Cayenne</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients. Place a little in blender for a few seconds on
  pulse cycle, low speed, just long enough for the tomatoes to become a
  little bit smaller (can also be done in food processor). Cover and
  refrigerate. Flavor blends best when made 24 hours before serving.
  Posted to MC-Recipe Digest by Nancy Berry &lt;nlberry@prodigy.net&gt; on Apr 06,
  1998
 
</step></directions></recipe></recipeml>
