<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Albacore Vegetable Pilaf</title>
      <categories>
        <cat>Casseroles</cat>
        <cat>Vegetables</cat></categories>
      <yield>68</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Uncooked white rice</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Each,diced red pepper, green</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Pepper, zucchini, broccoli</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Florets and corn  OR</item></ing>
      <ing>
        <amt>
          <qty>2 1/2</qty>
          <unit>cups</unit></amt>
        <item>Thawed frozen vegetables</item></ing>
      <ing>
        <amt>
          <qty>14 1/2</qty>
          <unit>ounces</unit></amt>
        <item>Can chicken stock</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Lemon juice</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Dairy sour cream</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Dill</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>12</qty>
          <unit>ounces</unit></amt>
        <item>Can white tuna</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Garlic powder</item></ing></ingredients>
    <directions>
      <step>  1. In heavy 1 1/2 qt. saucepan (with a tight lid) combine rice chicken
  stock, water, lemon juice, dill, salt, pepper and garlic powder. Bring to a
  boil, cover, reduce heat and simmer 20 minutes. 2. Stir in vegetables,cover
  and continue cooking 5 more minutes or until all the liquid is absorbed. 3.
  Blend in sour cream and Starkist Albacore. Serve hot or cold.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
