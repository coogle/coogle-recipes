<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Beef and Cabbage Soup</title>
      <categories>
        <cat>Soups/stews</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Lean ground beef</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Garlic salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Garlic powder</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>x</unit></amt>
        <item>Celery; chopped</item></ing>
      <ing>
        <amt>
          <qty>16</qty>
          <unit>ounces</unit></amt>
        <item>Kidney beans; drained</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit/></amt>
        <item>Chopped head cabbage</item></ing>
      <ing>
        <amt>
          <qty>28</qty>
          <unit>ounces</unit></amt>
        <item>Tomatoes *</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Water in empty tom. can</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Beef bouillon cubes</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Chopped parsley</item></ing></ingredients>
    <directions>
      <step>  In a Dutch oven, brown beef. Add all remaining ingredients except parsley;
  bring to boil. Reduce heat &amp; simmer, covered for 1 hour. Garnish with
  parsley. Yield 3 qts. * chopped &amp; liquid reserved
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
