<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>American Lamb Cassoulet</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>10</qty>
          <unit>ounces</unit></amt>
        <item>Canned diced tomatoes with green chiles</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Scallions; sliced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Clove garlic; minced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Beef bouillon granules</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Italian herb seasoning; crushed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Tomato sauce</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>pounds</unit></amt>
        <item>Lamb shoulder; cut into 3/4 inch cubes</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Zucchini; halved lengthwise and sliced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pound</unit></amt>
        <item>Canned Great Northern beans; drained</item></ing>
      <ing>
        <amt>
          <qty>Prep:</qty>
          <unit>10</unit></amt>
        <item>min, Cook: 15 min.</item></ing></ingredients>
    <directions>
      <step>  Combine all ingredients, except zucchini and beans, in a microwave
  casserole. Cover and microwave on high 3 minutes. Remove cover and stir.
  Continue to microwave another 4 minutes. Stir in zucchini and beans and
  microwave another 6-7 minutes, until meat is cooked throughout.
  
  Posted to recipelu-digest Volume 01 Number 620 by molony &lt;molony@scsn.net&gt;
  on Jan 28, 1998
 
</step></directions></recipe></recipeml>
