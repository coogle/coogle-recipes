<recipeml version="0.5">
  <recipe>
    <head>
      <title>Amaretto Pancakes</title>
      <categories>
        <cat>Breakfast</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Bisquick</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Almonds -- sliced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Amaretto</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Amaretto Butter  (see</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Recipe)</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Amaretto Syrup  (see</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Recipe)</item></ing></ingredients>
    <directions>
      <step>  Beat pancake ingredients together until smooth. Cook as usual.
  
