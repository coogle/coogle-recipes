<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Aloha Bread</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title>FOR 1 1/2 POUND LOAF</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>can</unit></amt>
          <item>(8 ounces) crushed pineapple in juice; drained and juice reserved</item></ing>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>Water</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Reserved pineapple juice</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Margarine or butter; softened</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Shredded coconut; toasted</item></ing>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>Pecan or walnut halves</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Dry milk</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Packed brown sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Yeast</item></ing></ing-div>
      <ing-div>
        <title>FOR 2 POUND LOAF</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>can</unit></amt>
          <item>(8 ounces) crushed pineapple in juice; drained and juice reserved</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Water</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Reserved pineapple juice</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Margarine or butter; softened</item></ing>
        <ing>
          <amt>
            <qty>4</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>2/3</qty>
            <unit>cups</unit></amt>
          <item>Shredded coconut; toasted</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Pecan or walnut halves</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Dry milk</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Plus 1 teaspoon Packed brown sugar</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1 3/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Yeast</item></ing></ing-div></ingredients>
    <directions>
      <step>  This bread machine recipe is delicious and one of my favorites
  
  Measure carefully, placing all ingredients in bread machine pan in the
  order recommended by the manufacturer.
  
  Select Sweet or Basic/White cycle. Use medium or light crust color. Do not
  use delay cycle. Remove baked bread from pan and cool on a wire rack
  
  Posted to recipelu-digest Volume 01 Number 423 by "rudolph"
  &lt;rudolph@1st.net&gt; on Dec 30, 1997
 
</step></directions></recipe></recipeml>
