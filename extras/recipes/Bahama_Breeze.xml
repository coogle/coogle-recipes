    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>ounce</unit></amt>
        <item>Rum, dark</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>ounces</unit></amt>
        <item>Liqueur, banana</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>ounces</unit></amt>
