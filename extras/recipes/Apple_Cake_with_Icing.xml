<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Cake with Icing</title>
      <categories>
        <cat>Cakes</cat>
        <cat>Desserts</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing-div>
        <title>CAKE</title>
        <ing>
          <amt>
            <qty>3</qty>
            <unit/></amt>
          <item>Large Eggs</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>cups</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Soda</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Vanilla</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Cored, Peeled, Diced Apples</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>Oil</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Pecans</item></ing></ing-div>
      <ing-div>
        <title>ICING</title>
        <ing>
          <amt>
            <qty>4</qty>
            <unit>ounces</unit></amt>
          <item>(1 cube) Margarine</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Milk</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Packed Brown Sugar</item></ing></ing-div></ingredients>
    <directions>
      <step>  Mix all cake ingredients together and pour into 9 x 13-inch oblong pan.
  Bake at 350 degrees F. for 45 minutes or until cake tester comes out clean
  when inserted in center of cake.
  
  Cook icing ingredients in small saucepan 2 1/2 minutes and pour over warm
  cake.
 
</step></directions></recipe></recipeml>
