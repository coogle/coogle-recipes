<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Another Bean Salad</title>
      <categories>
        <cat>Salads</cat>
        <cat>Vegetables</cat></categories>
      <yield>10</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Vinegar</item></ing>
      <ing>
        <amt>
          <qty>16</qty>
          <unit>ounces</unit></amt>
        <item>Yellow beans, can, drained</item></ing>
      <ing>
        <amt>
          <qty>16</qty>
          <unit>ounces</unit></amt>
        <item>Garbanzo beans, can, drained</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Green pepper, slivered</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Onions, medium, sliced thin</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>16</qty>
          <unit>ounces</unit></amt>
        <item>Green beans, can, drained</item></ing>
      <ing>
        <amt>
          <qty>16</qty>
          <unit>ounces</unit></amt>
        <item>Lima beans, can, drained</item></ing>
      <ing>
        <amt>
          <qty>16</qty>
          <unit>ounces</unit></amt>
        <item>Red kidney beans, drained</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Celery, sliced</item></ing></ingredients>
    <directions>
      <step>  Combine sugar, salt vinegar in pan, bring to boil for 1 min.
  Cool. Toss all other ingredients together and pour the vinegar mixture
  over them.  Marinate for 24 hrs in refrigerator, stirring occasionally.
  Cal: 313.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmealnew.zip
 
</step></directions></recipe></recipeml>
