<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Potato Topping</title>
      <categories>
        <cat>Potatoes</cat>
        <cat>Syd's book</cat></categories>
      <yield>2</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Mayonnaise</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Tarragon To Taste</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Garlic Powder To Taste</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Thyme To Taste</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Oregano To Taste</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Basil To Taste</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Rosemary To Taste</item></ing></ingredients>
    <directions>
      <step>  Blend together the mayonnaise, tarragon, garlic powder, thyme, oregano,
  basil and rosemary.  Top baked potato and blend in well with a fork. From:
  Syd's Cookbook.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/sydsbook.zip
 
</step></directions></recipe></recipeml>
