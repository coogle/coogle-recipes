<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple-Date Cream Cake</title>
      <categories>
        <cat>Cake</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Soda</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Allspice</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs; slightly beaten</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(21-oz) apple pie filling</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Chopped dates</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Chopped nuts</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Cool whip</item></ing></ingredients>
    <directions>
      <step>  Combine first 6 ingredients; stir gently. Combine next 4 ingredients; mix
  well and stir into dry ingredients. Stir in dates and nuts. Pour into a
  greased and floured 13x9-2-inch pan. Bake at 350 degrees for 35 minutes.
  Cool. Top with Cool Whip or whipped cream.
  
  From a collection of my mother's (Judy Hosey) recipe box which contained
  lots of her favorite recipes, clippings, etc.  Downloaded from Glen's MM
  Recipe Archive, http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
