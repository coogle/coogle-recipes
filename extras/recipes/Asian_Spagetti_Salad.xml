<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Asian Spagetti Salad</title>
      <categories>
        <cat>Salads</cat>
        <cat>Chinese</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty/>
            <unit>packages</unit></amt>
          <item>Cooked spaghetti</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Carrots; raw, sliced</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Plum tomatoes; raw, diced</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Scallions; raw, sliced</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>can</unit></amt>
          <item>Black Olives; small pitted</item></ing></ing-div>
      <ing-div>
        <title>DRESSING</title>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Soy sauce</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Italian dressing</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>teaspoons</unit></amt>
          <item>Sesame seed oil</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Garlic clove; crushed</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>teaspoons</unit></amt>
          <item>White pepper</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>teaspoons</unit></amt>
          <item>Ginger; grated</item></ing></ing-div></ingredients>
    <directions>
      <step>  Mix all ingredients in a large bowl - refrigerate for at least 2 hours -
  add additional soy sauce or Italian dressing if needed.
  Posted to recipelu-digest by Chtbaby &lt;Chtbaby@aol.com&gt; on Mar 9, 1998
 
</step></directions></recipe></recipeml>
