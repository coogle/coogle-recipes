<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana Nut Bread 3</title>
      <categories>
        <cat>Hand</cat>
        <cat>Made</cat></categories>
      <yield>10</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Butter</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Mashed bananas</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla extract</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>All-purpose flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Baking soda</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Buttermilk</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Nuts; chopped</item></ing></ingredients>
    <directions>
      <step>  Cream together butter and sugar. Add banana, eggs, and vanilla. Sift
  together dry ingredients. Add alternately with buttermilk. Blend well. Stir
  in nuts, and pour into a greased and floured loaf pan. Bake at 325 degrees
  for 1-1/4 hours.
