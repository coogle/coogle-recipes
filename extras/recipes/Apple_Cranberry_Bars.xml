<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Cranberry Bars</title>
      <categories>
        <cat>None</cat></categories>
      <yield>48</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Margarine -- softened</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Apples -- peeled/chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Cranberries -- chopped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Egg whites -- whipped</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Orange peel -- grated</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>2 1/4</qty>
          <unit>cups</unit></amt>
        <item>Unbleached flour</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Granulated sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Baking soda</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Topping----</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Margarine -- softened</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>ounces</unit></amt>
        <item>Fat-free cream cheese --</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Softened</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Powdered sugar --sifted</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Orange peel -- grated</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>teaspoons</unit></amt>
        <item>Skim milk -- at room</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Temperature</item></ing></ingredients>
    <directions>
      <step>  Preheat oven to 350 degrees.  Prepare a 15 x 10 x 1" jelly roll pan with
  cooking spray and flour.  In a mixing bowl, combine the margarine, apples,
  cranberries, egg whites, orange peel and vanilla.  In another mixing bowl,
  combine flour, baking soda and salt.  Mix dry ingredients with wet
  ingredients just until moistened.  Spread mixture into prepared pan.  Bake
  for 30 to 35 minutes.  Meanwhile, to prepare topping, combine the
  margarine, cream cheese, vanilla, orange peel, and milk.  Top when bars
  have cooled.
  
  Recipe By     : Land O Lakes Holiday Memories
  
  From: Ladies Home Journal- August 1991
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmdja006.zip
 
</step></directions></recipe></recipeml>
