  <recipe>
    <head>
      <title>Banana Oat Bran Bread</title>
      <categories>
        <cat>Bread machi</cat>
        <cat>Breads</cat></categories>
      <yield>14</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Active Dry Yeast</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Bread Flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Whole-Wheat Flour</item></ing>
      <ing>
        <amt>
          <qty>3/4</qty>
          <unit>cups</unit></amt>
        <item>Oat Bran</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Gluten Flour, 100%</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Poppy Seeds ; optional</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Nutmeg ; optional</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Molasses</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Fruit Concentrate ; or honey</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Bananas ; ripe, 2 mashed</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Water ; + 1 T if at high</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>; altitude</item></ing></ingredients>
    <directions>
      <step>  use white bread setting, light
  
