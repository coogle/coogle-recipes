<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Artichoke Cheese Squares</title>
      <categories>
        <cat>Appetizer</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>8</qty>
          <unit/></amt>
        <item>Egg whites</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Clove garlic; crushed</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>Artichoke hearts packed in water (drain and chop)</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Dry bread crumbs</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Grated non fat cheddar cheese</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Chopped parsley</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Onion flakes</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Fresh basil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>dash</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>dash</unit></amt>
        <item>Tabasco</item></ing></ingredients>
    <directions>
      <step>  From: "MIKE SHOCKLEY" &lt;Newshock@msn.com&gt;
  
  Date: Thu, 18 Jul 96 23:28:04 UT
  Spray a 9 x 9 pan lightly with pam.  Beat egg whites till foamy and stir in
  all other ingredients.  Pour into dish.  Cook at 325 degrees for 25
  minutes. Let cool and cut into squares.
  
