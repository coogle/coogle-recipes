<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Cake #3</title>
      <categories>
        <cat>Cake</cat></categories>
      <yield>16</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>cups</unit></amt>
        <item>Diced apples</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Wesson oil</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Chopped nuts</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Beaten eggs</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Soda</item></ing></ingredients>
    <directions>
      <step>  From: ranger@halcyon.com (sharon gorman)
  
  Date: Sat, 29 Oct 1994 20:38:00 +0000
  
  Put apples in large bowl, add sugar and mix.  Add oil, nuts, eggs and
  vanilla. Mix well. Sift flour, soda, cinnamon and salt together and add to
  apple mixture. Mix well. Put in a 13 x 9 x 2 in pan. Bake at 350 for 1
  hour.
  
