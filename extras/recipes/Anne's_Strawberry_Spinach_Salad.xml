  <recipe>
    <head>
      <title>Anne's Strawberry Spinach Salad</title>
      <categories>
        <cat>Salads</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Or 2 bunches of spinach --</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Washed &amp; drained</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Dill weed to taste</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>pint</unit></amt>
        <item>Cleaned cut strawberries</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Sesame seeds -- toasted </item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Red wine vinegar</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Garlic powder</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Onion powder</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Dry mustard</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>DRESSING:-----</item></ing></ingredients>
    <directions>
      <step>  Chill salad dressing and pour over salad before serving.
  
