  <recipe>
    <head>
      <title>Andes Mint Cheesecake</title>
      <categories>
        <cat>Cheesecakes</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>package</unit></amt>
        <item>OREO COOKIES</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>tablespoons</unit></amt>
        <item>BUTTER, melted</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Crush Oreos in a food processor; add melted butter, mix</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Well. Press into 10 inch springform pan.</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>CAKE</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>(32 oz.) pkg CREAM CHEESE</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>SUGAR</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit/></amt>
        <item>EGGS</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>(6 oz.) pkg SEMI-SWEET CHOCOLATE, melted</item></ing>
      <ing>
        <amt>
          <qty>18</qty>
          <unit/></amt>
        <item>ANDES MINT CANDIES, chopped</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>SOUR CREAM</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>VANILLA</item></ing></ingredients>
    <directions>
      <step>  ** CRUST
 
</step></directions></recipe></recipeml>
