<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baby Food Bars</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>cups</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>(4 1/2 oz) jars baby food 1 carrot; 1 applesauce and 1 apricot)</item></ing>
      <ing>
        <amt>
          <qty>1 1/4</qty>
          <unit>cups</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Baking soda</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Cinnamon</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>8oz pkg cream cheese</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Stick margarine</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Box powdered sugar</item></ing></ingredients>
    <directions>
      <step>  Mix eggs, sugar, flour, baby food, oil, soda and cinnamon together. Spread
  onto an ungreased cookie sheet. Bake at 350 for 25 to 30 minutes. Let cool.
  For icing: Mix cream cheese, margarine, vanilla and powdered sugar until
  smooth. Top bars with icing.
  
  Posted to Bakery-Shoppe Digest V1 #429 by "Elayne Murray"
  &lt;elaynemu@lodinet.com&gt; on Dec 1, 1997
 
</step></directions></recipe></recipeml>
