<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>60 Minute Rolls</title>
      <categories>
        <cat>Breads</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>packages</unit></amt>
          <item>Yeast dissolved in 1/4 c. warm water</item></ing></ing-div>
      <ing-div>
        <title>COMBINE WITH</title>
        <ing>
          <amt>
            <qty>4</qty>
            <unit>tablespoons</unit></amt>
          <item>Softened butter</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>Warm milk</item></ing>
        <ing>
          <amt>
            <qty>3</qty>
            <unit>tablespoons</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Beaten egg</item></ing>
        <ing>
          <amt>
            <qty>4</qty>
            <unit>cups</unit></amt>
          <item>Flour</item></ing></ing-div></ingredients>
    <directions>
      <step>  Knead and rest 15 minutes on board. Shape into rolls. When double in size,
  bake at 375 for 15 minutes.
  
