<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Banana Bread 6</title>
      <categories>
        <cat>Bread machi</cat>
        <cat>Bread maili</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3 1/2</qty>
          <unit>ounces</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>2 1/3</qty>
          <unit>cups</unit></amt>
        <item>Bread flour</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Butter; or margarine</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Nonfat dry milk</item></ing>
      <ing>
        <amt>
          <qty>2/3</qty>
          <unit>cups</unit></amt>
        <item>Banana; mashed</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Active dry yeast</item></ing></ingredients>
    <directions>
      <step>  Use *really* ripe bananas for the best banana flavor!
  
  &gt;From: Robin Hilp, robinr@ichips.intel.com
  From: Bread-Bakers Archives: ftp.best.com/pub/reggie/archives/bread/recipe
 
</step></directions></recipe></recipeml>
