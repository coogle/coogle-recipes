<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Basic White/whole Wheat Bread</title>
      <categories>
        <cat>Bread machi</cat>
        <cat>Bread maili</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/4</qty>
          <unit>cups</unit></amt>
        <item>Warm water</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>tablespoons</unit></amt>
        <item>Powdered milk</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>tablespoons</unit></amt>
        <item>Butter or margarine; or shortening</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Sugar; white</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit>cups</unit></amt>
        <item>White bread flour or 1-3/4 cups whole wheat flour; plus 1-1/4 cups white flour</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Active dry yeast</item></ing></ingredients>
    <directions>
      <step>  Add ingredients to bread bin in order of list (or as your manufacturer told
  you to). Select white/whole wheat bread with light crust. Takes from 3-4
  hours depending on your brand of machine. A good everyday bread.
  
  &gt;From: "Carole/Bob Walberg" &lt;walberg@Alpha.tkm.mb.ca&gt;
  From: Bread-Bakers Archives: ftp.best.com/pub/reggie/archives/bread/recipe
 
</step></directions></recipe></recipeml>
