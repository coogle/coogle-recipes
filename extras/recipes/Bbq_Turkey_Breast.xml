<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bbq Turkey Breast</title>
      <categories>
        <cat>Barbeque</cat>
        <cat>Poultry</cat>
        <cat>Turkey</cat>
        <cat>Main dish</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Egg; whipped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Oil</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Soy sauce</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Oyster sauce</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Red wine vinegar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Brown sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Horseradish; rounded</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Garlic salt</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>7-Up(TM)</item></ing>
      <ing>
        <amt>
          <qty>10</qty>
          <unit>pounds</unit></amt>
        <item>Turkey breast slices; 3/8" thick</item></ing></ingredients>
    <directions>
      <step>  Whip first 7 ingredients; add 7-UP. Marinate turkey in marinade over night.
  Grill over medium heat, being careful not to overcook. The sauce will
  carmalize over the heat creating a chewy crust.
