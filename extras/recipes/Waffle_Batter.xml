<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Waffle Batter</title>
      <categories>
        <cat>Breakfast</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Flour</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Baking powder</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Milk</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Heavy cream</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Eggs</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>teaspoons</unit></amt>
        <item>Melted butter</item></ing></ingredients>
    <directions>
      <step>  Sift flour, salt, powder amd sugar. In bowl beat milk, cream, eggs and
  butter until frothy. Slowly add dry ingredients to make a thick
  batter. Cook in well greased waffle iron.
  
  File ftp://ftp.idiscover.co.uk/pub/food/mealmaster/recipes/mmkah001.zip
 
</step></directions></recipe></recipeml>
