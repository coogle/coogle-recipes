<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Barbeque Meatballs</title>
      <categories>
        <cat>Beef</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>pound</unit></amt>
          <item>Ground beef</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Bread crumbs</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Egg; beaten</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Evaporated milk</item></ing></ing-div>
      <ing-div>
        <title>SAUCE</title>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Catsup</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Chili sauce</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Margarine</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Chopped onion</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Vinegar and brown sugar; to taste</item></ing></ing-div></ingredients>
    <directions>
      <step>  Mix and shape into balls. Brown meatballs in 1 Tbsp . cooking oil. Place
  into baking dish.
  
  Mix sacue and pour over meatballs. Bake at 375 for about 20 minutes.
  
  Catherine Kent
  
  Good served over rice.
  Posted to recipelu-digest Volume 01 Number 475 by "bunny"
  &lt;layla696@ix.netcom.com&gt; on Jan 8, 1998
 
</step></directions></recipe></recipeml>
