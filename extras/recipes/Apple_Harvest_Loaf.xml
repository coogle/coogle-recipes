<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Apple Harvest Loaf</title>
      <categories>
        <cat>Sweet</cat>
        <cat>Breads</cat>
        <cat>And</cat>
        <cat>Rolls</cat></categories>
      <yield>12</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Apple butter</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Water</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Salt</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Canola oil</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>teaspoons</unit></amt>
          <item>Hazelnut extract or almond extract</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit>cups</unit></amt>
          <item>Whole wheat flour</item></ing>
        <ing>
          <amt>
            <qty>2 1/2</qty>
            <unit>cups</unit></amt>
          <item>Bread flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Yeast</item></ing></ing-div>
      <ing-div>
        <title>FILLING</title>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>Dried cranberries</item></ing></ing-div></ingredients>
    <directions>
      <step>  Place ingredients into bread machine in the proper order for your machine.
  Bake on white bread cycle with light crust. At beeper, add the dried
  cranberries.
  
  Recipe by: Ruth G.
  
  Posted to JEWISH-FOOD digest V97 #314 by Ruth &amp; Shel &lt;fritzl@earthlink.net&gt;
  on Dec 01, 1997
 
</step></directions></recipe></recipeml>
