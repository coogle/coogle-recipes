<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>"Home Made" Potato Soup</title>
      <categories>
        <cat>None</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>6</qty>
          <unit/></amt>
        <item>Potatoes; peeled and cut into bite-size pieces</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Leeks; washed and cut up in small pieces</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit/></amt>
        <item>Onions; chopped</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Carrot; pared and sliced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Stalk celery; sliced</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit/></amt>
        <item>Chicken bouillon cubes</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Parsley flakes</item></ing>
      <ing>
        <amt>
          <qty>5</qty>
          <unit>cups</unit></amt>
        <item>Water</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>tablespoon</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>1/3</qty>
          <unit>cups</unit></amt>
        <item>Butter or margarine</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>can</unit></amt>
        <item>(13-ounce) evaporated milk</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Chopped chives</item></ing></ingredients>
    <directions>
      <step>  Put all ingredients in cooker except evaporated milk and chives. Cover and
  cook on low 10-12 hours or high 3-4 hours. Stir in evaporated milk with
  masher before serving. Serve topped with chopped chives.
  
  Posted to recipelu-digest Volume 01 Number 448 by "rudolph"
  &lt;rudolph@1st.net&gt; on Jan 4, 1998
 
</step></directions></recipe></recipeml>
