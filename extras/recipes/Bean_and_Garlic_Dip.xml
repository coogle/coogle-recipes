<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Bean and Garlic Dip</title>
      <categories>
        <cat>Appetizers</cat>
        <cat>Spreads</cat>
        <cat>Vegan</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Fava beans, cooked</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>cups</unit></amt>
        <item>Soy Mayonnaise</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Garlic clove, finely chopped</item></ing>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>teaspoons</unit></amt>
        <item>Ground red chiles</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Salt</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>dash</unit></amt>
        <item>Hot pepper</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>Tortilla chips</item></ing></ingredients>
    <directions>
      <step>  Mix all ingredients except tortilla chips. Cover and refrigerate 1 hour.
  Serve with tortilla chips.
  
