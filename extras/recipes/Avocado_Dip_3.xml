<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Avocado Dip</title>
      <categories>
        <cat>Dips</cat>
        <cat>Possum king</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>small</unit></amt>
        <item>Avocados; peeled, cut up</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Tomato; peeled. cut up</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Mayonnaise</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>small</unit></amt>
        <item>Onion</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>tablespoons</unit></amt>
        <item>Lemon Juice</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Worcestershire Sauce</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Salt</item></ing></ingredients>
    <directions>
      <step>  Place all ingredients in blender; cover. Blend on high until smooth.
  
  Cover; chill.
  
  Makes 2 cups.
  Posted to MC-Recipe Digest V1 #924 by "abprice@wf.net" &lt;abprice@wf.net&gt; on
  Nov 25, 1997
 
</step></directions></recipe></recipeml>
