<recipeml version="0.5">
  <recipe>
    <head>
      <title>Baked Creamed Potatoes</title>
      <categories>
        <cat>Potato cass</cat></categories>
      <yield>6</yield></head>
    <ingredients>
      <ing-div>
        <title>FARM J AMERICA'S BEST VEGS</title>
        <ing>
          <amt>
            <qty>4</qty>
            <unit>cups</unit></amt>
          <item>Potatoes, diced</item></ing>
        <ing>
          <amt>
            <qty/>
            <unit/></amt>
          <item>Salt and pepper</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Fresh dill, minced</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Heavy cream</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Milk</item></ing></ing-div></ingredients>
    <directions>
      <step>  Good with fish. Place potatoes in shallow baking dish; sprinkle with salt,
  pepper, and dill. Combine cream and milk; pour over potatoes. Bake 350 F,
       30    mins
  
