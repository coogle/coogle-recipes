<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Wacky Cake #1</title>
      <categories>
        <cat>Cake</cat></categories>
      <yield>8</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>1/3</qty>
            <unit>cups</unit></amt>
          <item>Cocoa</item></ing>
        <ing>
          <amt>
            <qty>1 1/2</qty>
            <unit>cups</unit></amt>
          <item>All purpose flour</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Vinegar</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Soda</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Vanilla</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Stick margarine; melted</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Water</item></ing></ing-div>
      <ing-div>
        <title>ICING</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>cup</unit></amt>
          <item>Sugar</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Milk</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>teaspoon</unit></amt>
          <item>Vanilla</item></ing>
        <ing>
          <amt>
            <qty>1/4</qty>
            <unit>cups</unit></amt>
          <item>Cocoa</item></ing>
        <ing>
          <amt>
            <qty>1/2</qty>
            <unit/></amt>
          <item>Stick margarine</item></ing></ing-div></ingredients>
    <directions>
      <step>  Mix dry ingredients together &amp; add remaining ingredients. Pour into an
  ungreased 8x11-inch pan. Bake at 350 for 20-25 minutes. Icing: Combine
  ingredients &amp; bring to a boil. Add vanilla. Beat until it begins to
  thicken. Pour over cake.
  
  From the &lt;The Bliss of Cooking Returns&gt;, Fort Bliss Officers Wives Club,
  Ft. Bliss, TX.  Downloaded from Glen's MM Recipe Archive,
  http://www.erols.com/hosey.
 
</step></directions></recipe></recipeml>
