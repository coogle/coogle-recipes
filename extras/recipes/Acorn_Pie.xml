<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>Acorn Pie</title>
      <categories>
        <cat>Pies</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Egg whites, beaten stiff</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Baking powder</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>cup</unit></amt>
        <item>Sugar</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>teaspoon</unit></amt>
        <item>Vanilla</item></ing>
      <ing>
        <amt>
          <qty>20</qty>
          <unit/></amt>
        <item>Soda crackers</item></ing>
      <ing>
        <amt>
          <qty/>
          <unit/></amt>
        <item>(coarsley broken)</item></ing>
      <ing>
        <amt>
          <qty>1/2</qty>
          <unit>cups</unit></amt>
        <item>Pecans, chopped</item></ing></ingredients>
    <directions>
      <step>  Beat egg whites until stiff; add baking powder and beat more. Add sugar and
  vanilla; beat again.  Fold in crackers and pecans. Put in buttered pie
  plate and bake at 300 degrees for 30 minutes. Let cool and top with Cool
  Whip and chopped pecans. Randy Rigg
 
</step></directions></recipe></recipeml>
